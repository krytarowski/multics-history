            >t>pnt_fs_gate_
          re    ..*

            >t>pnt_login_gate_
          re    Initializer.SysDaemon.*
          re    .SysAdmin.*

            >t>pnt_network_gate_
          re    Initializer.SysDaemon.*
          re    IMFT.Daemon.*
          re    Card_Input.Daemon.*
          re    .SysDaemon.*

            >t>pnt_priv_gate_
          re    Initializer.SysDaemon.*
