		Bindfile for mcs


/* bindfile for MCS, Multics Communications System */
/* This version contains the basic core image for DN355 type FNPs */
/* Created 040680 by Frank Martinson */
/* Modified 051881 by R. Holmstedt to specify no metering */

version:	5.19;


lsla:	3;
hsla:	2;
memory:	32;
console:	yes;
printer:	no;
meter:    no;

/* module load list */

order:	scheduler,
	interpreter,
	breakpoint_man,
	control_tables,
	autobaud_tables,
	dia_man,
	hsla_man,
	lsla_man,
	console_man,
	utilities,
	trace,
	init;

/* entry to init from bootload */

entry:	istart;

/* table size specifications */

module:	hsla_man;
 type:	hsla;
 size:	97;

module:	lsla_man;
 type:	lsla;
 size:	102;

module:	trace;
 type:	trace;
 mask:	317777;	/* trace enable mask */
 size:	1024;

end;



