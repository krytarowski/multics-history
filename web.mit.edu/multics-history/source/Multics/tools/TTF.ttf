Input_conversion: standard_input_conv;


/***** Typical ASCII teleprinter terminal *****/

terminal_type: ASCII;
 modes: default,ll79,^pl,can_type=overstrike,hndlquit,fulldpx,echoplex,crecho,lfecho,^tabs,tabecho;
 bauds:              110      133      150      300      600      1200      1800      2400      4800      7200      9600;
  vert_nl_delays:      0        1        1        5        9        18        27        36        72       108       127;
  horz_nl_delays:	   0.00     0.012    0.025    0.019    0.060     0.120     0.180     0.240     0.480     0.720     0.960;
  const_tab_delays:    0        0        0        0        2         3        10        16        42        68        94;
  var_tab_delays:	   0        0.180    0.250    0.250    0.500     1.000     1.000     1.000     1.000     1.000     1.000;
  backspace_delays:    0        0        0        0        1         2         3         4         8        12        16;
  vt_ff_delays:	   9       24       29       59      100       200       300       400       511       511       511;
 output_conversion: ascii_output_conv;
 special: ascii_special;
 line_types: ASCII, VIP, POLLED_VIP;
 old_type: 11;


