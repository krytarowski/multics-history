  home:		ESC "?";
  cursor_up:	ESC ";";
  cursor_right:	ESC "=";
  cursor_down:	LF;
  cursor_left:	BS;
  clear_screen:	^L, pad 48 ms;
  clear_to_eos:	ESC ^E, pad 384 ms;
  clear_to_eol:	ESC ^U, pad 16 ms;
  insert_chars:	ESC ^P;
  end_insert_chars:	ESC ^@;
  delete_chars:	ESC ^Q, pad 8 ms;
  insert_lines:	ESC ^R, pad 72 ms;
  delete_lines:	ESC ^B, pad 72 ms;


