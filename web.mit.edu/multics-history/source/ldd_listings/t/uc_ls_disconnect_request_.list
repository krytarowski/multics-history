      132 /*							       */
      133 /* *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  * */
      134 
      135 Abort:
      136      procedure options (variable);
      137 
      138 dcl  cu_$arg_list_ptr       entry returns (ptr);
      139 dcl  sys_log_$general       entry (ptr);
      140 
      141 	sl_info = sl_info_sev_code_msg;
      142 	sl_info.arg_list_ptr = cu_$arg_list_ptr ();
      143 	sl_info.caller = ME;
      144 	call sys_log_$general (addr (sl_info));
      145 	code = sl_info.code;
      146 	if code ^= 0 then go to RETURN;
      147 
      148      end Abort;
