       45 uc_ls_disconnect_request_:
       46      procedure (P_ls_request_server_info_ptr,
       47 	P_ls_request_ptr, P_ls_request_lth,
       48 	P_ls_response_ptr, P_ls_response_lth,
       49 	P_ls_ipc_reply_ptr, P_code);
       50 
       51 /* Parameters */
       52 
       53 dcl  P_ls_request_server_info_ptr ptr parameter;
       54 dcl  P_ls_request_ptr       ptr parameter;
       55 dcl  P_ls_request_lth       fixed bin (18) parameter;
       56 dcl  P_ls_response_ptr      ptr parameter;
       57 dcl  P_ls_response_lth      fixed bin (18) parameter;
       58 dcl  P_ls_ipc_reply_ptr     ptr parameter;
       59 dcl  P_code	        fixed bin (35) parameter;
       60 
       61 /* Automatic */
       62 
       63 dcl  code		        fixed bin (35) automatic;
       64 dcl  hangup_ev_message      fixed bin (71) automatic;
       65 
       66 /* Entries */
       67 
       68 dcl  hcs_$wakeup	        entry (bit (36) aligned, fixed bin (71), fixed bin (71), fixed bin (35));
       69 dcl  uc_logout_	        entry (ptr, char (*));
       70 dcl  user_table_mgr_$free entry (ptr);
       71 dcl  user_table_mgr_$utep_from_handle entry (bit (72) aligned) returns (ptr);
       72 
       73 /* External */
       74 
       75 dcl  error_table_$action_not_performed fixed bin (35) ext static;
       76 dcl  error_table_$id_not_found fixed bin (35) ext static;
       77 
       78 /* Constant */
       79 
       80 dcl  HANGUP_EV_MESSAGE      char (8) initial ("hangup") internal static options (constant);
       81 dcl  ME		        char (32) initial ("uc_ls_disconnect_request_") internal static options (constant);
       82 
       83 /* Builtin */
       84 
       85 dcl (addr, null, unspec)    builtin;
       86 
       87 /* Program */
       88 
       89 	ls_request_server_info_ptr = P_ls_request_server_info_ptr;
       90 	ls_request_ptr = P_ls_request_ptr;
       91 	ls_reply_message_ptr = P_ls_ipc_reply_ptr;
       92 	code = 0;
       93 
       94 	utep = user_table_mgr_$utep_from_handle (
       95 	     login_server_disconnect_request.handle);
       96 	if utep = null () then
       97 	     call Abort (SL_LOG_SILENT, error_table_$id_not_found,
       98 		"Could not locate user table entry for handle ^24.3b.",
       99 		login_server_disconnect_request.handle);
      100 
      101 	if ute.active = NOW_LOGGED_IN then do;		/* no process yet */
      102 	     call uc_logout_ (utep, "hangup, no process");
      103 	     call user_table_mgr_$free (utep);
      104 	     P_ls_response_lth = 0;
      105 	end;
      106 	else if ute.active = NOW_HAS_PROCESS then do;
      107 	     unspec (hangup_ev_message) = unspec (HANGUP_EV_MESSAGE);
      108 	     ansp = as_data_$ansp;
      109 	     call hcs_$wakeup (anstbl.as_procid, ute.event,
      110 		hangup_ev_message, code);
      111 	     if code ^= 0 then
      112 		call Abort (SL_LOG_BEEP, code,
      113 		"Sending hangup wakeup for ^[*^]^a.^a ^a ^12.3b.",
      114 		(ute.anonymous=1), ute.person, ute.project, ute.tty_name,
      115 		ute.proc_id);
      116 	end;
      117 	else do;
      118 	     call Abort (SL_LOG_BEEP, error_table_$action_not_performed,
      119 		"UTE (^d, ^p in ^a) not logged in (active= ^a, ^d).",
      120 		ute.ute_index, utep, TABLE_NAMES(ute.process_type),
      121 		ACTIVE_VALUES(ute.active), ute.active);
      122 	end;
      123 RETURN:
      124 	P_code = 0;
      125 	return;
