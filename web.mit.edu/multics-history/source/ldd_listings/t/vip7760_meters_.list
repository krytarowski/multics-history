      229 	     call ioa_$ioa_switch (iocbp, "Discarded printer frames^28t^a",
      230 		meter_format_$picture (discarded_printer_frames, 11));
      231 	end;
      232 
      233 	else do;
      234 	     if ^error
      235 	     then call ioa_$ioa_switch (iocbp, "Page overflows^28t^a", meter_format_$picture (display_pgofs, 11));
      236 	     call ioa_$ioa_switch (iocbp, "Exceeded page overflow limit^32t^a",
      237 		meter_format_$picture (pgof_limit_reached, 7));
      238 	end;
      239 
      240 	return;
