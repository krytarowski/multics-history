      213 	else brief = "0"b;
      214 
      215 	if flags & DISPLAY_MPX_ERROR
      216 	then error = "1"b;
      217 	if flags & DISPLAY_MPX_SINCE_BOOT
      218 	then since_boot = "1"b;
      219 	else since_boot = "0"b;
