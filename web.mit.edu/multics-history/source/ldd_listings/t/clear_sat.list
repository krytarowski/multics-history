       49 	then do;
       50 		call com_err_ (code, ME);
       51 		return;
       52 	     end;
       53 
       54 	if arg_count ^= 1
       55 	then do;
       56 		call com_err_ (0, ME, "Usage: ^a SAT_PATH.sat", ME);
       57 		return;
       58 	     end;
       59 
       60 	satp = null ();
       61 
       62 	call cu_$arg_ptr (1, ap, al, (0));
       63 	call expand_pathname_ (argument, dir_name, entryname, code);
       64 	if code ^= 0
       65 	then do;
       66 		call com_err_ (code, ME, "^a", argument);
       67 		return;
       68 	     end;
       69 
       70 	call initiate_file_ (dir_name, entryname, RW_ACCESS, satp, (0), code);
       71 	if code ^= 0
       72 	then do;
       73 		call com_err_ (code, ME, "^a", pathname_ (dir_name, entryname));
       74 		return;
       75 	     end;
       76 
       77 	sat.version = -1;				/* BROKEN */
       78 	deleted_satex = -1;
       79 	last_satex = -1;
       80 	new_n_projects = 0;
       81 	do satex = 1 to sat.current_size;
       82 	     satep = addr (sat.project (satex));
       83 	     if project.state ^= 1			/* deleted or free (not in use) */
       84 	     then if deleted_satex < 0
       85 		then deleted_satex = satex;
       86 		else ;
       87 	     else do;
       88 		     new_n_projects = new_n_projects + 1;
       89 		     if deleted_satex > 0
       90 		     then do;
       91 			     deleted_satep = addr (sat.project (deleted_satex));
       92 			     deleted_satep -> project = satep -> project;
       93 			     last_satex = max (last_satex, deleted_satex);
       94 						/* note highest we have seen */
       95 			     project.state = 0;
       96 			     deleted_satep = addr (sat.project (deleted_satex + 1));
       97 			     if deleted_satep -> project.state ^= 1
       98 			     then deleted_satex = deleted_satex + 1;
       99 			     else deleted_satex = satex;
      100 						/* we are now free */
      101 			end;
      102 		     else last_satex = satex;		/* don't move, but note */
      103 		end;
      104 	end;
      105 
      106 	if last_satex ^= new_n_projects
      107 	then call com_err_ (0, ME, "Inconsistency: n_projects ^d but last non-empty project index ^d.", new_n_projects,
      108 		last_satex);
      109 
      110 	sat.n_projects = new_n_projects;
      111 	sat.current_size = last_satex;
      112 	sat.version = SAT_version;
      113 	call terminate_file_ (satp, (0), TERM_FILE_TERM, (0));
      114 	return;
      115      end clear_sat;
