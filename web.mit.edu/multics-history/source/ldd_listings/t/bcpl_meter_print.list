    85	and PrintTime (t) be
    86	     $(	let Unit, Mult = "micro", 1
    87		if t ge Mult * 1000 do Unit, Mult := "milli", 1000
    88		if t ge Mult * 1000 do Unit, t := "", t / 1000
    89		Writech (OUTPUT, '*s')
    90		PrintFraction (t, Mult)
    91		Format (OUTPUT, " ^sseconds", Unit)
    92	     $)
    93	and PrintPercent (Meter) be
    94	     $(	Writech (OUTPUT, '*s')
    95		let T = MeterPointer!(Meter + 2) / 1000
    96		if T = 0 do T := 1
    97		PrintFraction (MeterPointer!Meter / T, 10)
    98		WriteS (OUTPUT, "%.*n")
    99	     $)
