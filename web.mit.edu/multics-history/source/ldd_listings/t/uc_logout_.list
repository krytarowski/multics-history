      130 /* *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  * */
      131 /*							       */
      132 /* Abort:  Report errors via sys_log_$general and stop execution.	       */
      133 /*							       */
      134 /* Syntax:  call Abort (code, ioa_ctl, args);			       */
      135 /*							       */
      136 /* *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  * */
      137 
      138 Abort:
      139      procedure options (variable);
      140 
      141 dcl  cu_$arg_list_ptr       entry returns (ptr);
      142 dcl  sys_log_$general       entry (ptr);
      143 
      144 	sl_info = sl_info_code_msg;
      145 	sl_info.arg_list_ptr = cu_$arg_list_ptr ();
      146 	sl_info.severity = SL_LOG_BEEP;
      147 	sl_info.caller = ME;
      148 	call sys_log_$general (addr (sl_info));
      149 	goto RETURN;
      150 
      151      end Abort;
      152 
      153 Setup:
      154      procedure ();
      155 
      156 	ansp = as_data_$ansp;
      157 	return;
      158      end Setup;
