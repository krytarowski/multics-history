      160 Update_Whotab:
      161      procedure ();
      162 
      163 dcl  whotab_idx	        fixed bin automatic;
      164 
      165 	whoptr = as_data_$whoptr;
      166 
      167 	whotab.n_users = whotab.n_users - 1;
      168 	whotab.n_units = whotab.n_units - ute.user_weight;
      169 	whotab_idx = ute.whotabx;
      170 	if whotab_idx ^= 0 then
      171 	     do;
      172 	     whotab.e (whotab_idx).active = NOW_FREE;
      173 	     whotab.e (whotab_idx).person = "";
      174 	     whotab.e (whotab_idx).project = "";
      175 	     whotab.e (whotab_idx).chain = whotab.freep;	/* chain free entries together */
      176 	     whotab.freep = whotab_idx;		/* on top of free queue */
      177 	     ute.whotabx = 0;
      178 	end;
      179 
      180 	if ute.process_type = PT_ABSENTEE then do;
      181 	     if ute.queue > 0 then			/* background absentee */
      182 		whotab.abs_users = whotab.abs_users - 1;
      183 	     else whotab.fg_abs_users = whotab.fg_abs_users - 1;
      184 	end;
      185 	else if ute.process_type = PT_DAEMON then
      186 	     whotab.n_daemons = whotab.n_daemons - 1;
      187 	return;
      188      end Update_Whotab;
