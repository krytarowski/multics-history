      115 /* *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  * */
      116 /*							       */
      117 /* Abort:  Report errors via sys_log_$general and stop execution.	       */
      118 /*							       */
      119 /* Syntax:  call Abort (code, ioa_ctl, args);			       */
      120 /*							       */
      121 /* *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  * */
      122 
      123 Abort:
      124      procedure options (variable);
      125 
      126 dcl  cu_$arg_list_ptr       entry returns (ptr);
      127 dcl  sys_log_$general       entry (ptr);
      128 
      129 	sl_info = sl_info_code_msg;
      130 	sl_info.arg_list_ptr = cu_$arg_list_ptr ();
      131 	sl_info.severity = SL_LOG_SILENT;
      132 	sl_info.caller = ME;
      133 	call sys_log_$general (addr (sl_info));
      134 	code = sl_info.code;
      135 	if code ^= 0 then go to RETURN;
      136 
      137      end Abort;
