       44 uc_ls_new_proc_request_:
       45      procedure (P_ls_request_server_info_ptr,
       46 	P_ls_request_ptr, P_ls_request_lth,
       47 	P_ls_response_ptr, P_ls_response_lth,
       48 	P_ls_ipc_reply_ptr, P_code);
       49 
       50 /* Parameters */
       51 
       52 dcl  P_ls_request_server_info_ptr ptr parameter;
       53 dcl  P_ls_request_ptr       ptr parameter;
       54 dcl  P_ls_request_lth       fixed bin (18) parameter;
       55 dcl  P_ls_response_ptr      ptr parameter;
       56 dcl  P_ls_response_lth      fixed bin (18) parameter;
       57 dcl  P_ls_ipc_reply_ptr     ptr parameter;
       58 dcl  P_code	        fixed bin (35) parameter;
       59 
       60 /* Automatic */
       61 
       62 dcl  code		        fixed bin (35) automatic;
       63 
       64 /* Entries */
       65 
       66 dcl  asu_$send_term_signal  entry (ptr, fixed bin) returns (bit (1) aligned);
       67 dcl  dpg_		        entry (ptr, char (*));
       68 dcl  uc_setup_process_connect_ entry (ptr, ptr, fixed bin (18), ptr, fixed bin (18), ptr, ptr, fixed bin (35));
       69 
       70 /* Constant */
       71 
       72 dcl  ME		        char (23) initial ("uc_ls_new_proc_request_") internal static options (constant);
       73 dcl  TRUE		        bit (1) aligned initial ("1"b) internal static options (constant);
       74 
       75 /* Builtins */
       76 
       77 dcl (addr, null)	        builtin;
       78 
       79 
       80 /* Program */
       81 
       82 	ls_request_server_info_ptr = P_ls_request_server_info_ptr;
       83 	ls_request_ptr = P_ls_request_ptr;
       84 	ls_reply_message_ptr = P_ls_ipc_reply_ptr;
       85 	code = 0;
       86 
       87 	call uc_setup_process_connect_ (P_ls_request_server_info_ptr,
       88 	     P_ls_request_ptr, P_ls_request_lth, P_ls_response_ptr,
       89 	     P_ls_response_lth, P_ls_ipc_reply_ptr, utep, code);
       90 	if code ^= 0 then
       91 	     call Abort (code,
       92 	     "Issuing new_proc for ^a.^a ^a",
       93 	     login_server_process_request.person_id,
       94 	     login_server_process_request.project_id,
       95 	     login_server_process_request.connection_name);
       96 
       97 /**** Tell the request server not to send a reply for this request, yet. */
       98 
       99 	P_ls_ipc_reply_ptr -> ls_reply_message.flags.do_not_reply = TRUE;
      100 	P_ls_response_lth = 0;
      101 
      102 	ute.logout_type = "newp";
      103 	ute.destroy_flag = WAIT_NEW_PROC_REQUEST;
      104 
      105 	if login_server_process_request.other_flags.immediate then
      106 	     call dpg_ (utep, "new_proc immediate");
      107 	else if ^asu_$send_term_signal (utep, PT_NEW_PROC_REQUEST) then
      108 	     call dpg_ (utep, "new_proc");
      109 	else ;					/* sent trm_ signal; go away until response comes in */
      110 
      111 RETURN:
      112 	P_code = code;
      113 	return;
