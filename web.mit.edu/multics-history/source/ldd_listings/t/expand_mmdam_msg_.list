        9 expand_mmdam_msg_:
       10      procedure ();
       11 
       12 /* *      EXPAND_MMDAM_MSG_
       13*   *
       14*   *      Expander for memory damage message generated by page_error and hardware_fault.
       15*   *
       16*   *	85-02-21, EJ Sharpe: initial coding
       17**/
       18 
       19 declare  P_log_message_format_ptr pointer parameter;
       20 declare  P_log_message_ptr pointer parameter;
       21 declare  P_expand_mode_ptr pointer parameter;
       22 declare  P_expansion char (*) varying parameter;
       23 declare  P_code fixed bin (35) parameter;
       24 
       25 declare  temp char (2000) varying;
       26 
       27 declare  expand_log_message_$unknown_syserr entry (pointer, pointer, pointer, char (*) varying, fixed bin (35));
       28 declare  expand_log_message_$unknown entry (pointer, pointer, pointer, char (*) varying, fixed bin (35));
       29 declare  ioa_$rsnnl entry options (variable);
       30 
       31 declare  (addr, binary, dimension, size) builtin;
       32 
       33 
       34 expand_mmdam_msg_$format:
       35      entry (P_log_message_format_ptr, P_log_message_ptr, P_expand_mode_ptr, P_expansion, P_code);
       36 
       37 	log_message_ptr = P_log_message_ptr;
       38 
       39 	mmdam_ptr = addr (log_message.data (1));
       40 
       41 	if size (mmdam_msg) = dimension (log_message.data, 1)
       42 	then call expand_mmdam ();
       43 	else call expand_log_message_$unknown
       44 		(P_log_message_format_ptr, log_message_ptr, P_expand_mode_ptr, P_expansion, P_code);
       45 
       46 	return;
       47 
       48 
       49 /* This is a compatibility entrypoint-- it processes an old-format "syserr" class
       50*   message, which has the syserr binary code in the first word of the data. */
       51 
       52 expand_mmdam_msg_$syserr:
       53      entry (P_log_message_format_ptr, P_log_message_ptr, P_expand_mode_ptr, P_expansion, P_code);
       54 
       55 	log_message_ptr = P_log_message_ptr;
       56 
       57 	mmdam_ptr = addr (log_message.data (2));
       58 
       59 	if (binary (log_message.data (1)) = SB_mmdam)
       60 	     & (size (mmdam_msg) = dimension (log_message.data, 1) - 1)
       61 	then call expand_mmdam ();
       62 	else call expand_log_message_$unknown_syserr
       63 		(P_log_message_format_ptr, log_message_ptr, P_expand_mode_ptr, P_expansion, P_code);
       64 
       65 	return;
       66 
       67 expand_mmdam:
       68      procedure ();
       69 
       70 	call ioa_$rsnnl ("^/Page at addr: ^oo, controller: ^a.", temp, (0), mmdam_msg.addr, mmdam_msg.ctrltag);
       71 
       72 	P_expansion = P_expansion || temp;
       73 
       74 	return;
       75      end expand_mmdam;
