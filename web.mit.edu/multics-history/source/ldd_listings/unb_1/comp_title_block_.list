      283 show:
      284   proc (datum, scale) returns (fixed dec (11, 3));
      285     dcl datum	   fixed bin (31);
      286     dcl scale	   fixed bin (31);
      287 
      288     return (round (dec (round (divide (datum, scale, 31, 11), 10), 11, 4), 3));
      289   end show;
