      234 make_atom:	entry;
      235 
      236 /* subr to make an uninterned atom given its pname as a string */
      237 
      238 	stack = addrel(stack_ptr, -2);
      239 	myname = fn_make_atom;
      240 	argno = 1;
      241 	call getstring;
      242 
      243 	call lisp_get_atom_(argp -> lisp_string.string, stack -> temp(1));	/* gc prot since arg is "always" a string */
      244 
