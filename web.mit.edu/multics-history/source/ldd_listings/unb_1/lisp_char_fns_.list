        6 lisp_char_fns_: proc;		/* LISP functions similar to the PL/I builtins for manipulating strings */
        7 
        8 dcl stack ptr,
        9     nargs fixed bin,
       10     argno fixed bin,
       11     argp ptr,
       12     argp2 ptr,
       13     myname fixed bin(35),
       14     slen fixed bin,
       15     lisp_error_ entry,
       16     lisp_alloc_ entry(fixed bin, fixed bin(71)),
       17     lisp_get_atom_ entry(char(*)aligned, fixed bin(71)),
       18     unm ptr,
       19     ercode(2) fixed bin(35) aligned based(unm),
       20     num fixed bin;
