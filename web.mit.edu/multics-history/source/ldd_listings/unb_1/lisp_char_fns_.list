      247 ItoC:	entry;
      248 
      249 /* convert an integer to a one character string*/
      250 
      251 	stack = addrel(stack_ptr, -2);
      252 	myname = fn_ItoC;
      253 	argno = 1;
      254 	call getnum;
      255 
      256 	call lisp_alloc_(2, stack -> temp(1));
      257 	stack -> temp_type(1) = String;
      258 	stack -> temp_ptr(1) -> lisp_string.string_length = 1;
      259 	unspec(substr(stack -> temp_ptr(1) -> lisp_string.string, 1, 1)) =
