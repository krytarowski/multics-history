      115 
      116 
      117 getnum:	entry;			/* gets number into num */
      118 
      119 
      120        do while("1"b);			/* loop until good arg gotten */
      121 	if addr(stack -> temp(argno))->fixnum_fmt.type_info = fixnum_type
      122 	   then do;
      123 		num = addr(stack -> temp(argno))->fixedb;
      124 		return;
      125 		end;
