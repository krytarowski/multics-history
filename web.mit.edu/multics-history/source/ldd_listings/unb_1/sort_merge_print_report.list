       29   if report_sw ^= "01"b then do;
       30      if input_driver_is_sort = on & output_driver_is_sort = on then do;
       31 	if read_count = release_count & read_count = return_count & read_count = write_count then do;
       32 	     call print_sorted;
       33 	     end;
