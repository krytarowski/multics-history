       37 /*   Start:   */
       38 start:
       39     state = 7;
       40      on cleanup call sort_cleanup_work;
       41      merge_compares = compares_counter - presort_compares;
       42      call print_report;
       43      if debug_sw = "0"b then do;
       44  	if rec_ptr_a ^= null() then call hcs_$delentry_seg(rec_ptr_a, hcs_code);
       45 	rec_ptr_a = null();
       46 	if rec_ptr_b ^= null() then call hcs_$delentry_seg(rec_ptr_b, hcs_code);
       47 	rec_ptr_b = null();
       48 	end;
       49      else do;
       50 	if rec_ptr_a ^= null() then call hcs_$truncate_seg(rec_ptr_a, 0, hcs_code);
       51 	if rec_ptr_a ^= null() then call hcs_$truncate_seg(rec_ptr_b, 0, hcs_code);
       52 	end;
       53      call sort_cleanup_work;
       54 exit:	state = 8;
       55      return;		/*   to caller   */
       56 
       57 
       58 print_report:  proc;
       59      if report_sw = "01"b then return;
       60      if terminate_print_sw = "0"b then return;
       61      if release_count = return_count then
       62 	do;
       63 	     if user_out_sw ^= "" then call ioa_$ioa_stream(user_out_sw,
       64 		"^7d records ^aed.", release_count, substr(whoami, 1, 4));
       65 	end;
       66      else
       67 	do;
       68 	     if user_out_sw ^= "" then call ioa_$ioa_stream(user_out_sw,
       69 		"^7d records released to ^a.", release_count, before(whoami, "_"));
       70 	     if user_out_sw ^= "" then call ioa_$ioa_stream(user_out_sw,
       71 		"^7d records returned from ^a.", return_count, before(whoami, "_"));
       72 	end;
       73 end print_report;
       74 
       75 end terminate;
