       89 /*   Start.   */
       90 
       91      call state_test(state_code);		/*   Test state variable.   */
       92      if state_code ^= 0 then do;
       93 	arg_err_sw = off;
       94 	fatal_sw = on;
       95 	call com_err_(0, "sort", " Sort will not be attempted.");
       96 	return;		/*   To command level without resetting state variable   */
       97 	end;
       98      etime(1) = clock_();
       99      call cpu_time_and_paging_(pf(1), vtime(1), pd_f(1));
      100      call hcs_$get_process_usage(addr(pu(1)), hcs_code);
      101 
      102      temp_sd,
      103      perm_sd = null();	/*   used by cleanup procedure   */
      104      debug_sw = off;	/*   used by cleanup procedure   */
      105      on cleanup call cleanup_proc;
      106      on sub_error_ call sort_merge_sub_error;
      107      call cu_$arg_count(total_args);
      108 /*   Get pointer to argument list, so that arguments may be obtained
      109*     within any procedure via cu_$arg_ptr_rel   */
      110      call cu_$arg_list_ptr(arg_list_ptr);
      111 
      112      call sort_merge_command(input_file, output_file, sort_desc, temp_dir, user_out_sw_temp, total_if_pns,
      113 			arg_err_sw, fatal_sw, no_args_sw, merge_order_temp, string_size_temp,
      114 			file_size_temp, total_args, arg_list_ptr, perm_sd, temp_sd);
      115 
      116      if no_args_sw = on then go to exit;	/*   Prompting message printed by sort_merge_command.   */
      117      if arg_err_sw = on then sort_code = 1;
      118      /*   to ensure that sort_ will not attempt to execute sorting process   */
      119      else sort_code = 0;
      120      if fatal_sw = off then		/*   go on if Sort Description is readable.   */
      121      call sort_$sort_private(input_file_temp, output_file, sort_desc, temp_dir, user_out_sw_temp, file_size_temp,
      122 			sort_code, merge_order_temp, string_size_temp);
      123 
      124      call sort_merge_command_finish(arg_err_code, sort_code, arg_err_sw, fatal_sw);
      125 
      126 exit:
      127      call cleanup_proc;
      128      state = 0;
      129      return;     /*  to command level   */
