      131 state_test: proc(state_code);		/*   Test state variable:   */
      132 dcl  state_code fixed bin(35) parameter;
      133      if state = 0
      134 	| (state = 8 & index(whoami, "_") ^= 0)		/*   Subroutine called last time   */
      135 	then call set_state;
      136      else do;		/*   Error   */
      137 	call query(pending_string);
      138 	if answer = "no" then do;
      139 	     state_code = 1;
      140 	     return;
      141 	     end;
      142 	else call set_state;		/*   answer = yes   */
      143 	end;
