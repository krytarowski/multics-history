      148 form_line:  proc(name, etime, rtime, vtime, pf, pd_f, compares);
      149 dcl (name char(*),
      150      etime fixed bin(71),
      151      rtime fixed bin(71),
      152      vtime fixed bin(71),
      153      pf    fixed bin,
      154      pd_f fixed bin,
      155      compares fixed bin(34) ) parameter;
      156      call ioa_$nnl("^8a ^8.2f ^8.2f ^8.2f ^6d ^6d",
      157 	name,
