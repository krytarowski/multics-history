       50 
       51 /*   Start   */
       52 
       53 /*   Test for errors found by sort(merge):   */
       54      call test_arg_error(arg_err_code);
       55      if arg_err_code ^= 0 then return;
       56 
       57 /*   Test for errors found by sort_(merge_):   */
       58      if sort_code ^= 0 then call test_sort_error;
       59      else if time_sw = on then do;
       60 	etime(5) = clock_();
       61 	call cpu_time_and_paging_(pf(5), vtime(5), pd_f(5));
       62 	call hcs_$get_process_usage(addr(pu(5)), hcs_code);
       63 	call print_time_info;
       64 	end;
       65      return;		/*   To caller (sort or merge).   */
       66 
       67 test_arg_error:  proc(code);		/*   Process errors detected in argument processing:   */
       68 dcl  code fixed bin(35) parameter;
       69      code = 0;
       70      if arg_err_sw = on then do;	/*   Checked before fatal_sw since both may be on.   */
       71 	call sort_com_err(1);		/*   Bad argument(s).   */
       72 	code = 1;
       73 	end;
       74      else if fatal_sw = on then do;
       75 	call sort_com_err(0);	/*   Fatal error.   */
       76 	code = 1;
       77 	end;
       78 end test_arg_error;
       79 
       80 test_sort_error:  proc;		/*   Process error detected by sort_(merge_).   */
       81      if sort_code = error_table_$bad_arg then do;	/*   Bad argument(s) detected by sort_(merge_).   */
       82 	call sort_com_err(1);
       83 	end;
       84      else do;			/*   Some other error detected by sort_(merge_).   */
       85 	call convert_status_code_(sort_code, shortinfo, longinfo);
       86 	call com_err_(0, whoami, " ^a  ^a is abandoned.", longinfo, caps(whoami));
       87 	end;
       88 end test_sort_error;
       89 
       90 sort_com_err:  proc(code);
       91 dcl  code fixed bin parameter;
       92      if code ^= 0 then call com_err_(0, whoami,
       93 			" Errors in arguments.  ^a will not be attempted.", caps(whoami));
       94      else call com_err_(0, whoami, " ^a will not be attempted.", caps(whoami));
       95 end sort_com_err;
       96 
       97 
       98 caps:  proc(whoami)  returns(char(5));
       99 dcl  whoami  char(*) parameter;
      100      if substr(whoami, 1, 4) = "sort" then return("Sort");
      101      else return("Merge");
      102 end caps;
      103 
      104 
      105 print_time_info:  proc;
      106 dcl  head char(8);
      107      if whoami = "sort" then do;
      108 	call ioa_("^/Merge order = ^d.", mii);
      109 	call ioa_("String size =~ ^d.", max2);
      110 	end;
      111      if compares_counter > 0 then head = "COMPARES"; else head = "";
      112      call ioa_("^/PHASE^3x^2xELAPSED^5xRCPU^5xVCPU^2xPAGES^2xP_DEV ^8a", head);
      113      call form_line("Overhead",
      114 	etime(5) - etime(4) + etime(2) - etime(1),
      115 	rcpu(5) - rcpu(4) + rcpu(2) - rcpu(1),
      116 	vtime(5) - vtime(4) + vtime(2) - vtime(1),
      117 	   pf(5) -    pf(4) +    pf(2) -    pf(1),
      118 	pd_f(5) - pd_f(4) + pd_f(2) - pd_f(1),
      119 	0);
      120      if whoami = "sort" then do;
      121 	call form_line("Presort",
      122 	     etime(3) - etime(2),
      123 	     rcpu(3) - rcpu(2),
      124 	     vtime(3) - vtime(2),
      125 	        pf(3) -    pf(2),
      126 	     pd_f(3) - pd_f(2),
      127 	        presort_compares);
      128 	end;
      129      call form_line("Merge",
      130 	etime(4) - etime(3),
      131 	rcpu(4) - rcpu(3),
      132 	vtime(4) - vtime(3),
      133 	   pf(4) -    pf(3),
      134 	pd_f(4) - pd_f(3),
      135 	   merge_compares);
      136      call form_line("TOTAL",
      137 	etime(5) - etime(1),
      138 	rcpu(5) - rcpu(1),
      139 	vtime(5) - vtime(1),
      140 	   pf(5) -    pf(1),
      141 	pd_f(5) - pd_f(1),
      142 	   compares_counter);
      143      call ioa_("");
      144 
      145 end print_time_info;
