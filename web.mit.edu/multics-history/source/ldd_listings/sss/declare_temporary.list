       51 	s = create_symbol(null,null,by_compiler);
       52 	bit36=type;
       53 	s->symbol.c_dcl_size=precision;
       54 	s->symbol.scale=scale;
       55 	s->symbol.dcl_size=length;
       56 	s->symbol.temporary = "1"b;
       57 	s->symbol.exp_extents = length ^= null;
