       66 
       67 	if s->symbol.bit | s->symbol.char | s->symbol.picture
       68 	then do;
       69 		q = s->symbol.reference;
       70 		q -> reference.padded_ref = "1"b;
       71 		if s->symbol.varying
       72 		then	q->reference.varying_ref = "1"b;
