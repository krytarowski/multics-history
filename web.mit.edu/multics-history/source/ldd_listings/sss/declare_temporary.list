       43 		if bit36^=type then goto next_temporary;
       44 		if s->symbol.c_dcl_size^=precision then goto next_temporary;
       45 		if s->symbol.scale^=scale then goto next_temporary;
       46 		if s->symbol.dcl_size^=null then goto next_temporary;
