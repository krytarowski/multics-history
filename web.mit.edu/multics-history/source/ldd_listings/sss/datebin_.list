       60 dcl shift_from_whotab bit aligned;
       61 dcl ec fixed bin (35);
       62 dcl (clk, save_clock) fixed bin (71);
       63 dcl (shift, day, month, year, hour, minute, second, day_of_week, abs_date, day_of_year) fixed bin;
       64 dcl (sys_hour, sys_minute, sys_day_of_week) fixed bin;
