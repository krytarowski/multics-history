       66       dcl	    p_bci_header_ptr       ptr;
       67       dcl	    p_return_ci	       fixed bin (24) uns parameter;
       68       dcl	    p_code	       fixed bin (35) parameter;
       69 
       70 /* Automatic */
       71 
       72       dcl	    code		       fixed bin (35);
       73       dcl	    (input_ci, last_ci, new_ci)
       74 			       fixed bin (24) uns init (0);
       75       dcl	    total_free_bytes       fixed bin (35) init (0);
       76       dcl	    fits_in_ci	       bit (1) aligned init ("0"b);
       77 
       78 /* Based */
       79 /* Builtin */
