      163 		go to RETURN_FROM_MSEG_OPERATIONS_;
      164 	     end;
      165 
      166 	last_operation_id = last_operation_id + 1;
      167 
      168 	mseg_operation_data_ptr = addr (p_operation_value);
      169 	mseg_operation_data.operation_id = last_operation_id;
