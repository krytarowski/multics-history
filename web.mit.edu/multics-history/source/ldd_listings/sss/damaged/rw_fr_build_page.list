     1072 	     = substr (pc_column_value, 1, pc_no_of_digits_to_the_left);
     1073 	else if pc_the_actual_decimal - 1 > pc_no_of_digits_to_the_left
     1074 	     then substr (fc_output_column, 1, pc_no_of_digits_to_the_left)
