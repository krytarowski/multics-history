       24 	/* gets unique copy of processed tree (thus a unique value) */
       25 
       26 	/* this program fails if subnodes of the tree (other than the
       27*	   result of std_call and last element of a list) have reference
       28*	   counts greater than 1 */
       29 
       30 	q = qp;
       31 
       32 	if q = null then return(q);
       33 
       34 	if q -> node.type = reference_node
       35 	   then do;
       36 	        if q -> reference.shared & q -> reference.processed
       37 		 then p = q;
       38 		 else do;
       39 		      p = create_reference(null);
       40 		      p -> reference = q -> reference;
       41 		      if ^ p -> reference.shared
       42 			then p -> reference.ref_count = 1;
       43 		      if p -> reference.qualifier ^= null
       44 		         then p -> reference.qualifier = copy_unique_expression((p -> reference.qualifier));
       45 		      if p -> reference.length ^= null
       46 		         then p -> reference.length = copy_unique_expression((p -> reference.length));
       47 		      if p -> reference.offset ^= null
       48 		         then p-> reference.offset = copy_unique_expression((p -> reference.offset));
       49 		      end;
       50 	        return(p);
       51 	        end;
       52 
       53 	if q -> node.type = operator_node
       54 	   then do;
       55 	        n = q -> operator.number;
       56 	        p = create_operator((q -> operator.op_code),n);
       57 	        p -> operator = q -> operator;
       58 
       59 	        do i = 1 to n;
       60 		 p -> operand(i) = copy_unique_expression((p -> operand(i)));
       61 	        end;
       62 
       63 	        if p -> operator.op_code = std_call
       64 		 then if p -> operand(3)^= null
       65 		      then do;
       66 			 arg = p -> operand(3) -> operand(2);
       67 			 arg -> element(arg -> list.number) = p -> operand(1);
       68 			 if p -> operand(1) ^= null
       69 			    then p -> operand(1) -> reference.ref_count = 2;
       70 			 end;
       71 
       72 	        return(p);
       73 	        end;
       74 
       75 	if q -> node.type = list_node
       76 	   then do;
       77 	        p = create_list((q -> list.number));
       78 	        p -> list = q -> list;
       79 
       80 	        do i = 1 to p -> list.number - 1;
       81 		 p -> element(i) = copy_unique_expression((p -> element(i)));
       82 	        end;
       83 
       84 	        return(p);
       85 	        end;
       86 
       87 	return(q);
       88 
       89 	end;
