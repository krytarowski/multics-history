        8 /*
        9*
       10*     This module is called by the the rw_display module to delete
       11*     the environment set up by rw_fr_new_report.
       12*
       13* 
       14*     Known Bugs:
       15* 
       16*     Other Problems:
       17*
       18*     History:
       19*
       20*     Written - Al Dupuis - August 1983
       21*     Changed - Al Dupuis - October 1984 - Became report_writer_.
       22*
       23**/
       24 
       25 rw_fr_delete_report: proc (
       26 
       27 	report_cip_parm,	/* input: point to report_control_info */
       28 	code_parm		/* output: success or failure */
       29 		         );



       30 
       31 dcl code_parm fixed bin (35) parm;
       32 dcl report_cip_parm ptr parm;



       33 
       34 	report_cip = report_cip_parm;
       35 	code_parm = 0;
       36 	sci_ptr = report_control_info.subsystem_control_info_ptr;

       37 
       38 	/* If we kept a copy of the original report while we were making it then delete it. */

       39 
       40 	if report_control_info.flags.permanent_report
       41 	then do;
       42 	     format_report_ip = report_control_info.format_report_info_ptr;
       43 	     iocb_ptr = format_report_info.report_iocb_ptr;
       44 	     if iocb_ptr ^= null ()
       45 	     then do;
       46 		call iox_$close (iocb_ptr, code);
       47 		if code ^= 0
       48 		then call ssu_$print_message (sci_ptr, code,
       49 		     "While trying to close ^p.", iocb_ptr);
       50 		call iox_$detach_iocb (iocb_ptr, code);
       51 		if code ^= 0
       52 		then call ssu_$print_message (sci_ptr, code,
       53 		     "While trying to detach ^p.", iocb_ptr);
       54 		call iox_$destroy_iocb (iocb_ptr, code);
       55 		if code ^= 0
       56 		then call ssu_$print_message (sci_ptr, code,
       57 		     "While trying to destroy ^p.", iocb_ptr);
       58 		call delete_$path (format_report_info.report_directory_name,
       59 		     
       60 		     format_report_info.report_entry_name,
       61 		     "100111"b, "display", code);
       62 		if code ^= 0
       63 		then call ssu_$print_message (sci_ptr, code,
       64 		     "While trying to delete ^a in directory^/^a.",
       65 		     format_report_info.report_entry_name,
       66 		     format_report_info.report_directory_name);
       67 		format_report_info.report_iocb_ptr = null ();
       68 		report_control_info.flags.permanent_report = "0"b;
       69 	     end;
       70 	end;

       71 
       72 	/* Refresh our area instead of freeing everything. */

       73 
       74 	work_area_ptr = report_control_info.report_work_area_ptr;
       75 	call ssu_$release_area (sci_ptr, work_area_ptr);
       76 	call ssu_$get_area (sci_ptr, report_control_info.area_info_ptr,
       77 	     "rw_.report_area", report_control_info.report_work_area_ptr);

       78 
       79 	return;
       80 
       81 dcl code fixed bin (35);

       82 
       83 dcl delete_$path entry (char(*), char(*), bit(6), char(*), fixed bin(35));

       84 
       85 dcl iocb_ptr ptr;
       86 dcl iox_$close entry (ptr, fixed bin(35));
       87 dcl iox_$destroy_iocb entry (ptr, fixed bin(35));
       88 dcl iox_$detach_iocb entry (ptr, fixed bin(35));

       89 
       90 dcl null builtin;

       91 
       92 dcl sci_ptr ptr;
       93 dcl ssu_$get_area entry (ptr, ptr, char(*), ptr);
       94 dcl ssu_$print_message entry() options(variable);
       95 dcl ssu_$release_area entry (ptr, ptr);

       96 
