      552 CLEAN_UP: proc ();
      553 
      554 /* cleanup and exit procedure -- terminates all the input archives */
      555 
      556 dcl  idx fixed bin;
      557 dcl  tempp pointer;
      558 
      559 	if inpp ^= null then do;
      560 	     do idx = 1 to inp.ntotal;
      561 		if inp.archive (idx).ptr ^= null () then do;
      562 		     tempp = inp.archive (idx).ptr;
      563 		     inp.archive (idx).ptr = null ();
      564 		     call hcs_$terminate_noname (tempp, (0));
      565 		end;
      566 	     end;
      567 
      568 	     call release_temp_segment_ (WHOAMI, inpp, (0));
      569 	     binder_invoked = "0"b;			/* Always turn off the flag */
      570 	end;
      571 
      572 	return;
      573 	end CLEAN_UP;
