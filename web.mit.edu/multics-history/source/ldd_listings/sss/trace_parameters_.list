      708 APPEND_N_PARAMETER:
      709     proc (A_n_i, B_n_i, control_arg_name_i);
      710       dcl	 A_n_i			   fixed bin (34) parm;
      711       dcl	 B_n_i			   fixed bin (34) parm;
      712       if just_differences & A_n_i = B_n_i
