      721       dcl	 A_onoff_i		   bit aligned parm;
      722       dcl	 B_onoff_i		   bit aligned parm;
      723       if just_differences & A_onoff_i = B_onoff_i
      724       then return;
      725       if A_onoff_i = "0"b
      726       then one_parm_string = "-" || control_arg_name_i || " off";
      727       else one_parm_string = "-" || control_arg_name_i || " on";
      728       go to APPEND_STRING;
