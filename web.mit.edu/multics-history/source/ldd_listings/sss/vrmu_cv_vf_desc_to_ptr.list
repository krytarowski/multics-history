       69 extend_seg_ptr_array: proc;
       70 
       71 	old_array_limit = seg_ptr_array_limit;
       72 	old_array_ptr = seg_ptr_array_ptr;
       73 	f_b_ptr = indx_cb.file_base_ptr;
       74 	seg_ptr_array_limit = file_base.last_comp_num;
       75 	allocate seg_ptr_array in (get_system_free_area_ () -> cb_area) set (seg_ptr_array_ptr);
       76 
       77 	do i = 0 to old_array_limit;
       78 	     seg_ptr_array (i) = old_array (i);
       79 	end;
       80 	do i = old_array_limit + 1 to seg_ptr_array_limit;
       81 	     seg_ptr_array (i) = null ();
       82 	end;
       83 
       84 	free old_array;				/* in systemfree */
       85 
       86 
       87 	dcl     cb_area		 area based;
       88 	dcl     get_system_free_area_	 entry () returns (ptr);
       89 	dcl     i			 fixed bin;
       90 	dcl     old_array_limit	 fixed bin;
       91 	dcl     old_array_ptr	 ptr;
       92 	dcl     old_array		 (0:old_array_limit) ptr based (old_array_ptr);
       93 
       94      end extend_seg_ptr_array;
