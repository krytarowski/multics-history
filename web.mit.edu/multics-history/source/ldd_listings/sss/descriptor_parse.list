       77 	     name = token;
       78 	     in_generic = (name = null);
       79 	     level = 0;
       80 
       81 	     if in_generic & t_table.type = asterisk	/* no attributes for this descriptor */
       82 	     then do;
       83 		     s = null;
       84 		     k = k + 1;
       85 		end;
       86 	     else do;
       87 		     if t_table.type = dec_integer		/* a structure level number */
       88 		     then do;
       89 			     level = token_to_binary(token_list(k));
       90 			     k=k+1;
       91 			end;
       92 
       93 		     if ^in_generic
       94 		     then if substr (name -> t_table.string, name -> t_table.size, 1) ^= "]"
       95 			then name = create_token (name -> t_table.string || bindec$vs ((n)) || "]", identifier);
       96 
       97 		     s = create_symbol(cblock,name,by_compiler);
       98 		     s->symbol.level = level;
       99 		     call link_symbol(previous_descriptor,s);	/* perform appropriate structure linking */
      100 		     k=k-1;
      101 		     call attribute_parse(cblock,s,k,in_generic);
      102 		end;
      103 
      104 	     if level <= 1
      105 	     then do;
      106 		     p = create_list(2);
      107 		     p->list.element(1) = s;
      108 
      109 		     if listptr = null
      110 		     then listptr = p;
      111 		     else previous_listptr->list.element(2) = p;
      112 		     previous_listptr = p;
      113 		end;
      114 
      115 	     if t_table.type = comma
      116 	     then k = k + 1;
      117 	     else if t_table.type ^= right_parn
      118 		then call declare_parse$abort (1, null);
      119 	end;
