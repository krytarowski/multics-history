      601 	     call ioa_ ("Warning: the -long control argument is only valid for
      602 printer, punch or plotter generic types.");
      603 	     info.long_sw = ""b;			/* if we don't know the structure of this generic type */
      604 	end;					/* just list the stuff in the message header */
      605 
      606 /* Now set first and last queue according to arguments or defaults (but -q argument processing sets them in-line) */
      607 
      608 	if info.all_opt_sw | info.search_all then do;
