      639 		call release_temp_segment_ (id, tp, code); /* give it back */
      640 	end;					/* end begin block */
      641 
      642 /* END CONDITION HANDLER */
      643 
      644 	call get_temp_segment_ (id, tp, code);
      645 	if code ^= 0 then do;
      646 	     err_msg = "temporary segment";
      647 	     call print_err_msg (-1, code);
      648 	     return;
      649 	end;
      650 
      651 /* Initialize, before entering loop over queues */
