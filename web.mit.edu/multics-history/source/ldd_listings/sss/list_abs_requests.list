      670 		call ioa_$rsnnl ("^[Attempting to open^x^;^]^a>^a", err_msg, rs_len, (info.mseg_idx = 0), dir, ent);
      671 		call print_err_msg (-1, code);
      672 	     end;
      673 	     info.mseg_idx = 0;			/* so cleanup handler doesn't try to close it again */
      674 	     info.input_count = info.output_count;	/* copy new length of temorary */
      675 	end;
      676 
      677 /* End of loop over queues. Now print the segment built by lar_util_. */
      678 
      679 /* Under some circumstances, lar_util_ will put nothing in the segment, rather than put out lots of lines
      680*   that say "no requests". So here we check for those circumstances, and print a line of explanation. */
      681 
      682 	if (info.all_opt_sw & info.message_count = 0 & ^info.no_total_sw) /* if -all and Qs were all empty */
      683 	| (info.search_all & info.select_count = 0) then do; /* or -search_all and no requests were selected */
      684 
      685 /* We want to say one of:
      686*   There are 		}
      687*   You have 		} no requests in any ^a queue.
      688*   ^a.^a has 		}
      689*   Selection arguments matched}
      690*
      691*   Figure out which. */
      692 
      693 	     if info.message_count = 0 then
      694 		i = 1;				/* There are no requests in ... */
      695 	     else if info.user_select_count = 0 then
      696 		if ^info.user_sw then		/* if user name not given */
      697 		     i = 2;			/* You have no requests in ... */
      698 		else i = 3;			/* ^a.^a has no requests in ... */
      699 	     else i = 4;				/* Selection arguments matched no requests in ... */
      700 
      701 /* Now say it */
      702 
      703 	     call ioa_ (
      704 		"^[^2sThere are^;^2sYou have^;^a.^a has^;^2sSelection arguments matched^] no requests in any ^a queue.",
      705 		i, info.person, info.project, info.queue_name);
      706 
      707 	end;
      708 
      709 /* If we printed the above message, there should be nothing in the segment except an initial newline.
      710*   If there is something else, we have a bug. Print it, so the bug will be obvious. */
      711 
      712 	if info.output_count > 1 then do;
      713 	     call iox_$put_chars (iox_$user_output, info.temptr, info.output_count, code);
      714 	     if code ^= 0 then do;
      715 		err_msg = "user_output";
      716 		call print_err_msg (-1, code);
