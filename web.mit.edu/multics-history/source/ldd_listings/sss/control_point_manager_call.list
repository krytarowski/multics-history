      921 
      922 	if display_the_id
      923 	then call ioa_ ("Control point ^12.3b created^[^[, started, and marked preferred^; and started^]^].",
      924 		control_point_id, start_the_control_point, prefer_the_control_point);
      925 
      926 	return;
      927 
