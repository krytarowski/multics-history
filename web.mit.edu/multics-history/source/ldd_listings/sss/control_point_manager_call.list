      673 		then do;
      674 			if users_entry_ptr ^= null ()
      675 			then do;
      676 				if have_users_info_ptr
      677 				then do;
      678 					call cv_ptr_$terminate (users_entry.info_ptr);
      679 					have_users_info_ptr = "0"b;
      680 				     end;
      681 				free users_entry in (system_area);
      682 				users_entry_ptr = null ();
      683 			     end;
      684 		     end;
