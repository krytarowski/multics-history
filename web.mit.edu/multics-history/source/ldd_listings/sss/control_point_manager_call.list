      648 create_control_point$run:
      649      entry ();
      650 
      651 	if n_arguments = 0
      652 	then call display_usage_message_and_abort ("{-control_args} COMMAND_LINE");
      653 
      654 	create_operation = "0"b;
      655 	run_operation = "1"b;
      656 
      657 	go to CREATE_CONTROL_POINT;
