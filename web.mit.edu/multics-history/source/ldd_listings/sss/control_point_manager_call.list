      611 dcl	1 ccpi_header	like create_control_point_info.header aligned;
      612 
      613 dcl	atd_argument	character (atd_argument_lth) based (atd_argument_ptr);
      614 dcl	atd_argument_ptr	pointer;
      615 dcl	atd_argument_lth	fixed binary (21);
