      113 	  if p_code ^= 0
      114 	  then call ERROR_RETURN;
      115          end;
      116       call
      117          CHECK_VERSION (definition_ptr -> print_vector_array.version, PRINT_VECTOR_ARRAY_VERSION_2, "print_vector_array");
      118 
      119       call SET_ID_REQUIRED (p_result_ptr, OPTION_NAME_DIM_NAME, option_name_identifier);
