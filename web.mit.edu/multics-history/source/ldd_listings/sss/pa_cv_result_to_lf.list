      169 SET_ID_REQUIRED:
      170    entry (si_p_pva_ptr, si_p_dim_name, si_p_dim_id);
      171       si_dim_is_required = "1"b;
      172 
      173 SI_JOIN:
      174       do si_p_dim_id = 1 to si_p_pva_ptr -> print_vector_array.number_of_dimensions
      175          while (si_p_pva_ptr -> print_vector_array.dimension_table (si_p_dim_id).name ^= si_p_dim_name);
      176       end;
      177       if si_p_dim_id > si_p_pva_ptr -> print_vector_array.number_of_dimensions
