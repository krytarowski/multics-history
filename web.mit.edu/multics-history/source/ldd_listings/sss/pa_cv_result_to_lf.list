      180 	         sub_err_ (error_table_$bad_arg, myname, ACTION_CANT_RESTART, null, 0,
      181 	         "^/The argument processing definition structure must have a ^a dimension in it,
      182 but the one provided does not.", requote_string_ ((si_p_dim_name)));
      183 	 else si_p_dim_id = UNDEFINED_DIMENSION;
      184 
