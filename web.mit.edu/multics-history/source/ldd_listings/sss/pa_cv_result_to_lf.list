      132 	  call GET_VALUE (print_vector_ptr, option_name_identifier, option_name);
      133 	  call GET_VALUE (print_vector_ptr, instance_identifier, instance_count);
      134 	  if instance_count = old_instance_count
      135 	  then call GET_ARG_VALUES (blfb_is_arg_dimension, print_vector_ptr, current_linear_form);
      136 	  else call GET_REFNAME_AND_VALUES (blfb_is_arg_dimension, print_vector_ptr, option_name, current_linear_form);
      137 	  p_linear_form = p_linear_form || " " || current_linear_form;
      138          end BLFB_PRINT_VECTOR_LOOP;
      139       end BUILD_LINEAR_FORM_BLOCK;
      140       call FINISH;
