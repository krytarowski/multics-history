      105 
      106       call SET_ID_REQUIRED (p_result_ptr, COMMAND_NAME_DIM_NAME, command_name_id);
      107       call GET_VALUE (p_result_ptr -> print_vector_array.vector_slot (1), command_name_id, result_command_name);
