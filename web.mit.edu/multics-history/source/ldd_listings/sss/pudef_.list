       42 defcnt:	entry( ival ) ;	/* entry to count the number of relocation bits for the definition portion */
       43 
       44 	ival = 0 ;
       45 
       46 	itemp = idfpc - 1 ;
       47 
       48 label_1000:
       49 	do i = 0 to itemp ;
       50 
       51 		call object_$getbdf( i, lbits, rbits ) ;
       52 
       53 		nl = 1 ;
       54 
       55 		nr = 1 ;
       56 
       57 		if lbits ^= 0 then nl = 5 ;
       58 
       59 		if rbits ^= 0 then nr = 5 ;
       60 
       61 		ival = nl + nr + ival ;
       62 
       63 end label_1000 ;
       64 
       65 
       66 	idfcnt = ival ;
       67 
