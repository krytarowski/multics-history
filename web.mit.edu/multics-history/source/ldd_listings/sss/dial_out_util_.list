      119 send:
      120      procedure (Data_ptr, Nelem, Code);
      121 
      122 dcl  Code fixed bin (35) parameter;
      123 dcl  Data_ptr ptr parameter;
      124 dcl  Nelem fixed bin (21) parameter;
      125 
      126 rewrite:
      127 	call iox_$put_chars (do_info.ci.net_iocb, Data_ptr, Nelem, Code);
      128 	if Code = error_table_$line_status_pending then do;
      129 	     call line_status;
      130 	     goto rewrite;
      131 	     end;
      132 	return;
      133      end send;
