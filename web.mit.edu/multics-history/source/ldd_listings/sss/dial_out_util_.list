      289 		if code ^= 0 then do_info.net_rs.data_available = "1"b;
      290 
      291 		if do_info.net_rs.data_available
      292 		then call net_input;
      293 		else need_to_check_net = "0"b;
      294 		end;
      295 
      296 	     if need_to_check_term & ^waiting then do;
      297 		call iox_$control (iox_$user_input, "read_status", addr (term_rs), code);
      298 		if code ^= 0 then term_rs.data_available = "1"b;
      299 
      300 		if term_rs.data_available
      301 		then call terminal_input ("0"b);
