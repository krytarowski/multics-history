      103 /* * * * * * * * * * SEND_NL * * * * * * * * * */
      104 
      105 send_nl:
      106      entry (Dop, Code);
      107 
      108 	dop = Dop;
      109 	call send (addr (CR), 1, Code);
      110 	if Code ^= 0 then return;
      111 	if do_info.flags.send_lf_sw then do;
      112 	     call send (addr (NL), 1, Code);
      113 	     if Code ^= 0 then return;
      114 	     end;
      115 	return;
