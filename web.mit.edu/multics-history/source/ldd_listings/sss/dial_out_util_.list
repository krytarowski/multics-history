      280 	in_command, need_to_echo_command, escape_seen = "0"b;
      281 	need_to_check_net, need_to_check_term = "1"b;	/* set everything to give us a chance to see what's there */
      282 	cmd_len = 0;
      283 	term_rs.ev_chn = 0;
      284 	do_info.abort_code = 0;
