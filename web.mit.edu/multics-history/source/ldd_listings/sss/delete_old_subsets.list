       53 
       54 	dname =
       55 	     substr (attach_descrip, PATHNAME_START,
       56 	     p_iocb_ptr -> iocb.actual_iocb_ptr -> iocb.attach_data_ptr -> atb.dname_len);
       57 	ename =
