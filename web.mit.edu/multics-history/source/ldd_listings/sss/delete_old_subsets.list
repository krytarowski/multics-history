       73 	if code ^= 0
       74 	then call sub_err_ (code, myname, "s", null, 0, "Unable to open the msf for temporary subsets.");
       75 
       76 	call msf_manager_$adjust (fcb_ptr, 0, 0, SET_BC_AND_TRUNCATE, code);
       77 	if code ^= 0
       78 	then call sub_err_ (code, myname, "s", null, 0, "Unable to adjust the msf ofr temporary subsets.");
