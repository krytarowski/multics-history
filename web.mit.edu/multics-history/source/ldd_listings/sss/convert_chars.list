      614 	if lp -> operator.op_code = negate then n_args = 5;
      615 	else do;
      616 	     n_args = 7;
      617 	     arg(6) = ref(3);
      618 	     arg(7) = get_desc(arg(6));
      619 	     c = c | arg(6) -> reference.symbol -> symbol.complex;
      620 	     end;
      621 
      622 	if c | s -> symbol.complex then k = k1; else k = k2;
