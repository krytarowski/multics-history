      192 
      193 	p1, arg(1) = left;
      194 	p2, arg(2) = right;
      195 	right = null;
      196 
      197 	call state_man$flush_ref(p1);
      198 
      199 /* Initialize by filling in arrays */
      200 
      201 	do i = 1 to 2;
      202 	sym(i) = arg(i) -> reference.symbol;
