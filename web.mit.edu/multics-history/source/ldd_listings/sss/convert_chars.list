      279 		     if sym(1) -> symbol.unaligned
      280 		     then do;
      281 			     q -> reference.units = digit_;
      282 			     q -> reference.c_length = 2;
      283 			     macro = zero_4bcs;
      284 			end;
      285 		     else do;
      286 			     q -> reference.units = character_;
      287 			     q -> reference.c_length = 1;
      288 			     macro = zero_cs;
      289 			end;
      290 
      291 		     call expmac$one_eis(macro,q);
