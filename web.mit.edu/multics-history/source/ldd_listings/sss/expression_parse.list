      222 	/* have constant operands, evalute expression if possible */
      223 
      224 	q = evaluate((op_table(opindex)),r2,r3);
      225 	goto pop;
      226 
      227 fail:	r = null;
      228 	go to ret;
      229 
      230 succeed:	r = stack(0);
      231 	i = k;
      232 
      233 ret:	return(r);
