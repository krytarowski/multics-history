      245 /*****	Declaration for builtin function	*****/
      246 
      247 dcl	(substr, mod, binary, fixed, addr, addrel, rel, length, string, unspec, null, index)
      248 			builtin;
      249 
      250 /*****	End of declaration for builtin function	*****/
