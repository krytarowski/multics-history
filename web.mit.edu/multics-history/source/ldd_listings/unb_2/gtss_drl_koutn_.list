       14 gtss_drl_koutn_: proc (mcpp, increment);
       15 
       16 /**	Terminal output then input.
       17*
       18*   Authors: Robert J. Grimes	Created
       19*   -	  Albert N. Kepner	  1978
       20*   -	  Robert M. May
       21*   -	  David B. Ward
       22*   Changed: Al Dupuis 05/08/79
       23*                              To have gtss_ext_$get_line called instead
       24*                              of iox_$get_line. Also bytes_read and code
       25*                              to match gtss_ext_'s declares.
       26*   Changed: Dave Ward	07/01/79 Long input lines.
       27*
       28**/
       29 dcl  mcpp                     ptr parm;
       30 dcl  increment                fixed bin parm;
       31 	mcp = mcpp;
       32 
       33 /*
       34*
       35*   DRL KOUTN, KEYBOARD OUTPUT THEN INPUT (octal 3)
       36*
       37*
       38*   8_________1_6_____________________
       39*
       40*   DRL      KOUTN
       41*   ZERO     L(tally),L(char)
       42*
       43*
       44*   This derail sends output to  the  keyboard  device  with  an
       45*   anticipated reply.  The L(tally) and L(char) fields are identical
       46*   to  those  for  the  KOUT  sequence.   In this case, however, the
       47*   Executive  adds  this  output  message  to  any  data  that   has
       48*   accumulated  in  the  keyboard  output  buffer and sends the data
       49*   directly to the keyboard device.  The transfer  of  data  differs
       50*   from  that for KOUT in that the line is left open for a response.
       51*   The response can be retrieved by means of DRL KIN.
       52*
       53**/
       54 
       55 	scup = addr (mc.scu);
       56 
       57 /**	Print caller's data on terminal. **/
       58 	call gtss_drl_kotnow_ (mcp, increment);
       59 
       60 /**	Refill remote i/o buffer with
       61*	terminal input line. (like build mode).
       62* **/
       63 reinput:	;
       64 	call gtss_ext_$get_line (iox_$user_input
       65 	     , addr (characters_transmitted)
       66 	     , min (gtss_ust.lrtll.char_length, hbound (characters_transmitted, 1))
       67 	     , bytes_read
       68 	     , code);
       69 	if code ^= 0 then do;
       70 	     if code = error_table_$long_record then do;
       71 		call iox_$control (
       72 		     iox_$user_input
       73 		     , "resetread"
       74 		     , null ()
       75 		     , code
       76 		     );
       77 dcl iox_$control entry (ptr, char (*), ptr, fixed bin (35));
       78 dcl  error_table_$long_record fixed bin (35) ext;
       79 dcl  iox_$user_output         ext ptr;
       80 
       81 dcl  retry_message            char(21)static int options(constant)init("RETRANSMIT LAST LINE
       82 ");
       83 		call gtss_ext_$put_chars (		/* Print retry message. */
       84 		     iox_$user_output
       85 		     , addr (retry_message)
       86 		     , length (retry_message)
       87 		     , code);
       88 		goto reinput;
       89 	     end;
       90 	     call com_err_ (code, "gtss_drl_koutn_", "Terminal failure.");
       91 	     signal cond (gtss_fail);
       92 	end;
       93 	count_of_characters_transmitted = bytes_read;
       94 	number_words_transmitted =
       95 	     divide ((bytes_read+3), 4, 24, 0);
       96 	characters_transmitted (bytes_read) = CR;
       97 
       98 	increment = 1;
       99 	gtss_ext_$last_k_was_out = "0"b;		/* Last tty i/o was not output. */
      100 	gtss_ust.lbuf.tally = "0"b;
      101 	return;
