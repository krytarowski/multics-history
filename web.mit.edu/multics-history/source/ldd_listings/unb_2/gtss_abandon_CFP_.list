        9 gtss_abandon_CFP_: proc;
       10 
       11 /* Reset to non-command file processing.
       12*
       13*   Author: Dave Ward 03/27/79
       14*
       15*   Changed: Al Dupuis 05/22/79 To turn off copy_on, etc...bits.
       16*   Changed: Al Dupuis 10/01/79 Added call to timer_manager_.
       17*   Changed: Al Dupuis 10/01/79 To zero out any termination code in start_term.
       18*   Changed: Al Dupuis 11/10/79 End of ..init processing.
       19**/
       20 	call timer_manager_$reset_cpu_call (gtss_fault_processor_$timer_runout);
       21 	call timer_manager_$reset_cpu_call (gtss_abs_$cpu_runout);
       22 	gtss_ext_$flags.ss_time_limit_set = "0"b;
       23 	gtss_ext_$flags.timer_ranout = "0"b;
       24 	gtss_ust_ext_$ust.gtss_ust.limit = 0;
       25 	gtss_ext_$get_line = iox_$get_line;
       26 	gtss_ext_$put_chars = iox_$put_chars;
       27 	cpos_called = "0"b;
       28 	CFP_in_progress = "0"b;
       29 	dele_on = "0"b;
       30 	copy_on = "0"b;
       31 	talk_on = "0"b;
       32 	trap_off = "0"b;
       33 	lswt2.b6 = "0"b;
       34 	gtss_ust.lcfst.start_term = 0;		/* reset any abort term code */
       35 
       36 /* If we are just finishing processing a ..init file thru CRUN, but still
       37*   have to execute DRUN (we are under absentee) prepare things	*/
       38 	if gtss_ext_$process_type = 2
       39 	then do;
       40 	     if ((gtss_ust.lcjid = "0"b)
       41 	     & (gtss_ext_$drun_jid ^= " "))
       42 	     then do;
       43 		gtss_ust.lcfst.start_term = 17;
       44 		call gtss_ascii_bcd_ (addr (gtss_ext_$drun_jid), 5, addr (gtss_ust.lcjid));
       45 		call gtss_aft_$find ("*CFP", fn, code);
       46 		if code ^= 0
       47 		then return;
       48 		call gtss_ios_close_ (fn,
       49 		     addr (fms_status),
       50 		     code);
       51 		if fms_status.bit12 ^= "4000"b3
       52 		then do;
       53 		     call ioa_ ("gtss_abandon_CFP_: "
       54 			|| "*CFP won't close. Status (^w).",
       55 			fms_status);
       56 		     return;
       57 		end;
       58 		call gtss_aft_$delete ("*CFP", fn, code);
       59 	     end;
       60 	end;
       61 	return;
       62 
       63 dcl  code				fixed bin (35);
       64 dcl  fn				fixed bin (24);
       65 dcl  ioa_                               entry options (variable);
       66 dcl  iox_$get_line            	entry ext;
       67 dcl  iox_$put_chars           	entry ext;
       68 dcl  timer_manager_$reset_cpu_call	entry (entry);
       69 dcl  1 fms_status aligned,
       70        2 bit12 bit (12) unaligned,
       71        2 bit60 bit (60) unaligned;
