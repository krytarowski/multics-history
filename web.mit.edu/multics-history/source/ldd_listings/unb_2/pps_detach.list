       63 /*
       64*
       65*   Initialize necessary data.
       66*
       67**/
       68 
       69 
       70 	code = 0;
       71 	mask = 0;
       72 	iocb_ptr = iocb_ptr_arg;
       73 	ppsab_ptr = iocb.attach_data_ptr;
       74 	ppscb_ptr = ppsab.ppscb_ptr;
