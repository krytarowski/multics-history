       14 gtss_drl_switch_: proc (mcpp, increment);
       15 
       16 /* *	gtss exchange two temp files.
       17*
       18*   Authors: Robert J. Grimes	Created
       19*   -	  Albert N. Kepner	  1978
       20*   -	  Robert M. May
       21*   -	  David B. Ward
       22*   Change:  Dave Ward	03/28/79 Added aborts, debug code.
       23*   * */
       24 dcl  mcpp ptr parm;
       25 dcl  increment fixed bin parm;
       26 	mcp = mcpp;
       27 
       28 /* *
       29*
       30*   DRL SWITCH, SWITCH TEMPORARY FILE NAMES (octal 53)
       31*
       32*
       33*   8_________1_6_____________________
       34*
       35*   DRL      SWITCH
       36*   ZERO     L(fileid1),L(fileid2)
       37*   Error return location.
       38*   Successful return location.
       39*
       40*   (See TSS SYSTEM PROGRAMMER'S
       41*   REFERENCE MANUAL DD17B, Rev. 0, July 1976
       42*   Page 3-54)
       43*
       44**/
       45 dcl  arg_ptr ptr init(null());
       46 
       47 dcl 1 arglist aligned based (arg_ptr),
       48     2 file_id_1 fixed bin (18) unsigned unaligned,
       49     2 file_id_2 fixed bin (18) unsigned unaligned;
       50 
       51 dcl  file_id_1_ptr ptr init(null());
       52 
       53 dcl  file_id_1_name char (8) based (file_id_1_ptr);
       54 
       55 dcl  file_id_2_ptr ptr init(null());
       56 
       57 dcl  file_id_2_name char (8) based (file_id_2_ptr);
       58 
       59 dcl  status fixed bin (24);
       60 dcl  code fixed bin (35);
       61 
       62 dcl  file_no_1 fixed bin (24);
       63 dcl  file_no_2 fixed bin (24);
       64 
       65 dcl  gseg ptr init(null());
       66 
       67 dcl 1 T aligned like gtss_file_attributes;
       68 
       69 
       70 /*		P R O C E D U R E 			*/
       71 
       72 
       73 	scup = addr (mc.scu);			/* get the system conditions */
       74 
       75 	increment = 1;				/* set up for possible error */
       76 
       77 	mc.regs.a = "0"b;				/* set up for error possiblity */
       78 
       79 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_); /* save pointer to this segment */
       80 
       81 /* Check for arguments out of bounds. */
       82 	if (fixed (scu.ilc, 18)+2) >= fixed (gtss_ust.lsize.limit, 18) then
       83 	     call gtss_abort_subsystem_ (
       84 	     mcp,
       85 	     "gtss_drl_switch_",
       86 	     4,
       87 	     gtss_pnterr.err4,
       88 	     fixed (scu.ilc, 18));
       89 
       90 	arg_ptr = addrel (gseg, fixed (scu.ilc) + 1);	/* wrap pointer around the arguments */
       91 
       92 /* Check for arguments out of bounds. */
       93 	if (arglist.file_id_1+1) > fixed (gtss_ust.lsize.limit, 18) then
       94 	     call gtss_abort_subsystem_ (
       95 	     mcp,
       96 	     "gtss_drl_switch_",
       97 	     4,
       98 	     gtss_pnterr.err4,
       99 	     fixed (scu.ilc, 18));
      100 
      101 /* Check for arguments out of bounds. */
      102 	if (arglist.file_id_2+1) > fixed (gtss_ust.lsize.limit, 18) then
      103 	     call gtss_abort_subsystem_ (
      104 	     mcp,
      105 	     "gtss_drl_switch_",
      106 	     4,
      107 	     gtss_pnterr.err4,
      108 	     fixed (scu.ilc, 18));
      109 
      110 	file_id_1_ptr = addrel (gseg, arglist.file_id_1);
      111 	file_id_2_ptr = addrel (gseg, arglist.file_id_2);
      112 
      113 	call gtss_aft_$find (file_id_1_name, file_no_1, code);
      114 	if code ^= 0 then
      115 	     call gtss_abort_subsystem_ (
      116 	     mcp
      117 	     , 0
      118 	     , "1st name ""^a"" not in aft."
      119 	     , file_id_1_name
      120 	     );
      121 
      122 	call gtss_aft_$find (file_id_2_name, file_no_2, code);
      123 	if code ^= 0 then
      124 	     call gtss_abort_subsystem_ (
      125 	     mcp
      126 	     , 0
      127 	     , "2nd name ""^a"" not in aft."
      128 	     , file_id_2_name
      129 	     );
      130 
      131 	call gtss_ios_exchange_names_ (file_no_1, file_no_2, status, code);
      132 	if status = 13 then mc.regs.a = (36)"1"b;	/* => Exchanging perm file. */
      133 	else
      134 	if status = 14 then do;			/* => file not open. */
      135 	     call com_err_ (0, "gtss_drl_switch_",
      136 		"File not open but in aft ""^a"" (^i) ""^a"" (^i).",
      137 		file_id_1_name, file_no_1,
      138 		file_id_2_name, file_no_2);
      139 	     call gtss_abort_subsystem_ (
      140 		mcp,
      141 		"gtss_drl_switch_",
      142 		0,
      143 		"DRL_SWITCH: IN AFT BUT NOT OPEN (BUG?) at (^60)",
      144 		fixed (scu.ilc, 18));
      145 	end;
      146 	else
      147 	if status = 15 then do;			/* Bad file number. */
      148 	     call com_err_ (0, "gtss_drl_switch_",
      149 		"Bad file nos for exchange ^i ^i.",
      150 		file_no_1, file_no_2);
      151 	     call gtss_abort_subsystem_ (
      152 		mcp,
      153 		"gtss_drl_switch_",
      154 		0,
      155 		"DRL_SWITCH: BAD FILE NUMBERS? at (^6o)",
      156 		fixed (scu.ilc, 18));
      157 	end;
      158 	else
      159 	increment = 2;				/* go back to good return */
      160 
      161 	if db_drl_switch then
      162 	     call com_err_ (0, "gtss_drl_switch_"
      163 	     , "file ""^a"" (aft ^i) switched with ""^a"" (^i)."
      164 	     , file_id_1_name, file_no_1
      165 	     , file_id_2_name, file_no_2
      166 	     );
      167 
      168 /* Exchange attributes blocks for files being switched */
      169 
      170 	T = temp_file (file_no_1);
      171 	temp_file (file_no_1) = temp_file (file_no_2);
      172 	temp_file (file_no_2) = T;
      173 
      174 	return;
