  5     3 
  5     4 /*  Last modified 07/19/78 by RAL to conform with MULTICS standard linkage map */
  5     5 /*  Last modified June 26, 1974 by AEG              */
  5     6 
  5     7 
  5     8 
  5     9 declare	1 linkage_header aligned based(link_base_ptr),
  5    10 	  2 pad bit(36),
  5    11 	  2 def_section_relp bit(18) unaligned,
  5    12 	  2 first_reference_relp bit(18) unaligned,
  5    13 	  2 obsolete_ptr ptr unal,
  5    14 	  2 original_linkage_ptr ptr unal,
  5    15 	  2 unused bit (72),
  5    16 	  2 links_relp bit(18) unaligned,
  5    17 	  2 linkage_section_length bit(18) unaligned,
  5    18 	  2 object_seg bit(18) unaligned,
  5    19 	  2 obsolete_length bit(18) unaligned;
  5    20 
  5    21 
  5    22 /*
  5    23*link_base_ptr		is a pointer to the base of the
  5    24*			linkage section and is an element
  5    25*			of the structure   cobol_ located in
  5    26*			  cobol_external_seg.
  5    27*
  5    28*pad			is reserved for future use and
  5    29*			must be zero.
  5    30*
  5    31*def_section_relp		is a pointer (relative to the base
  5    32*			of the object segment) to the base
  5    33*			of the definition section.
  5    34*
  5    35*first_reference_relp	is a pointer (relative to the base
  5    36*			of the linkage section) to the
  5    37*			array of first-reference traps.
  5    38*			If the value of this item is "0"b
  5    39*			then there are no first-reference
  5    40*			traps.
  5    41*
