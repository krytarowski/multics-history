      153 dcl	cobol_call_op	entry (fixed bin, fixed bin);
      154 dcl	cobol_pointer_register$get
      155 			entry (ptr);
      156 dcl	cobol_register$load entry (ptr);
      157 dcl	cobol_emit	entry (ptr, ptr, fixed bin);
      158 dcl	cobol_move_gen	entry (ptr);
      159 dcl	cobol_alloc$stack	entry (fixed bin, fixed bin, fixed bin);
