      180 /*************************************/
      181 get_bit_code:
      182      proc (fbin_code) returns (bit (4));
      183 
      184 dcl	fbin_code		fixed bin;
      185 dcl	bit_code		bit (4);
      186 
      187 	if fbin_code = 9
      188 	then bit_code = "0010"b;			/*  Q  */
      189 	else if fbin_code = 8
      190 	then bit_code = "0001"b;			/*  A  */
      191 	else bit_code = "1"b || substr (unspec (fbin_code), 34, 3);
      192 
      193 	return (bit_code);
      194 
      195      end get_bit_code;
