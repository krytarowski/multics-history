      155 start_after_operator:
      156 	/***..... dcl LOCAL_NAME2 char (15) int static init ("$AFTER_OPERATOR");/**/
      157 	/***.....if Trace_Bit then call cobol_gen_driver_$Tr_Beg(MY_NAME||LOCAL_NAME2);/**/
      158 	i = 1;
      159 	j = 4;
      160 	go to emit_;
