       43 dcl	reset_r_instr	(4) bit (36) static init ("110000000000011000011101000001010000"b,
       44 						/* epp0	pr6|30,*		*/
       45 			"110000000000100100011111000001010000"b,
       46 						/* epp4	pr6|44,*		*/
       47 			"110000000001101110011101011101010000"b,
       48 						/* epp3	pr6|156,*		*/
       49 			"110000000001110000011111001101010000"b);
       50 						/* epp5	pr6|160,*		*/
       51 
       52 dcl	cobol_emit	entry (ptr, ptr, fixed bin);
       53 dcl	(i, j)		fixed bin;
       54 dcl	k		fixed bin;
       55 dcl	op_flag		fixed bin static init (1);
       56 dcl	ptr_val		(1:4) fixed bin int static init (0, 4, 3, 5);
       57 
       58 dcl	ptr_no		bit (3);
