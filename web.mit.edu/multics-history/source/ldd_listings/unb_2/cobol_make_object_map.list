       24 /* Modified since Version 4.4 */
       25 
       26 /* format: style3 */
       27 cobol_make_object_map:
       28      proc;
       29 
       30 /* Produces an Object Map in the List segment when "map" option is specified. */
       31 
       32 dcl	p		ptr;
       33 dcl	q		ptr;
       34 dcl	(ff, nl)		char (1);
       35 dcl	c		char (4) based (addr (s));
       36 dcl	bit9		bit (9) based;
       37 dcl	(i, j, t)		fixed bin;
       38 dcl	char_off		fixed bin (21);
       39 dcl	number		fixed bin (35);
       40 dcl	s		fixed bin (35);
       41 dcl	mcode		fixed bin (35);
       42 dcl	char_string	char (1048576) based (cobol_$list_ptr);
       43 dcl	print_line	char (120) unaligned based;
       44 
       45 dcl	1 map_line	unaligned based,
       46 	  2 map_field	(7) unaligned,
       47 	    3 line_field	char (7),
       48 	    3 sp1		char (1),
       49 	    3 loc_field	char (6),
       50 	    3 sp2		char (3),
       51 	  2 new_line_char	char (1) unaligned;
       52 
       53 dcl	1 file_line	unaligned based (q),
       54 	  2 file_fld	char (2),
       55 	  2 sp1		char (1),
       56 	  2 line_fld	char (4);
       57 
       58 dcl	1 header_line	unaligned based,
       59 	  2 form_feed_char	char (1),
       60 	  2 new_line_char1	char (1),
       61 	  2 header_field	char (119),
       62 	  2 new_line_char2	char (1);
       63 
       64 dcl	1 map_data_table	aligned based (cobol_$map_data_ptr),
       65 	  2 no_source_stmts fixed bin aligned,
       66 	  2 data		(0 refer (map_data_table.no_source_stmts)),
       67 	    3 line_no	fixed bin unaligned,
       68 	    3 text_addr	fixed bin unaligned,	/*[4.4-1]*/
       69 	    3 col		fixed bin unal,		/*[4.4-1]*/
       70 	    3 label	bit unal;
       71 
       72 dcl	hcs_$set_bc_seg	entry (ptr, fixed bin (24), fixed bin (35));
       73 
       74 dcl	addr		builtin;
       75 dcl	divide		builtin;
       76 dcl	mod		builtin;
       77 dcl	substr		builtin;
       78 
       79 	addr (ff) -> bit9 = "000001100"b;		/* form feed char */
       80 	addr (nl) -> bit9 = "000001010"b;		/* new line char */
       81 	char_off = cobol_$list_off;
       82 
       83 	p = addr (substr (char_string, char_off, 1));
       84 	p -> header_line.form_feed_char = ff;
       85 	p -> header_line.new_line_char1 = nl;
       86 	p -> header_line.header_field = (7)"   LINE    LOC   ";
       87 	p -> header_line.new_line_char2 = nl;
       88 	char_off = char_off + 122;
       89 	p = addr (substr (char_string, char_off, 1));
       90 	p -> print_line = " ";
       91 	j = 1;
       92 
       93 	do i = 1 to no_source_stmts;
       94 	     eln_index = line_no (i);
       95 	     if eln_index > 0
       96 	     then do;
       97 		     number = eln_tab.fno (eln_index);
       98 
       99 		     if number > 0
      100 		     then do;			/* copy file */
      101 
      102 			     q = addr (p -> line_field (j));
      103 
      104 			     do t = 2 to 1 by -1 while (number > 0);
      105 
      106 				s = mod (number, 10) + 48;
      107 						/* "0" = 48(decimal) */
      108 				substr (file_fld, t, 1) = substr (c, 4, 1);
      109 				number = divide (number, 10, 35, 0);
      110 			     end;
      111 			     number = eln_tab.lno (eln_index);
      112 			     do t = 4 to 1 by -1 while (number > 0);
      113 				s = mod (number, 10) + 48;
      114 						/* "0" = 48(decimal) */
      115 				substr (line_fld, t, 1) = substr (c, 4, 1);
      116 				number = divide (number, 10, 35, 0);
      117 
      118 			     end;
      119 			end;
      120 		     else do;			/* normal source */
      121 
      122 			     number = eln_tab.lno (eln_index);
      123 
      124 			     do t = 6 to 1 by -1 while (number > 0);
      125 
      126 				s = mod (number, 10) + 48;
      127 						/* "0" = 48(decimal) */
      128 				substr (p -> line_field (j), t, 1) = substr (c, 4, 1);
      129 				number = divide (number, 10, 35, 0);
      130 
      131 			     end;
      132 			end;
      133 
      134 /* Fill location field. */
      135 
      136 		     number = text_addr (i) + cobol_$constant_offset;
      137 		     p -> loc_field (j) = "000000";
      138 
      139 		     do t = 6 to 1 by -1 while (number > 0);
      140 
      141 			s = mod (number, 8) + 48;	/* "0" = 48(decimal) */
      142 			substr (p -> loc_field (j), t, 1) = substr (c, 4, 1);
      143 			number = divide (number, 8, 35, 0);
      144 		     end;
      145 		     if j = 7
      146 		     then do;
      147 			     p -> map_line.new_line_char = nl;
      148 			     char_off = char_off + 120;
      149 			     j = 1;
      150 			     p = addr (substr (char_string, char_off, 1));
      151 			     p -> print_line = " ";
      152 
      153 			end;
      154 
      155 		     else j = j + 1;
      156 
      157 		end;
      158 
      159 	end;
      160 
      161 	if j > 1
      162 	then do;					/* print last partial line */
      163 
      164 		number = 17 * (j - 1) + 1;
      165 		substr (p -> char_string, number, 1) = nl;
      166 		char_off = char_off + number;
      167 
      168 	     end;
      169 
      170 	cobol_$list_off = char_off;
      171 
      172 	call hcs_$set_bc_seg (cobol_$list_ptr, 9 * (cobol_$list_off - 1), mcode);
      173 
