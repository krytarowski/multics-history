      871 		     then token_type = Quad_Token;
      872 
      873 		     else if character = QDelta
      874 			then token_type = Delta_Token;
      875 
      876 			else if character = QRightBracket
      877 			     then token_type = RB_Token;
      878 
      879 			else do;
      880 got_bad_token:
      881 			     token_type = Bad_Token;
      882 			     return;
      883 			end;
