  7    57*		35 = suspend
  7    58*		36 = terminate
  7    59*		37 = unstring
  7    60*		38 = write
  7    61*		39 = use
  7    62*		40 = compute
  7    63*		41 = disable
  7    64*		42 = display
  7    65*		43 = enable
  7    66*		45 = generate
  7    67*		46 = hold
  7    68*		48 = process
  7    69*		49 = sort
