       26      proc (in_token_ptr, binit_tag, fxs_tag, fxs_locno, decl_flag);
       27 
       28 declare	(
       29 	binit_tag		fixed bin,
       30 	fxs_tag		fixed bin,
       31 	fxs_locno		fixed bin,
       32 	decl_flag		fixed bin
       33 	)		parameter;
