       24 cobol_decl_gen:
       25      proc (fxs_tag);
       26 
       27 /*
       28*The procedure cobol_decl_gen is called by cobol_gen_driver_ only if the
       29*program being processed contains DECLARATIVES and requires neither
       30*data nor fixed segment initialization.  When called it;
       31*     1.  Reserves a tag, fxs_tag, to be associated by cobol_end_gen 
       32*         with the first instruction of the first statement of the
       33*         first non_Dedclarative Section of the program.
       34*     2.  Generates the instruction and necessary fix-up directive
       35*         to achieve unconditional transfer of control to the
       36*         instruction to be associated with fxs_tag.
       37*
       38*U__s_a_g_e:_
       39*
       40*     declare cobol_decl_gen entry (fixed bin);
       41*
       42*     call cobol_decl_gen (fxs_tag);
       43*						   */
       44 declare	fxs_tag		fixed bin;
       45 
       46 /*}*/
       47 /*
       48*fxs_tag is the number of the tag associated with the first in-
       49*        struction of the first executable statement of the pro-
       50*        gram (first statement of the first non-Declarative Sec-
       51*        tion).  This variable is initially set by cobol_pologue_gen
       52*        to zero if no initialization (either data or segment) is
       53*        required or to the next available tag number if initiali-
       54*        zation is required.  (Input/Output)
       55*						   */
       56 
       57 /*
       58*G__e_n_e_r_a_t_e_d_C__o_d_e:_
       59*
       60*The following code is generated by cobol_decl_gen only if fxs_tag is
       61*zero.  If fxs_tag is non_zero , no code is generated.
       62*
       63*     tra  fxs_relp,ic
       64*
       65*where:
       66*fxs_relp is the offset, relative to the instruction in which it
       67*         appears, of the first instruction of the first execut-
       68*         able statement of the program (first statement of the
       69*         first non_Declarative Section).
       70*						   */
       71 
       72 /*
       73*D__a_t_a:_
       74*
       75*     % include cobol_;
       76*
       77*	Items in cobol_$incl.pl1 used (u) and/or set (s) by
       78*	cobol_decl_gen:
       79*
       80*	     cobol_ptr (u)
       81*	     next_tag (u/s)
       82*	     text_wd_off (u)
       83*						   */
       84 
       85 dcl	tra_inst		bit (36) static init ("000000000000000000111001000000000100"b);
       86 
       87 /*
       88*where:
       89*tra_inst is an unconditional transfer instruction.
       90*
       91*						   */
       92 
       93 /*
       94*P__r_o_c_e_d_u_r_e_s_C__a_l_l_e_d:_
       95*						   */
       96 dcl	cobol_emit	entry (ptr, ptr, fixed bin),
       97 	cobol_make_tagref	entry (fixed bin, fixed bin, ptr);
       98 
