      347 /* Interal procedure to generate codes to call sort_$initiate.
      348*	The call is through the operator sort_initiate.	*/
      349 initiate:
      350      proc;
      351 
      352 
      353 dcl	inst_seq		(4) bit (18) init ("000000000000000000"b, "011101011100000100"b,
      354 						/* epp3 compare,ic	*/
      355 			"000000000000000000"b, "111010010000000111"b);
      356 						/* lxl2 sort_stack_off,dl	*/
      357 dcl	rel_seq		(4) bit (5) aligned init ("00000"b, "00000"b, "00000"b, "00000"b);
      358 
      359 
      360 	call cobol_call_op (16, 0);
      361 	retry_tag = cobol_$next_tag;
      362 	call cobol_define_tag_nc (retry_tag, cobol_$text_wd_off);
      363 	cobol_$next_tag = cobol_$next_tag + 1;
      364 	if cobol_$same_sort_merge_proc ^= "0"b
      365 	then temp = -sort_stack_off;
      366 	else temp = sort_stack_off;
      367 	inst_seq (3) = substr (unspec (temp), 19, 18);
      368 	call cobol_emit (addr (inst_seq), addr (rel_seq), 2);
      369 
      370 	compare_hold_ic = cobol_$text_wd_off - 2;
      371 	call cobol_call_op (14, retry_tag);
      372 	call cobol_reg_manager$after_op (14);
      373 
      374 	temp = cobol_$text_wd_off + 3 - compare_hold_ic;
      375 	addrel (cobol_$text_base_ptr, compare_hold_ic) -> hold_addr = substr (unspec (temp), 19, 18);
      376 
      377      end initiate;
