        9 gtss_abs_login_banner_: proc returns (char (*));
       10 
       11 	call user_info_$login_data (pid, pjid, acct, anon, stby, wt, time_login, lw);
       12 	call decode_clock_value_$date_time (time_login, month, dom, year, hour, minute, second, microsecond, dow, "", code);
       13 	call gtss_bcd_ascii_ (addr (gtss_ust.lcjid), 5, addr (ascii_lcjid));
       14 	call ioa_$rs ("HIS SERIES 6000 ON ^d/^d/^d AT ^d.^d  DEFERRED # ^a",
       15 		    ret_string, ret_len,
       16 		    month, dom, year - 1900,
       17 		    hour, minute * 1000 / 60, ascii_lcjid);
       18 	return (substr (ret_string, 1, ret_len));
       19 
       20 /* gtss_abs_login_banner_ local declares */
       21 dcl acct char (32);
       22 dcl anon fixed bin;
       23 dcl ascii_lcjid char (5);
       24 dcl clock_ entry returns (fixed bin (71));
       25 dcl code fixed bin (35);
       26 dcl decode_clock_value_$date_time entry (fixed bin (71), fixed bin, fixed bin, fixed bin, fixed bin,
       27 		fixed bin, fixed bin, fixed bin (71), fixed bin, char (3), fixed bin (35));
       28 dcl dom fixed bin;
       29 dcl dow fixed bin;
       30 dcl hour fixed bin;
       31 dcl ioa_$rs entry options (variable);
       32 dcl lw char (5);
       33 dcl minute fixed bin;
       34 dcl microsecond fixed bin (71);
       35 dcl month fixed bin;
       36 dcl pid char (22);
       37 dcl pjid char (9);
       38 dcl ret_len fixed bin (21);
       39 dcl ret_string char (256);
       40 dcl second fixed bin;
       41 dcl stby fixed bin;
       42 dcl time_login fixed bin (71);
       43 dcl wt fixed bin;
       44 dcl user_info_$login_data entry (char (*), char (*), char (*), fixed bin, fixed bin, fixed bin, fixed bin (71), char (*));
       45 dcl year fixed bin;
