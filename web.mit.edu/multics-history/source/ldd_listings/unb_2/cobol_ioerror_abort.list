       22 /* Modified on 02/10/81 by FCH, [4.4-1], tra instr not emitted in certain cases, [4.4-1], BUG462(TR8970) */
       23 
       24 /* format: style3 */
       25 cobol_ioerror_abort:
       26      proc (mesno, lineno, stoff, restartad, exlink);
       27 
       28 dcl	mesno		fixed bin;		/* number of cobol error message */
       29 dcl	lineno		fixed bin;		/* line on which error occurred */
       30 dcl	stoff		fixed bin;		/* word offset in stack of code */
       31 dcl	restartad		fixed bin;		/* restartad for restart transfer */
       32 dcl	exlink		fixed bin;		/* extra error code to check */
       33 
       34 dcl	1 pr4_struct	static,
       35 	  2 pr4		fixed bin init (4),
       36 	  2 pointer_no	bit (3),
       37 	  2 lock		fixed bin init (0),
       38 	  2 switch	fixed bin init (1),
       39 	  2 segno		fixed bin init (3002),
       40 	  2 offset	fixed bin init (0),
       41 	  2 reset		fixed bin;
       42 
       43 dcl	1 areg_struct	static,
       44 	  2 areg		fixed bin init (1),
       45 	  2 reg_no	bit (4),
       46 	  2 lock		fixed bin init (0),
       47 	  2 already_there	fixed bin,
       48 	  2 contains	fixed bin init (0),
       49 	  2 null_ptr	ptr init (null ()),
       50 	  2 lit		fixed bin;
       51 
       52 dcl	instr		(3) bit (36);
       53 dcl	reloc		(6) bit (5) aligned static
       54 			init ("10100"b, "00000"b, "00000"b, "00000"b, "00000"b, "00000"b);
       55 
       56 dcl	szn		bit (12) static init ("010011100001"b);
       57 dcl	cmpa		bit (12) static init ("001001101001"b);
       58 dcl	lda		bit (12) static init ("010011101001"b);
       59 dcl	tra_icmod		bit (18) static init ("111001000000000100"b);
       60 dcl	tnz_icmod		bit (18) static init ("110000001000000100"b);
       61 dcl	full_tze_icmod	bit (36) static init ("000000000000000100110000000000000100"b);
       62 dcl	indmod		bit (6) static init ("010000"b);
       63 
       64 dcl	utemp		fixed bin;
       65 dcl	hold_ic		fixed bin static init (0);
       66 dcl	hold_addr		bit (18) based;
       67 dcl	instr_ptr		ptr;
       68 
       69 dcl	cobol_process_error entry (fixed bin, fixed bin, fixed bin);
       70 dcl	cobol_emit	entry (ptr, ptr, fixed bin);
       71 dcl	cobol_pointer_register$get
       72 			entry (ptr);
       73 dcl	cobol_register$load entry (ptr);
       74 
       75 
       76 /*************************************/
       77 start:
       78 	instr_ptr = addr (instr);
       79 	instr (1) = "110"b || substr (unspec (stoff), 22, 15) || szn;
       80 	if restartad = 0
       81 	then do;
       82 		if exlink > 0
       83 		then instr (2) = full_tze_icmod;
       84 		else do;
       85 			utemp = hold_ic - cobol_$text_wd_off;
       86 			instr (2) = substr (unspec (utemp), 19, 18) || tnz_icmod;
       87 		     end;
       88 		if stoff > 0
       89 		then call cobol_emit (instr_ptr, null (), 2);
       90 		if exlink > 0
       91 		then do;
       92 			call cobol_pointer_register$get (addr (pr4_struct));
       93 			call cobol_register$load (addr (areg_struct));
       94 			instr (2) = substr (instr (1), 1, 18) || cmpa;
       95 			instr (1) = "100"b || substr (unspec (exlink), 22, 15) || lda || indmod;
       96 			utemp = hold_ic + 2 - cobol_$text_wd_off;
       97 			instr (3) = substr (unspec (utemp), 19, 18) || tnz_icmod;
       98 			call cobol_emit (instr_ptr, addr (reloc), 3);
       99 		     end;
      100 	     end;
      101 	else do;
      102 		if stoff > 0
      103 		then do;
      104 			instr (2) = full_tze_icmod;
      105 			hold_ic = cobol_$text_wd_off + 1;
      106 			call cobol_emit (instr_ptr, null (), 2);
      107 			if exlink > 0
      108 			then do;
      109 				call cobol_pointer_register$get (addr (pr4_struct));
      110 				call cobol_register$load (addr (areg_struct));
      111 				instr (2) = substr (instr (1), 1, 18) || cmpa;
      112 				instr (1) = "100"b || substr (unspec (exlink), 22, 15) || lda || indmod;
      113 				instr (3) = full_tze_icmod;
      114 				call cobol_emit (instr_ptr, addr (reloc), 3);
      115 			     end;
      116 		     end;
      117 		call cobol_process_error (mesno, lineno, stoff);
      118 		utemp = restartad - cobol_$text_wd_off;
      119 		instr (1) = substr (unspec (utemp), 19, 18) || tra_icmod;
      120 						/*[4.4-1]*/
      121 		if stoff ^= 0 | exlink ^= 0
      122 		then call cobol_emit (instr_ptr, null (), 1);
      123 		if stoff > 0
      124 		then do;
      125 			utemp = cobol_$text_wd_off - hold_ic;
      126 			addrel (cobol_$text_base_ptr, hold_ic) -> hold_addr = substr (unspec (utemp), 19, 18);
      127 			utemp = utemp - 3;
      128 			if exlink > 0
      129 			then addrel (cobol_$text_base_ptr, hold_ic + 3) -> hold_addr =
      130 				substr (unspec (utemp), 19, 18);
      131 		     end;
      132 	     end;
      133 	return;
      134 
