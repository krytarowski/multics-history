      436 	substr (tra_instr, 1, 18) = substr (unspec (utemp), 19, 18);
      437 	call cobol_emit (addr (tra_instr), null (), 1);
      438 	utemp = cobol_$text_wd_off - save_ic;
      439 	substr (text (save_ic), 1, 18) = substr (unspec (utemp), 19, 18);
