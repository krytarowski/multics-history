      485 pool_nullptr:
      486      proc;
      487 
      488 dcl	nullcon		bit (72) aligned static
      489 			init ("111111111111111111000000000000100011000000000000000001000000000000000000"b);
      490 dcl	maskcon		bit (72) aligned static
      491 			init ("000111111111111111000000000000111111111111111111111111000111111000111111"b);
      492 dcl	char8b		char (8) based;
      493 
      494 	call cobol_pool (addr (nullcon) -> char8b, 2, off);
      495 	nulloff = -off - cobol_$text_wd_off;
      496 	call cobol_pool (addr (maskcon) -> char8b, 2, off);
      497 	maskoff = -off - cobol_$text_wd_off;
      498 exit_pool_nullptr:
      499 	return;
      500      end pool_nullptr;
      501 
