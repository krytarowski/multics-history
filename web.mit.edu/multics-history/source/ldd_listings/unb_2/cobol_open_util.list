      409 check_noentry1:
      410      entry (ft_ptr, cobol_code, ioerror_ptr, mcode_off);
      411 
      412 dcl	cobol_code	fixed bin;
      413 dcl	mcode_off		fixed bin;
      414 
      415 	save_retry = ioerror.restartad;
      416 	save_ic = cobol_$text_wd_off + 1;
      417 	patch_ic = cobol_$text_wd_off + 3;
      418 	substr (noentry_instr (1), 4, 15) = substr (unspec (mcode_off), 22, 15);
      419 	call cobol_make_link$type_4 (off, "error_table_$noentry");
      420 	if fixed_common.options.profile
      421 	then do;
      422 		fixup_directive.location.offset = cobol_$text_wd_off;
      423 		call cobol_make_fixup (addr (fixup_directive));
      424 	     end;
      425 	substr (noentry_instr (3), 4, 15) = substr (unspec (off), 22, 15);
      426 	call cobol_emit (addr (noentry_instr), addr (noentry_reloc), 4);
      427 	call cobol_ioerror (ft_ptr, cobol_code, mcode_off, ioerror_ptr);
      428 	call cobol_emit (addr (tra_instr), null (), 1);
      429 	utemp = cobol_$text_wd_off - patch_ic;
      430 	substr (text (patch_ic), 1, 18) = substr (unspec (utemp), 19, 18);
      431 exit_check_noentry1:
      432 	return;
