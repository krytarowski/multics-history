       27 /*{*/
       28 /* format: style3 */
       29 cobol_process_error:
       30      proc (cobol_error_code, line_no, error_code_relp);
       31 
       32 /* This sub-generator produces code to generate an
       33*object time error message via the run-time support routine
       34*cobol_error_. */
       35 
       36 dcl	cobol_error_code	fixed bin;
       37 dcl	line_no		fixed bin;
       38 dcl	error_code_relp	fixed bin;
       39 
       40 /*
       41*     cobol_error_code  a fixed bin number indicating the "COBOL
       42*               error number" (see below).  If 0, then there is no
       43*               associated COBOL error number and no COBOL message
       44*               is printed.
       45*
       46*This program will set up a call to a run-time error
       47*routine cobol_error_.  Run-time support routines encountering
       48*errors will also call cobol_error_.  Thus all error messages will
       49*be funnelled through one run-time routine.
       50*
       51*The interface is as follows:
       52*
       53*  call cobol_error_ (cobol_code,multics_code,line_no1,
       54*                                   line_no2,progname,error_ptr);
       55*
       56*     cobol_code  corresponds to cobol_error_code described above.
       57*
       58*     multics_code  a fixed bin corresponding to the location
       59*               identified by sp|error_code_relp described above.
       60*
       61*     line_no1  a fixed bin containing the line number on
       62*               which the error occurred.  This is derived from
       63*               the internal line number given in line_no
       64*               described above.  If it is 0, no line number is
       65*               applicable to this error and no such indication
       66*               will be given in the error message.
       67*
       68*     line_no2  a fixed bin containing the additional line
       69*               number.  This is derived from the internal line
       70*               number line_no described above.  If it is 0, then
       71*               only a one-part line number is given.
       72*
       73*     progname  a varying char field containing the name of the
       74*               program which produced the error.  The
       75*               cobol_process_error sub-generator will always set it
       76*               to the name of the program for which code is being
       77*               generated.  Run-time support routines may identify
       78*               the program which called them or themselves,
       79*               depending on the error.
       80*
       81*     error_ptr  a pointer to the location at which the error was
       82*               discovered or to the location at which
       83*               cobol_control_ is called from.  The
       84*               cobol_process_error sub-generator will always set it
       85*               to the latter.
       86*
       87*
       88*The following message(s) will be output to the "error_output"
       89*stream:
       90*
       91*     ["progname": Multics message (from com_err_)]
       92*     ["progname": COBOL error message]
       93*     Error occurred at "segno|offset"
       94*          [in "progname" [on line ["line_no2"] "line_no1"]]
       95*
       96*The first line is printed only if multics_code is non-zero.  The
       97*second line is printed only if cobol_code is non-zero.  The third
       98*line is always printed.  The progname portion of it is not
       99*present if progname is null; the line_no2 portion of it is not
      100*present is line_no2 is zero;  the line_no1 portion of it is not
      101*present if line_no1 is zero.  If progname is null and the first
      102*and/or the second line is present, the the string "cobol_error"
      103*is used in place of it.
      104*}*/
      105 
      106 dcl	inst_seq		bit (36) static init ("000000000000000000111000000001000000"b);
      107 						/* tsx0	pr0|4095+cobol_code	*/
      108 dcl	add_use_code	bit (36) static init ("110000000001001001000101100001000000"b);
      109 						/* aos	pr6|73	*/
      110 						/* -5-18-76- */
      111 
      112 dcl	sw		fixed bin;
      113 dcl	temp		fixed bin;
      114 
      115 dcl	cobol_emit	entry (ptr, ptr, fixed bin);
      116 dcl	cobol_reg_manager$before_op
      117 			entry (fixed bin);
      118 dcl	cobol_reg_manager$after_op
      119 			entry (fixed bin);
      120 
      121 /*************************************/
      122 	sw = 1;
      123 	go to start;
      124 use:
      125      entry (cobol_error_code, line_no, error_code_relp);
      126 	sw = 2;
      127 start:
      128 	if cobol_error_code = 0
      129 	then cobol_error_code = 58;
      130 	temp = 4095 + cobol_error_code;
      131 	call cobol_reg_manager$before_op (temp);
      132 	if sw = 2
      133 	then call cobol_emit (addr (add_use_code), null (), 1);
      134 	substr (inst_seq, 4, 15) = substr (unspec (temp), 22, 15);
      135 	call cobol_emit (addr (inst_seq), null (), 1);
      136 	call cobol_reg_manager$after_op (temp);
      137 	return;
      138 
      139 
      140 /*************************************/
      141 
      142 /*****	Declaration for builtin function	*****/
      143 
      144 dcl	(substr, mod, binary, fixed, addr, addrel, rel, length, string, unspec, null, index)
      145 			builtin;
      146 
      147 /*****	End of declaration for builtin function	*****/
