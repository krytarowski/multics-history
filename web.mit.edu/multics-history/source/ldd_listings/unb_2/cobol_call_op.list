       22 /* Modified on 12/18/78 by FCH, [3.0-1], entry skip added */
       23 /* Modified since Version 3.0.	*/
       24 /* format: style3 */
       25 cobol_call_op:
       26      proc (op_num, tagno);
       27 
       28 dcl	(tagno, tag1)	fixed bin,		/* tag number for no error.	*/
       29 	op_num		fixed bin;		/* operator number for the call.	*/
       30 						/* [3.0-1] */
       31 declare	tag_1		fixed bin;
       32 
       33 
       34 dcl	temp		fixed bin;
       35 dcl	inst_seq		(4) bit (18) init ("000000000000000000"b, "111000000001000000"b,
       36 						/* tsx0	pr0|op_num	*/
       37 			"000000000000000000"b, "111001000000000100"b);
       38 						/* tra	0,ic	*/
       39 
       40 /***********************************/
       41 
       42 /* [3.0-1] */
       43 	tag_1 = 0;
       44 
       45 start:
       46 	if first_time ^= cobol_$compile_count
       47 	then do;
       48 		first_time = cobol_$compile_count;
       49 		op_bit = (200)"0"b;
       50 	     end;
       51 
       52 	temp = op_num;
       53 
       54 	if temp < 200
       55 	then substr (op_bit, temp + 1, 1) = "1"b;
       56 
       57 	call cobol_reg_manager$before_op (temp);
       58 
       59 	substr (inst_seq (1), 4, 15) = substr (unspec (op_num), 22, 15);
       60 
       61 
       62 	call cobol_emit (addr (inst_seq), null (), 1);
       63 
       64 	if tagno = 0
       65 	then do;
       66 		call cobol_reg_manager$after_op (temp);
       67 	     end;
       68 	else do;
       69 		call cobol_emit (addr (inst_seq (3)), null (), 1);
       70 		call cobol_make_tagref (tagno, cobol_$text_wd_off - 1, null ());
       71 
       72 /* [3.0-1] */
       73 		if tag_1 ^= 0			/* [3.0-1] */
       74 		then do;
       75 			call cobol_emit (addr (inst_seq (3)), null (), 1);
       76 						/* [3.0-1] */
       77 			call cobol_make_tagref (tag_1, cobol_$text_wd_off - 1, null ());
       78 						/* [3.0-1] */
       79 		     end;
       80 	     end;
       81 	return;
       82 
       83 get_op:
       84      entry (operator_struc_ptr);
       85 dcl	operator_struc_ptr	ptr;
       86 
       87 	operator_struc_ptr = addr (op_bit);
       88 	return;
       89 
       90 skip:
       91      entry (op_num, tagno, tag1);
       92 
       93 /* [3.0-1] */
       94 	tag_1 = tag1;
       95 
       96 	go to start;
