       14 gtss_drl_callss_: proc (mcpp, increment);
       15 
       16 /*	Internal call to another subsystem.
       17*
       18*   Authors: Robert J. Grimes	Created
       19*   -	  Albert N. Kepner	  1978
       20*   -	  Robert M. May
       21*   -	  David B. Ward
       22*   Changed: Dave Ward	05/17/79 Debug display of ss name.
       23*   Changed: Bob Alvarado	11/26/79 Added callss_pound_sign_comd entry.
       24*   Changed: Dave Ward         1981     sorted dcl, added %page
       25**/
       26 dcl  increment                fixed bin parm;
       27 dcl  mcpp                     ptr parm;
       28 	mcp = mcpp;
       29 
       30 /*
       31*
       32*   DRL CALLSS, INTERNAL CALL TO ANOTHER SUBSYSTEM (octal 30)
       33*
       34*
       35*   8_________1_6_____________________
       36*
       37*   DRL      CALLSS
       38*   ASCII    1,name
       39*
       40*   (See TSS SYSTEM PROGRAMMER'S
       41*   REFERENCE MANUAL DD17C, Rev. 0, July 1977
       42*   Page 3-9)
       43*
       44**/
       45 
       46 	increment = 1 ;				/* One argument */
       47 	scup = addr (mc.scu);
       48 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       49 	arg_list_ptr = addrel (gseg, fixed (scu.ilc)+1);	/* get addres of arg_list */
       50 
       51 /* Check for DRL RETURN following the DRL CALLSS. If found,
       52*   the combination is treated as a DRL T.GOTO */
       53 	if next_instruction = "000005002000"b3 then do;	/* DRL RETURN */
       54 	     if db_drl_callss then
       55 		call ioa_ ("gtss_drl_callss_: DRL T.GOTO ""^a""", ss_name);
       56 	     call gtss_interp_prim_$t_goto (ss_name);
       57 	end;
       58 
       59 /* The above call does not return. The t_goto entry point causes
       60*   the process stack to be unwound to the previous invocation of
       61*   the primitive interpreter. */
       62 	goto cont;
       63 
       64 callss_pound_sign_comd: entry (ap, increment);
       65 dcl  ap                       ptr parm;
       66 	mcp = null ();
       67 	arg_list_ptr = ap;
       68 cont:
       69 
       70 /* Call a new subsystem */
       71 /* But first save the bar mode stack pointer */
       72 
       73 	sb = baseptr (baseno (addr (bar_mode_stack_ptr))); /* Get ptr to stack_4 */
       74 	bar_mode_stack_ptr = stack_header.bar_mode_sp;
       75 
       76 	if db_drl_callss then
       77 	     call ioa_ ("gtss_drl_callss_: DRL CALLSS ""^a""", ss_name);
       78 	call gtss_interp_prim_$callss (ss_name, mcp);
       79 
       80 /* Restore bar mode stack pointer */
       81 	stack_header.bar_mode_sp = bar_mode_stack_ptr;
       82 	return;
