       33 /*************************************/
       34 /*{*/
       35 stack:
       36      entry (num_char, boundry, offset);			/* allocate space in the object time stack */
       37 
       38 dcl	num_char		fixed bin;
       39 dcl	boundry		fixed bin;
       40 dcl	offset		fixed bin;		/*
       41* num_char	is the number of characters to be allocated
       42*	(input)
       43* boundry	specifies the allocation of the letmost character
       44*	(input)
       45*	0 word boundry, return character offset
       46*	1 word boundry, return word offset
       47*	2 even word boundry, return wd offset
       48*	3 odd word boundry, return word offset
       49* offset	is the word or character offset of the leftmost
       50*	character relative to the base of the stack frame
       51*	(output
       52**/
       53 						/*}*/
       54 dcl	utemp		fixed bin;
       55 dcl	words		fixed bin;
       56 dcl	1 stack_err	static,
       57 	  2 name		char (32) init ("cobol_alloc$stack"),
       58 	  2 message_len	fixed bin (35) init (42),
       59 	  2 message	char (42) init ("The object time stack exceeds 16,384 words");
       60 dcl	signal_		entry (char (*), ptr, ptr);
       61 
       62 start_stack:
       63 	utemp = num_char + 3;
       64 	words = fixed (substr (unspec (utemp), 1, 34));
       65 	if boundry > 1
       66 	then do;
       67 		if substr (unspec (cobol_$stack_off), 36, 1) = "1"b
       68 		then do;
       69 			if boundry = 2
       70 			then cobol_$stack_off = cobol_$stack_off + 1;
       71 		     end;
       72 		else do;
       73 			if boundry = 3
       74 			then cobol_$stack_off = cobol_$stack_off + 1;
       75 		     end;
       76 	     end;
       77 	if (words + cobol_$stack_off) > 16384
       78 	then do;
       79 		cobol_$stack_off = cobol_$init_stack_off;
       80 		call signal_ ("command_abort_", null (), addr (stack_err));
       81 		return;
       82 	     end;
       83 	else if boundry ^= 0
       84 	then offset = cobol_$stack_off;
       85 	else offset = 4 * cobol_$stack_off;
       86 	cobol_$stack_off = cobol_$stack_off + words;
       87 exit_stack:
       88 	return;
       89 
       90 
       91 /*************************************/
       92 /*{*/
       93 cobol_data:
       94      entry (cd_num_char, cd_boundry, cd_offset);		/* allocate space in cobol data */
       95 
       96 dcl	cd_num_char	fixed bin (24);
       97 dcl	cd_boundry	fixed bin;
       98 dcl	cd_offset		fixed bin (24);
       99 
      100 /*
      101* cd_num_char	is the number of characters to be
      102*		allocated. (input)
      103* cd_boundry	specifies the allocation of the leftmost
      104*		character. (input)
      105*		0 word boundry, return character offset
      106*		1 word boundry, return word offset
      107*		2 even word boundry, return word offset
      108*		3 odd word boundry, return word offset
      109* cd_offset	is the word or character offset of the
      110*		leftmost character relative to the
      111*		base of the stack frame. (output)
      112**/
      113 /*}*/
      114 
      115 dcl	1 cobol_data_err	static,
      116 	  2 name		char (32) init ("cobol_alloc$cobol_data"),
      117 	  2 message_len	fixed bin (35) init (44),
      118 	  2 message	char (44) init ("the cobol data segment exceeds 262,144 words");
      119 
      120 
      121 start_cobol_data:
      122 	utemp = cd_num_char + 3;
      123 	words = fixed (substr (unspec (utemp), 1, 34));
      124 	if cd_boundry > 1
      125 	then do;
      126 		if substr (unspec (cobol_$cobol_data_wd_off), 36, 1) = "1"b
      127 		then do;
      128 			if boundry = 2
      129 			then cobol_$cobol_data_wd_off = cobol_$cobol_data_wd_off + 1;
      130 		     end;
      131 		else do;
      132 			if boundry = 3
      133 			then cobol_$cobol_data_wd_off = cobol_$cobol_data_wd_off + 1;
      134 		     end;
      135 	     end;
      136 	if (words + cobol_$cobol_data_wd_off) > 262144
      137 	then do;
      138 		call signal_ ("command_abort_", null (), addr (cobol_data_err));
      139 		cobol_$cobol_data_wd_off = 0;
      140 		return;
      141 	     end;
      142 	else if cd_boundry ^= 0
      143 	then cd_offset = cobol_$cobol_data_wd_off;
      144 	else cd_offset = 4 * cobol_$cobol_data_wd_off;
      145 	cobol_$cobol_data_wd_off = cobol_$cobol_data_wd_off + words;
      146 exit_cobol_data:
      147 	return;
