       15 gtss_com_err_: proc;
       16 
       17 /**       This procedure is called from gtss after the condition
       18*	command_error has been signalled by com_err_. It takes
       19*	the com_err_ string and sends it to *CFP. com_err_ is
       20*	then requested not to print it if gtss_ext_$flags.gtss_com_err_sw
       21*	is true. If the call was a result of problems with the
       22*	*CFP file the write to it is not attempted.
       23*
       24*	Author: Al Dupuis      11/02/79.                         **/
       25 
       26 	/** Check for *CFP problems **/
       27 	if gtss_ust.lcfst.start_term > 0
       28 	& gtss_ust.lcfst.start_term < 5
       29 	then return;
       30 	
       31           /** Locate the error condition information **/
       32 	if find_frame ("command_error") = null ()
       33 	then do;
       34 	     call ioa_ ("Couldn't get frame for command_error.");
       35 	     return;
       36 	end;
       37 
       38 	/** And write error message to *CFP after preceding it with cr lf **/
       39 	if errmess_lth > gtss_max_lth - 2
       40 	then errmess_lth = gtss_max_lth - 2;
       41 	error_message = cr_lf || error_message;
       42 	errmess_lth = errmess_lth + 2;
       43 	call gtss_write_starCFP_ (null (), errmess_ptr, fixed (errmess_lth, 21), code);
       44 	if gtss_ext_$flags.gtss_com_err_sw
       45 	then gtss_ext_$flags.gtss_com_err_sw,
       46 	     print_sw = "0"b;
       47 	
       48 
       49 find_frame: proc (condition_parm) returns (ptr);
       50 
       51 	     sp = find_condition_frame_ (null ());
       52 	     do while (sp ^= null ());
       53 		call find_condition_info_ (sp, addr (cond_info), code);
       54 		if cond_info.condition_name = condition_parm then
       55 		     return (cond_info.infoptr);
       56 		sp = find_condition_frame_ (sp);
       57 	     end;
       58 	     return (null ());
       59 
       60 dcl condition_parm char (32) varying parm;
       61 	end;					/* find_frame */
