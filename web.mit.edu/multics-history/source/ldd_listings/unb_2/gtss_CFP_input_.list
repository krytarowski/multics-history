      208 bug_complaint: proc;
      209 
      210 	     gtss_ust.lcfst.start_term = 2;
      211 	     call gtss_abort_subsystem_ (
      212 		gtss_find_cond_frame_ ("derail"),
      213 		"gtss_CFP_input",
      214 		0,
      215 		"Encountered a bad i/o to terminal or *CFP."
      216 		);
      217 	     return;
      218 	end					/* bug_complaint */;
