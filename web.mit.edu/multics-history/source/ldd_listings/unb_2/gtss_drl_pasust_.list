       74*     IDENTIFIER		ATTRIBUTES	**/
       75 dcl  (addr,
       76       addrel,
       77       fixed,
       78       null,
       79       size)                   builtin;
       80 dcl  arg_list_ptr             ptr init(null());
       81 dcl  gseg                     ptr init(null());
       82 dcl  n                        fixed bin(24);
       83 dcl  n4                       fixed bin(24);
       84 dcl  n_words                  char(n4)aligned based;
       85 dcl  offset                   fixed bin(24);
       86 
       87 dcl 1 arg_list		aligned based(arg_list_ptr)
       88 ,     2 L_buffer		bit(18)unal
       89 ,     2 N			fixed bin(17)unal
       90 ;
       91 
       92 dcl 1 arg_list_2		aligned based(arg_list_ptr)
       93 ,     2 L_buffer		bit(18)unal
       94 ,     2 N			unal
       95 ,       3 XXX                 unsigned fixed bin(9) unal  /* offset into UST */
       96 ,       3 YYY                 unsigned fixed bin(9) unal  /* number of words to move */
       97 ;
       98 
