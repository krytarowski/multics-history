        6 gtss_drl_pasust_: proc (mcpp, increment);
        7 
        8 /**	gtss pass UST or SY** information to caller.
        9*
       10*	Authors:	Al Kepner
       11*	Authors:	Robert J. Grimes	Created
       12*		Albert N. Kepner	  1978
       13*		Robert M. May
       14*		David B. Ward
       15*		Bob Alvarado	08/22/79 added cpu time to ust.
       16*		Paul Benjamin 	08/31/79 corrected upper bound of SPA.
       17*  Modified: Ron Barstad  83-06-29  To 4js3 version where Num words is an offset and number of words
       18* **/
       19 dcl  mcpp                     ptr parm;
       20 dcl  increment                fixed bin parm;
       21 	mcp = mcpp;
       22 
       23 /**
       24*
       25*   DRL PASUST, PASS UST TO SUBSYSTEM (octal 33)
       26*
       27*
       28*   8________16_____________________
       29*
       30*   DRL      PASUST
       31*   ZERO     L(buffer),N
       32*
       33*    (See TSS Reference Manual
       34*         DJ31-A, Sept. 1980, page 6-58)
       35*
       36**/
       37 
       38 	increment = 1 ;				/* One arguments */
       39 	scup = addr (mc.scu);
       40 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       41 	arg_list_ptr = addrel (gseg, fixed (scu.ilc, 18)+1); /* get addres of arg_list */
       42 	n = arg_list.N;				/* Number of words to move from UST. */
       43 	gtss_ust.lsprt = gtss_ust.lsprt + (total_cpu_time_ ()/1000);
       44 	if n = -1 then do;				/* => Move SY** to caller's buffer. */
       45 	     n = 640;				/* Size of SY** */
       46 	     n4 = n*4;				/* "n" words in characters. */
       47 	     if (arg_list.L_buffer < "000144"b3) |	/* 144oct = 100dec. */
       48 	     ((fixed (arg_list.L_buffer, 18)+n)> fixed (gtss_ust.lsize.limit, 18)) then goto failed;
       49 	     addrel (gseg, arg_list.L_buffer) -> n_words =
       50 		gtss_disk (gtss_ext_$SYstarstar_file_no).single_segment_ptr -> n_words;
       51 	     return;
       52 	end;
       53 
       54 	n = arg_list_2.YYY;
       55 	offset = arg_list_2.XXX;
       56 	if (n>size (gtss_ust_ext_$ust)-1) | (n<1) then n = size (gtss_ust_ext_$ust)-1;
       57 	if (arg_list.L_buffer < "000144"b3) |		/* 144oct = 100dec. */
       58 	((fixed (arg_list.L_buffer, 18)+n)> fixed (gtss_ust.lsize.limit, 18)) then do;
       59 failed:	     ;
       60 	     call gtss_abort_subsystem_ (
       61 		mcp
       62 		, "gtss_drl_pasust"
       63 		, 4
       64 		, gtss_pnterr.err4
       65 		, fixed (scu.ilc, 18)
       66 		);
       67 	     return;
       68 	end;
       69 	n4 = n*4;					/* "n" words in characters. */
       70 	addrel (gseg, arg_list.L_buffer) -> n_words = addrel (addr(gtss_ust), offset) -> n_words;
       71 	return;
       72 
