       14 gtss_drl_grow_: proc (mcpp, increment);
       15 
       16 /* *	gtss grow a file.
       17*
       18*   Authors: Robert J. Grimes	Created
       19*   -	  Albert N. Kepner	  1978
       20*   -	  Robert M. May
       21*   -	  David B. Ward
       22*   Change:  Dave Ward	02/24/79 $db debug switch.
       23*   Change:  Dave Ward	10/31/79 Record quota overflow handling.
       24*   * */
       25 dcl  mcpp                     ptr parm;
       26 dcl  increment                fixed bin parm;
       27 	mcp = mcpp;
       28 
       29 /* *
       30*
       31*   DRL GROW, GROW A PERMANENT OR TEMPORARY FILE (octal 50)
       32*
       33*
       34*   8_________1_6_____________________
       35*
       36*   DRL      GROW
       37*   ZERO     L(n),L(fileid)
       38*   ZERO     L(buff),L(stat)
       39*
       40*   (See TSS SYSTEM PROGRAMMER'S
       41*   REFERENCE MANUAL DD17C, Rev. 0, July 1976
       42*   Page 3-51)
       43*
       44**/
       45 
       46 	increment = 2 ;				/* Two arguments */
       47 	scup = addr (mc.scu);
       48 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       49 	arg_list_ptr = addrel (gseg, fixed (scu.ilc)+1);	/* get addres of arg_list */
       50 
       51 /* *	Obtain name of file to grow.	* */
       52 	if L_fileid >= gtss_ust.lsize.limit then do;
       53 	     call gtss_abort_subsystem_ (
       54 		mcp
       55 		, "gtss_drl_grow_"
       56 		, 4				/* Invalid drl argument */
       57 		, "FILE NAME OUTSIDE MEMORY AT (^o)"
       58 		, fixed (scu.ilc, 18)
       59 		);
       60 	     go to ret;
       61 	end;
       62 	afn_ptr = addrel (gseg, L_fileid);
       63 
       64 /* *	Obtain pointer to return status word.	* */
       65 	if L_stat >= gtss_ust.lsize.limit then do;
       66 	     call gtss_abort_subsystem_ (
       67 		mcp
       68 		, "gtss_drl_grow_"
       69 		, 4				/* Invalid drl argument */
       70 		, "STATUS WORD OUTSIDE MEMORY (^o)"
       71 		, fixed (scu.ilc, 18)
       72 		);
       73 	     go to ret;
       74 	end;
       75 	grow_status_ptr = addrel (gseg, L_stat);
       76 
       77 /* *	Obtain (fn) the AFT index to the file.	* */
       78 	call gtss_aft_$find (ascii_file_name, fn, code);
       79 	if code = 1 then do;			/* Unknown file (i.e., not in AFT). */
       80 	     grow_status = "4024"b3;			/* Failure of name scan (not in AFT). */
       81 	     go to ret;
       82 	end;
       83 
       84 /* *	Obtain amount to grow file.	* */
       85 	if L_n >= gtss_ust.lsize.limit then do;
       86 	     call gtss_abort_subsystem_ (
       87 		mcp
       88 		, "gtss_drl_grow_"
       89 		, 4				/* Invalid drl argument */
       90 		, "ATTEMPT TO GROW FILE OUTSIDE MEMORY (^o)"
       91 		, fixed (scu.ilc, 18)
       92 		);
       93 	     go to ret;
       94 	end;
       95 	n_ptr = addrel (gseg, L_n);
       96 
       97 /* *	Grow the file.	* */
       98 	call gtss_ios_change_size_ (
       99 	     fn
      100 	     , fixed (n.add, 24, 0)
      101 	     , in_links
      102 	     , status
      103 	     , code);
      104 
      105 /* *	adjust size field in file descriptor in attributes structure.   */
      106 	call gtss_adjust_size_ (gtss_disk.attributes_ptr (fn));
      107 
      108 	if status ^= 0 & status ^= 2 then do;
      109 	     if db_drl_grow then
      110 		call com_err_ (
      111 		code
      112 		, "gtss_drl_grow_"
      113 		, "gtss_ios_change_size_ (file ^a) status ^i"
      114 		, ascii_file_name
      115 		, status
      116 		);
      117 	     if code = error_table_$rqover then
      118 		grow_status = "4010"b3;		/* Exhausted links. */
      119 	     else
      120 	     grow_status = "4002"b3;			/* I/O error. */
      121 	     go to ret;
      122 	end;
      123 
      124 	grow_status = "4000"b3;			/* No errors. */
      125 ret:	;
      126 	if db_drl_grow then
      127 	     call ioa_ (
      128 	     "GROW: file ^a (aft ^i) by ^o (^i.) ^[links^;llinks^],"
      129 	     ||"^/now ^o (^i.) words. Status ^4o"
      130 	     , ascii_file_name
      131 	     , fn
      132 	     , fixed (n.add, 17)
      133 	     , fixed (n.add, 17)
      134 	     , in_links
      135 	     , file_size (fn)
      136 	     , file_size (fn)
      137 	     , fixed (grow_status, 12)
      138 	     );
      139 	return;
