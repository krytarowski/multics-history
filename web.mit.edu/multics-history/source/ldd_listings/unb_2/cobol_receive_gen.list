       26 /* format: style3 */
       27 cobol_receive_gen:
       28      proc (in_token_ptr, next_stmt_tag);
       29 
       30 /* Declaration for static data.	*/
       31 dcl	1 pr_struc	static,
       32 	  2 what_ptr	fixed bin init (2),
       33 	  2 pointer_no	bit (3),
       34 	  2 lock		fixed bin init (0),
       35 	  2 switch	fixed bin init (0);
       36 
       37 dcl	1 alpha_type9	static,
       38 	  2 header	(4) fixed bin init (112, 0, 0, 9),
       39 	  2 repl_ptr	(2) ptr init ((2) null ()),
       40 	  2 fill1		bit (108) init (""b),
       41 	  2 file_key_info,
       42 	    3 fb1		(3) fixed bin init (0, 0, 0),
       43 	    3 size	fixed bin init (0),
       44 	    3 fb2		(2) fixed bin init (0, 0),
       45 	    3 flags1	bit (36) init ("000000100100000000010000000100000000"b),
       46 	    3 flags2	bit (36) init (""b),
       47 	    3 seg		fixed bin init (0),
       48 	    3 off		fixed bin,
       49 	  2 fill2		(7) fixed bin init (0, 0, 0, 0, 0, 0, 0);
       50 
       51 dcl	inst_seq		(12) bit (18) unaligned static init ("110000000000000000"b, "010101010001000000"b,
       52 						/* spri2	pr6|offset	*/
       53 			"000000000000000000"b, "010011101000000111"b,
       54 						/* lda	0,dl		*/
       55 			"110000000000000000"b, "111101101001000000"b,
       56 						/* sta	pr6|offset	*/
       57 			"110000000000000000"b, "100101000001000000"b,
       58 						/* stz	pr6|offset	*/
       59 			"110000000000000000"b, "010011100001000000"b,
       60 						/* szn	pr6|offset	*/
       61 			"000000000000000000"b, "110000000000000100"b);
       62 						/* tze	0,ic		*/
       63 
       64 
       65 /*	Automatic data	*/
       66 dcl	next_stmt_tag	fixed bin,		/* PARAMETER for NO DATA statement	*/
       67 	stoff		fixed bin,
       68 	dn_ptr		ptr,
       69 	temp		fixed bin;
       70 
       71 /* External procedure	*/
       72 dcl	cobol_call_op	entry (fixed bin, fixed bin),
       73 	cobol_pointer_register$priority
       74 			entry (fixed bin, fixed bin, bit (3)),
       75 	cobol_emit	entry (ptr, ptr, fixed bin),
       76 	cobol_define_tag	entry (fixed bin),
       77 	cobol_make_tagref	entry (fixed bin, fixed bin, ptr),
       78 	cobol_set_pr	entry (ptr, ptr),
       79 	cobol_reg_manager$after_op
       80 			entry (fixed bin),
       81 	cobol_get_size	entry (ptr, fixed bin, fixed bin);
       82 
       83 start:						/* Generate epp2 instruction for communication token.	*/
       84 	cdtoken_ptr = in_token.token_ptr (2);
       85 	alpha_type9.seg = cdtoken.cd_seg;
       86 	eos_ptr = in_token.token_ptr (in_token.n);
       87 	alpha_type9.off = cdtoken.cd_off - 60;
       88 	call cobol_set_pr (addr (pr_struc), addr (alpha_type9));
       89 
       90 /* Allocate 12 words in stack frame for parameters	*/
       91 	stoff = 74;				/* Communication stack frame  from pr6|74	*/
       92 
       93 /* Store cd_token address.	*/
       94 	substr (inst_seq (1), 4, 15) = substr (unspec (stoff), 22, 15);
       95 	call cobol_emit (addr (inst_seq (1)), null, 1);
       96 
       97 /* Set up parameter for message type.	*/
       98 	temp = stoff + 4;
       99 	if end_stmt.b | end_stmt.c
      100 	then do;
      101 		if ^end_stmt.b
      102 		then substr (inst_seq (3), 17, 2) = "01"b;
      103 		else if end_stmt.c
      104 		then substr (inst_seq (3), 17, 2) = "11"b;
      105 		else substr (inst_seq (3), 17, 2) = "10"b;
      106 		substr (inst_seq (5), 4, 15) = substr (unspec (temp), 22, 15);
      107 		call cobol_emit (addr (inst_seq (3)), null, 2);
      108 	     end;
      109 	else do;
      110 		substr (inst_seq (7), 4, 15) = substr (unspec (temp), 22, 15);
      111 		call cobol_emit (addr (inst_seq (7)), null, 1);
      112 	     end;
      113 
      114 /* Generate epp2 instruction for receiving data item.	*/
      115 	dn_ptr = in_token.token_ptr (3);
      116 	call cobol_set_pr (addr (pr_struc), dn_ptr);
      117 
      118 /* Store into stack frame.	*/
      119 	temp = stoff + 2;
      120 	substr (inst_seq (1), 4, 15) = substr (unspec (temp), 22, 15);
      121 	call cobol_emit (addr (inst_seq (1)), null, 1);
      122 
      123 /* Set up the size of the data token.	*/
      124 	temp = stoff + 5;
      125 	call cobol_get_size (dn_ptr, temp, 0);
      126 
      127 /* Call cobol_operators_	*/
      128 	call cobol_call_op (70, 0);
      129 
      130 	call cobol_reg_manager$after_op (70);
      131 
      132 /* Handle NO DATA statement.	*/
      133 	if end_stmt.b
      134 	then do;
      135 		temp = stoff + 7;
      136 		substr (inst_seq (9), 4, 15) = substr (unspec (temp), 22, 15);
      137 		call cobol_emit (addr (inst_seq (9)), null, 2);
      138 		next_stmt_tag = cobol_$next_tag;
      139 		cobol_$next_tag = cobol_$next_tag + 1;
      140 		call cobol_make_tagref (next_stmt_tag, cobol_$text_wd_off - 1, null);
      141 	     end;
      142 
      143 exit:
      144 	return;
      145 
      146 /*****	Declaration for builtin function	*****/
      147 
      148 dcl	(substr, mod, binary, fixed, addr, addrel, rel, length, string, unspec, null, index)
      149 			builtin;
      150 
      151 /*****	End of declaration for builtin function	*****/
