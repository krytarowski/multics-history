       22 /* Modified on 7/7/76 by George Mercuri to correct use entry when error code ^= 0. */
       23 /* Modified on 6/2/76 by Bob Chang to implement the reg_reset entry. */
       24 /* Modified on 5/18/76 by Bob Chang to fix the aos instruction. */
       25 /* Modified on 5/16/76 by Bob Chang to take out set_pr5 statment.	*/
       26 /* Modified on 5/12/76 by George Mercuri to add pr0| to tsx0 instruction. */
       27 /* Modified on 5/12/76 by George Mercuri for additional feature on cobol_error_code. */
       28 /* Modified on 5/10/76 by George Mercuri for error handling. */
       29 /* Created  on 05/05/76 by Bob Chang to interface with cobol_reg_manager. */
       30 /*{*/
       31 /* format: style3 */
       32 cobol_gen_error:
       33      proc (cobol_error_code, tagno);
       34 
       35 /* This sub-generator produces code to generate an
       36*object time error message via the run-time support routine
       37*cobol_error_. */
       38 
       39 dcl	cobol_error_code	fixed bin;
       40 dcl	tagno		fixed bin;
       41 dcl	tt		fixed bin;
       42 
       43 /*
       44*     cobol_error_code  a fixed bin number indicating the "COBOL
       45*               error number" (see below).  If 0, then it is
       46*	     assumed that the appropriate cobol_operators_ has
       47*	     set the cobol_error_code value in X1.
       48*
       49*This program will set up a call to a run-time error
       50*routine cobol_error_.  Run-time support routines encountering
       51*errors will also call cobol_error_.  Thus all error messages will
       52*be funnelled through one run-time routine.
       53*
       54*The interface is as follows:
       55*
       56*  call cobol_error_ (cobol_code,multics_code,line_no1,
       57*                                   line_no2,progname,error_ptr);
       58*
       59*     cobol_code  corresponds to cobol_error_code described above.
       60*
       61*     multics_code  a fixed bin corresponding to the location
       62*               identified by sp|error_code_relp described above.
       63*
       64*     line_no1  a fixed bin containing the line number on
       65*               which the error occurred.  This is derived from
       66*               the internal line number given in line_no
       67*               described above.  If it is 0, no line number is
       68*               applicable to this error and no such indication
       69*               will be given in the error message.
       70*
       71*     line_no2  a fixed bin containing the additional line
       72*               number.  This is derived from the internal line
       73*               number line_no described above.  If it is 0, then
       74*               only a one-part line number is given.
       75*
       76*     progname  a varying char field containing the name of the
       77*               program which produced the error.  The
       78*               cobol_gen_error sub-generator will always set it
       79*               to the name of the program for which code is being
       80*               generated.  Run-time support routines may identify
       81*               the program which called them or themselves,
       82*               depending on the error.
       83*
       84*     error_ptr  a pointer to the location at which the error was
       85*               discovered or to the location at which
       86*               cobol_control_ is called from.  The
       87*               cobol_gen_error sub-generator will always set it
       88*               to the latter.
       89*
       90*
       91*The following message(s) will be output to the "error_output"
       92*stream:
       93*
       94*     ["progname": Multics message (from com_err_)]
       95*     ["progname": COBOL error message]
       96*     Error occurred at "segno|offset"
       97*          [in "progname" [on line ["line_no2"] "line_no1"]]
       98*
       99*The first line is printed only if multics_code is non-zero.  The
      100*second line is printed only if cobol_code is non-zero.  The third
      101*line is always printed.  The progname portion of it is not
      102*present if progname is null; the line_no2 portion of it is not
      103*present is line_no2 is zero;  the line_no1 portion of it is not
      104*present if line_no1 is zero.  If progname is null and the first
      105*and/or the second line is present, the the string "cobol_error"
      106*is used in place of it.
      107*}*/
      108 
      109 dcl	add_use_code	bit (36) static init ("110000000001001001000101100001000000"b);
      110 						/* aos	pr6|73	*/
      111 						/* -5-18-76 - */
      112 
      113 
      114 dcl	temp		fixed bin;
      115 dcl	inst_seq		(6) bit (18) static init ("000000000000000000"b, "111000000001000000"b,
      116 						/* tsx0	pr0|op_num	*/
      117 			"000000000000000000"b, "111001000000000100"b,
      118 						/* tra	0,ic	*/
      119 			"000000111111111111"b, "111000000001001001"b);
      120 						/* tsx0 pr0|4095,1	*/
      121 
      122 /*
      123*Procedures Called		*/
      124 
      125 dcl	cobol_emit	entry (ptr, ptr, fixed bin),
      126 	cobol_define_tag	entry (fixed bin),
      127 	cobol_reg_manager$before_op
      128 			entry (fixed bin),
      129 	cobol_reg_manager$after_op
      130 			entry (fixed bin),
      131 	cobol_make_tagref	entry (fixed bin, fixed bin, ptr);
      132 
      133 
      134 
      135 /***********************************/
      136 
      137 start:
      138 	if tagno = 0
      139 	then do;
      140 		tt = cobol_$next_tag;
      141 		cobol_$next_tag = cobol_$next_tag + 1;
      142 		call cobol_define_tag (tt);
      143 	     end;
      144 	else tt = tagno;
      145 
      146 	if cobol_error_code = 0
      147 	then /*5/12/76*/
      148 	     call cobol_emit (addr (inst_seq (5)), null (), 1);
      149 	else do;
      150 		temp = 4095 + cobol_error_code;
      151 		substr (inst_seq (1), 4, 15) = substr (unspec (temp), 22, 15);
      152 		call cobol_emit (addr (inst_seq (1)), null (), 1);
      153 	     end;
      154 	call cobol_emit (addr (inst_seq (3)), null (), 1);
      155 	call cobol_make_tagref (tt, cobol_$text_wd_off - 1, null ());
      156 	return;
      157 
      158 
      159 /***********************************/
      160 
      161 reg_reset:
      162      entry (cobol_error_code, tagno);
      163 
      164 	if tagno = 0
      165 	then do;
      166 		tt = cobol_$next_tag;
      167 		cobol_$next_tag = cobol_$next_tag + 1;
      168 		call cobol_define_tag (tt);
      169 	     end;
      170 	else tt = tagno;
      171 
      172 	call cobol_reg_manager$before_op (0);
      173 	if cobol_error_code = 0
      174 	then /*5/12/76*/
      175 	     call cobol_emit (addr (inst_seq (5)), null (), 1);
      176 	else do;
      177 		temp = 4095 + cobol_error_code;
      178 		substr (inst_seq (1), 4, 15) = substr (unspec (temp), 22, 15);
      179 		call cobol_emit (addr (inst_seq (1)), null (), 1);
      180 	     end;
      181 	call cobol_reg_manager$after_op (0);
      182 	call cobol_emit (addr (inst_seq (3)), null (), 1);
      183 	call cobol_make_tagref (tt, cobol_$text_wd_off - 1, null ());
      184 	return;
      185 
      186 
      187 
      188 /*************************************/
      189 
      190 
      191 use:
      192      entry (cobol_error_code);			/*5/12/76*/
      193 	call cobol_emit (addr (add_use_code), null (), 1);
      194 
      195 	if cobol_error_code = 0
      196 	then call cobol_emit (addr (inst_seq (5)), null (), 1);
      197 	else do;
      198 		temp = 4095 + cobol_error_code;
      199 		substr (inst_seq (1), 4, 15) = substr (unspec (temp), 22, 15);
      200 		call cobol_emit (addr (inst_seq (1)), null (), 1);
      201 	     end;
      202 
      203 	return;
      204 
      205 
