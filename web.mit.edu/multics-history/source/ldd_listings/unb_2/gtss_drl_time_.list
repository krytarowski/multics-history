       15 gtss_drl_time_: proc (mcp, increment);
       16 
       17 /*	Obtain processor time and time of day.
       18*
       19*	Authors:	Robert J. Grimes	Created
       20*		Albert N. Kepner	  1978
       21*		Robert M. May
       22*		David B. Ward
       23*  Changed: Ron Barstad  02/04/83  To fix size error in convert to 64ths msec
       24**/
       25 
       26 /*
       27*   DRL TIME, OBTAIN PROCESSOR TIME AND TIME&OF&DAY (octal 21)
       28*
       29*
       30*   8_________1_6______
       31*
       32*   DRL      TIME
       33*   ZERO     L(date)
       34*
       35*
       36*   From this derail, the processor time  used  by  the  current
       37*   user, the time of day, and (optionally) the date are returned, in
       38*   the following form:
       39*
       40*
       41*
       42*   C(A)    Processor time
       43*
       44*   C(Q)    Time of day
       45*
       46*
       47*
       48*   The unit of time is 1/64 of a millisecond.
       49*
       50*
       51*   At  location  date, the date is entered, in ASCII code, with
       52*   slashes inserted between the values in the following form:
       53*
       54*
       55*
       56*   DATE    M   M   /   D
       57*
       58*   +1      D   /   Y   Y
       59*
       60*
       61*   Where:
       62*
       63*
       64*   MM is the month
       65*   DD is the day
       66*   YY is the year
       67*
       68*
       69*   If the value of L(date) is zero, the date is not stored.
       70*   */
       71 /* 	D E C L A R A T I O N S					 */
       72 
       73 
       74 /* 	External Entries						 */
       75 
       76 dcl  clock_ ext entry returns (fixed bin (71));
       77 dcl  date_time_ ext entry (fixed bin (71), char (*));
       78 dcl  decode_clock_value_ ext entry
       79     (fixed bin (71), fixed bin, fixed bin, fixed bin, fixed bin (71), fixed bin, char (3) aligned);
       80 
       81 /* 	Work Variables						 */
       82 
       83 dcl  increment fixed bin;				/* no of param words to skip */
       84 dcl  time fixed bin (71);				/* time in microseconds */
       85 dcl  time64ths fixed bin (35) based;                        /* time in 64ths of a msec */
       86 dcl  date_ptr ptr init(null());
       87 dcl  processor_time fixed bin (35);
       88 dcl  date char (8) based (date_ptr);
       89 dcl  arg_ptr ptr init(null());
       90 dcl 1 arglist aligned based (arg_ptr),
       91     2 address fixed bin (17) unaligned,
       92     2 fill bit (18) unaligned;
       93 dcl  date_time char (8);				/* ascii date and time */
       94 
       95 dcl  gseg ptr ;
       96 dcl  i fixed bin (71);				/* temp */
       97 dcl (j, m, d, y, w) fixed bin;
       98 dcl  z char (3) aligned;
       99 dcl (addrel, fixed, unspec, addr, divide) builtin;
      100 
      101 /* 	P     R     O     C     E     D     U     R     E		 */
      102 
      103 
      104 
      105 
      106 	scup = addr (mc.scu);
      107 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
      108 
      109 	increment = 1;				/* one parameter */
      110 	time = clock_ ();				/* get current time */
      111 
      112 	call date_time_ (time, date_time);		/* get ascii date and time */
      113 
      114 
      115 /*  look for date location required */
      116 	arg_ptr = addrel (gseg, fixed (scu.ilc) + 1);
      117 	if arglist.address ^= 0 then
      118 	     do;
      119 						/* check  for buffer address out of bounds */
      120 	     if arglist.address > fixed (gtss_ust.lsize.limit, 18)
      121 	     | (arglist.address+1) > fixed (gtss_ust.lsize.limit, 18) then do;
      122 		call gtss_abort_subsystem_ (mcp,
      123 		     "gtss_drl_time_",
      124 		     4,
      125 		     gtss_pnterr.err4,
      126 		     fixed (scu.ilc, 18));
      127 		return;
      128 	     end;
      129 	     date_ptr = addrel (gseg, arglist.address);
      130 	     date = date_time;
      131 	end;
      132 						/* get total proc time used before ss started and time so far by ss */
      133 	processor_time = gtss_ust.lsprt+gtss_ust.lspts;
      134 	mc.regs.a = unspec (processor_time);
      135 
      136 	call decode_clock_value_ (time, m, d, y, i, w, z); /* just to get time since midnight local time,
      137*						   in microseconds, into i */
      138 
      139 	i = divide (i, 1000, 71, 0);			/* convert to msecs */
      140 	addr (mc.regs.q) -> time64ths = i * 64;           /* convert to 64ths of a msec */
      141 	return;
