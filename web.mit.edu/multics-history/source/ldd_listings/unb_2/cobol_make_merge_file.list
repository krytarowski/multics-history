       31 /* format: style3 */
       32 cobol_make_merge_file:
       33      proc (name_ptr, file_ptr, type12_ptr, type9_ptr);
       34 
       35 dcl	name_ptr		ptr,
       36 	dn_ptr		ptr,
       37 	ft_ptr_save	(20) ptr static,
       38 	first_call	fixed bin static init (0),
       39 	merge_count	fixed bin static,
       40 	ft_ptr		ptr;
       41 dcl	stop_bit		bit (1),
       42 	temp_id		char (5),
       43 	temp_no		fixed bin,
       44 	status_code	fixed bin (35),
       45 	(i, j)		fixed bin,
       46 	rec_id		char (5);
       47 dcl	1 temp_type9	static,
       48 	  2 header	(4) fixed bin init (112, 0, 0, 9),
       49 	  2 repl_ptr	(2) ptr init ((2) null ()),
       50 	  2 fill1		bit (108) init (""b),
       51 	  2 file_key_info,
       52 	    3 fb1		(3) fixed bin init (0, 0, 0),
       53 	    3 size	fixed bin init (0),
       54 	    3 fb2		(2) fixed bin init (0, 0),
       55 	    3 flags1	bit (36) init ("000000100100000000010000000100000000"b),
       56 	    3 flags2	bit (36) init (""b),
       57 	    3 seg		fixed bin init (0),
       58 	    3 off		fixed bin,
       59 	  2 fill2		(7) fixed bin init (0, 0, 0, 0, 0, 0, 0);
       60 dcl	temp_type12	char (60),
       61 	(merge_ft_ptr, merge_file_ptr)
       62 			ptr,
       63 	temp_file_ptr	(20) ptr static,
       64 	temp_type12_ptr	(20) ptr static,
       65 	(file_ptr, type12_ptr, type9_ptr)
       66 			ptr,
       67 	temp_based	char (1024) based,
       68 	temp_file		char (1024),
       69 	temp_type9_ptr	(20) ptr static;
       70 
       71 dcl	cobol_read_rand	entry (fixed bin, char (5), ptr),
       72 	cobol_read_ft	entry (fixed bin, ptr),
       73 	cobol_alloc$stack	entry (fixed bin, fixed bin, fixed bin),
       74 	cobol_io_$cobol_vdwf_sput
       75 			entry (ptr, fixed bin (35), ptr, fixed bin, char (5)),
       76 	cobol_make_fsb_link entry (ptr);
       77 
       78 start:	/***.....	if Trace_Bit then call cobol_gen_driver_$Tr_Beg(MY_NAME);/**/
       79 						/*[5.3-1]*/
       80 	merge_file_ptr = addrel (cobol_com_ptr, 8 + size (fixed_common));
       81 	if first_call = cobol_$compile_count
       82 	then do i = 1 to merge_count;
       83 		if ft_ptr_save (i) -> file_table.max_cra_size = temp_file_ptr (i) -> file_table.max_cra_size
       84 		then do;
       85 			file_ptr = temp_file_ptr (i);
       86 			type9_ptr = temp_type9_ptr (i);
       87 			type12_ptr = temp_type12_ptr (i);
       88 			go to mfex;
       89 		     end;
       90 	     end;
       91 	else do;
       92 		merge_count = 0;
       93 		first_call = cobol_$compile_count;
       94 	     end;
       95 
       96 	merge_count = merge_count + 1;
       97 	i = fd_token.file_no;
       98 	call cobol_read_ft (i, ft_ptr_save (merge_count));
       99 
      100 	temp_type12_ptr (merge_count) = addr (temp_type12);
      101 	temp_file_ptr (merge_count) = addr (temp_file);
      102 	temp_type9_ptr (merge_count) = addr (temp_type9);
      103 	rec_id = fixed_common.filedescr_offsets (1);
      104 	stop_bit = "0"b;
      105 
      106 	do while (^stop_bit);
      107 	     call cobol_read_rand (1, rec_id, ft_ptr);
      108 	     rec_id = file_table.next;
      109 	     if rec_id = "00000"
      110 	     then stop_bit = "1"b;
      111 	end;
      112 
      113 	substr (temp_type12, 1, 60) = substr (name_ptr -> temp_based, 1, 60);
      114 	temp_no = file_table.file_no + 1;
      115 	temp_type12_ptr (merge_count) -> fd_token.file_no = temp_no;
      116 	i = fd_token.file_no;
      117 	call cobol_read_ft (i, merge_ft_ptr);
      118 
      119 	substr (temp_file, 1, 1024) = substr (merge_ft_ptr -> temp_based, 1, 1024);
      120 						/*[5.3-1]*/
      121 	call cobol_io_$cobol_vdwf_sput (merge_file_ptr, status_code, addr (temp_file), 1024, temp_id);
      122 
      123 	file_table.next = temp_id;
      124 	if temp_no <= 20
      125 	then fixed_common.filedescr_offsets (temp_no) = temp_id;
      126 	call cobol_read_rand (1, temp_id, ft_ptr);
      127 
      128 /*	setup temporary file table.	*/
      129 	file_table.temp = "1"b;
      130 	file_table.ao_len = 0;
      131 	file_table.cat_nm = "cobol_temp_merge_file_";
      132 	file_table.id = "cobol_temp_merge_switch_";
      133 	file_table.device = 6;
      134 	file_table.catalogued = 0;
      135 	file_table.ifn = "cobol_merge_ifn_";
      136 	file_table.external = "1"b;
      137 	file_table.file_status = "0"b;
      138 	file_table.extra_status = "0"b;
      139 	file_table.cra_seg = 1000;
      140 	i = file_table.max_cra_size;
      141 	file_table.file_no = temp_no;
      142 	file_table.max_cra_size = ft_ptr_save (merge_count) -> file_table.max_cra_size;
      143 	file_table.rec_do = ft_ptr_save (merge_count) -> file_table.rec_do;
      144 	call cobol_alloc$stack (i, 2, j);
      145 
      146 	file_table.cra_offset = j * 4;
      147 	cobol_$init_stack_off = cobol_$stack_off;
      148 
      149 	call cobol_make_fsb_link (ft_ptr);
      150 
      151 	temp_file_ptr (merge_count) = ft_ptr;
      152 	temp_type12_ptr (merge_count) -> fd_token.type = 12;
      153 	temp_type9_ptr (merge_count) -> data_name.seg_num = file_table.cra_seg;
      154 	temp_type9_ptr (merge_count) -> data_name.offset = file_table.cra_offset;
      155 	temp_type9_ptr (merge_count) -> data_name.file_num = file_table.file_no;
      156 	temp_type9_ptr (merge_count) -> data_name.item_length = file_table.max_cra_size;
      157 
      158 /*[5.3-1]*/
      159 	call cobol_io_$cobol_vdwf_sput (merge_file_ptr, status_code, temp_type9_ptr (merge_count), 112, temp_id);
      160 	call cobol_read_rand (1, temp_id, temp_type9_ptr (merge_count));
      161 						/*[5.3-1]*/
      162 	call cobol_io_$cobol_vdwf_sput (merge_file_ptr, status_code, temp_type12_ptr (merge_count), 60, temp_id);
      163 	call cobol_read_rand (1, temp_id, temp_type12_ptr (merge_count));
      164 
      165 	file_ptr = temp_file_ptr (merge_count);
      166 	type12_ptr = temp_type12_ptr (merge_count);
      167 	type9_ptr = temp_type9_ptr (merge_count);
      168 
      169 mfex:	/***.....	if Trace_Bit then call cobol_gen_driver_$Tr_End(MY_NAME);/**/
      170 	return;
      171 
      172 	/***.....	dcl cobol_gen_driver_$Tr_Beg entry(char(*));/**/
      173 	/***.....	dcl cobol_gen_driver_$Tr_End entry(char(*));/**/
      174 
      175 	/***.....	dcl Trace_Bit bit(1) static external;/**/
      176 	/***.....	dcl Trace_Lev fixed bin static external;/**/
      177 	/***.....	dcl Trace_Line char(36) static external;/**/
      178 	/***.....	dcl ioa_ entry options(variable); /**/
      179 	/***..... dcl MY_NAME char (21) int static init ("COBOL_MAKE_MERGE_FILE");/**/
