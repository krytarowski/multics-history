       14 gtss_drl_setlno_: proc (mcpp, increment);
       15 
       16 /**	gtss set automatic (or autox) build line mode.
       17*
       18*	Author:	Bob Grimes		04/01/78
       19*	Changed:	Dave Ward			06/19/78
       20*	Authors:	Robert J. Grimes	Created
       21*		Albert N. Kepner	  1978
       22*		Robert M. May
       23*		David B. Ward
       24* **/
       25 dcl  mcpp			ptr parm;
       26 dcl  increment		fixed bin(24)parm;
       27 	mcp = mcpp;
       28 /*
       29*   DRL SETLNO, SET LINE&NUMBER/INCREMENT IN UST (octal 37)
       30*
       31*
       32*   8_________1_6_____________________
       33*
       34*   DRL      SETLNO
       35*
       36*
       37*   0
       38*
       39*   C(A)           Line number
       40*
       41*   C(Q)     X         Increment
       42*
       43*   This derail initiates the  automatic  line  numbering  mode.
       44*   The  specified  line number and increment value are stored in the
       45*   user status table for use by  Line  Service.   An  indicator  for
       46*   automatic  line-numbering  mode  is  set  in  .LFLG2 (in UST).  A
       47*   blank/no-blank indicator is set in .LFLG2 also, as  specified  by
       48*   bit  0  of the Q-register.  If the bit value (X) is 1, a blank is
       49*   not supplied following the line number.
       50**/
       51 
       52 	increment = 0;
       53 	if (Line_number<0)| (Increment<0) then return;	/* Don't enter auto[x] mode. */
       54 	gtss_ust.lflg2.b24 = X;			/* 1 => do NOT provide space after line number. */
       55 	gtss_ust.lflg2.b25 = "1"b;			/* 1 => auto mode set on. */
       56 	if Line_number>0 then gtss_ust.linno = Line_number; /* Reset only if >0. */
       57 	if Increment>0 then gtss_ust.lincr = Increment;	/* Reset only if >0. */
       58 	return;
       59 
       60 dcl 1 a_q			aligned based(addr(mc.regs.a))
       61 ,     3 Line_number		fixed bin(35)
       62 ,     3 X			bit(1)unal
       63 ,     3 Increment		fixed bin(34)unal
       64 ;
