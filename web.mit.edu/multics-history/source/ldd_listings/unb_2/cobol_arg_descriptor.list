      105 type9:
      106      entry (token_ptr, out_word, out_conoff);
      107 
      108 dcl	token_ptr		ptr;
      109 
      110 /* the following assumes a data type of character string
      111*				and not packed */
      112 	dn_ptr = token_ptr;
      113 
      114 	if data_name.elementary
      115 	then do;
      116 		desc_cnt = 0;
      117 		call set_descriptor;
      118 	     end;
      119 	else if fixed_common.descriptor = "01"b
      120 	then call SD;
      121 	else do;
      122 		desc_cnt = 1;
      123 
      124 		if data_name.level = 1 | data_name.level = 77
      125 		then desc_wd (1) = "10101010"b;
      126 		else desc_wd (1) = "10101011"b;
      127 
      128 		substr (desc_wd (1), 13, 24) = substr (unspec (data_name.item_length), 13, 24);
      129 	     end;
      130 
      131 	desc_wd (0) = substr (unspec (desc_cnt), 1, 36);
      132 
      133 	call cobol_pool (substr (desc_char, 1, (desc_cnt + 1) * 4), 2, out_conoff);
      134 
      135 	out_conoff = out_conoff - 1;
      136 	utemp = -out_conoff - cobol_$text_wd_off;
      137 	substr (out_word, 1, 18) = substr (unspec (utemp), 19, 18);
      138 
      139 	return;
