       29 /* format: style3 */
       30 cobol_arg_descriptor:
       31      proc (in_word, out_word, out_conoff);
       32 
       33 dcl	in_word		bit (36);
       34 dcl	out_word		bit (36);
       35 dcl	out_conoff	fixed bin;
       36 
       37 dcl	01 data_based	based,
       38 	  02 filler	char (24),
       39 	  02 offset	fixed bin;
       40 
       41 dcl	my_ptr		ptr;
       42 dcl	utemp		fixed bin;
       43 dcl	prec_len		fixed bin;
       44 dcl	my_count		fixed bin (24);
       45 dcl	my_word		bit (36);
       46 dcl	con		(0:1) bit (36) aligned based (my_ptr);
       47 dcl	dn_ptr		ptr;
       48 dcl	(desc_cnt, main_level, prev_level, i, redef_level)
       49 			fixed bin,
       50 	(cont_flag, found, con_bit)
       51 			bit (1),
       52 	desc_wd		(0:8000) bit (36),
       53 	desc_char		char (32000) based (addr (desc_wd (0)));
       54 
       55 
       56 /*************************************/
       57 
       58 	my_word = in_word;
       59 	my_ptr = addrel (cobol_$con_end_ptr, 0);
       60 	my_count = 1;
       61 	con_bit = "1"b;
       62 
       63 	do while (con_bit);
       64 
       65 	     if my_count >= cobol_$con_wd_off
       66 	     then do;
       67 
       68 /* the word is not in the pool, put it there*/
       69 
       70 		     if (cobol_$text_wd_off + cobol_$con_wd_off) <= 262144
       71 		     then do;
       72 			     con (0) = my_word;
       73 			     cobol_$con_wd_off = cobol_$con_wd_off + 1;
       74 
       75 			     call set_out;
       76 			end;			/* text plus constants exceed segment size */
       77 
       78 		     else call signal_ ("command_abort_", null (), addr (con_err));
       79 		     return;
       80 		end;
       81 
       82 	     if con (0) ^= my_word
       83 	     then do;
       84 		     my_count = my_count + 1;
       85 		     my_ptr = addrel (my_ptr, -1);
       86 		end;
       87 	     else con_bit = "0"b;
       88 
       89 	end;
       90 
       91 	call set_out;
       92 
       93 	return;
