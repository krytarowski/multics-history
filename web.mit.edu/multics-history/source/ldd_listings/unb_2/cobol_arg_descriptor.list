      142 /* set_descriptor */
      143 set_descriptor:
      144      proc;
      145 
      146 	if data_name.bin_18				/*[5.2-2]*/
      147 	then if data_name.sync			/*[5.2-2]*/
      148 	     then my_word = "100000100000000000000000000000010001"b;
      149 						/* comp-7 sync -- 1 */
      150 						/*[5.2-2]*/
      151 	     else my_word = "100000110000000000000000000000010001"b;
      152 						/* comp-7 -- 1 */
      153 
      154 	else if data_name.bin_36			/* comp-6 -- 1 */
      155 						/*[5.2-2]*/
      156 	then my_word = "100000100000000000000000000000100011"b;
      157 	else do;
      158 
      159 
      160 
      161 /*[5.2-2]*/
      162 /*my_word = "101010110000"b;*/
      163 		prec_len = data_name.item_length;
      164 
      165 		if data_name.display & data_name.numeric
      166 		then do;
      167 
      168 			if data_name.sign_type = "000"b
      169 						/* unsigned--35 */
      170 						/*[5.2-1]*/
      171 			then my_word = "11000111"b;
      172 			else if data_name.sign_separate = "0"b
      173 			then do;			/* leading overpunch--29 */
      174 						/*[3.0-2]*/
      175 				if data_name.sign_type = "010"b
      176 				then my_word = "10111011"b;
      177 						/* trailing overpunch--30 */
      178 						/*[3.0-2]*/
      179 				else if data_name.sign_type = "001"b
      180 				then my_word = "10111101"b;
      181 
      182 			     end;
      183 
      184 /* leading separate sign--9 */
      185 
      186 			else if data_name.sign_type = "100"b
      187 			then do;
      188 
      189 /*[3.0-2]*/
      190 				my_word = "10010011"b;
      191 				prec_len = prec_len - 1;
      192 			     end;			/* trailing separate sign--36 */
      193 			else if data_name.sign_type = "011"b
      194 			then do;			/*[3.0-2]*/
      195 				my_word = "11001001"b;
      196 				prec_len = prec_len - 1;
      197 
      198 			     end;
      199 		     end;
      200 
      201 		else if data_name.ascii_packed_dec	/* comp or comp-8 */
      202 		then do;
      203 
      204 			if data_name.item_signed
      205 			then do;
      206 
      207 
      208 /*[3.0-2]*/
      209 				if data_name.ascii_packed_dec_h
      210 						/*[3.0-2]*/
      211 				then if data_name.sync
      212 						/*[5.2-2]*/
      213 				     then my_word = "11010111"b;
      214 						/* comp-8 signed synch -- 43 */
      215 						/*[3.0-2]*/
      216 				     else my_word = "11010011"b;
      217 						/* comp-8 signed non-synch -- 41 */
      218 
      219 /* comp-5 signed--39 */
      220 
      221 /*[3.0-2]*/
      222 				else my_word = "11001111"b;
      223 
      224 			     end;			/*[5.2-2]*/
      225 			else if data_name.ascii_packed_dec_h
      226 						/* comp-8 */
      227 						/*[5.2-2]*/
      228 			then if data_name.sync	/*[5.2-2]*/
      229 			     then my_word = "11010001"b;
      230 						/* comp-8 unsigned sync -- 40 */
      231 						/*[5.2-2]*/
      232 			     else my_word = "11001101"b;
      233 						/* comp-8 unsigned -- 38 */
      234 						/*[5.2-2]*/
      235 			else my_word = "11010001"b;	/* comp-5 unsigned -- 40 */
      236 
      237 			prec_len = data_name.places_left + data_name.places_right;
      238 
      239 		     end;				/*[5.2-2]*/
      240 		else my_word = "10101011"b;		/* non-numeric -- 21 */
      241 
      242 
      243 /*[4.0-3]*/
      244 		substr (my_word, 13, 24) =
      245 		     /*[4.0-3]*/ substr (unspec (data_name.places_right), 25, 12) /*[4.0-3]*/
      246 		     || /*[4.0-3]*/ substr (unspec (prec_len), 25, 12);
      247 
      248 	     end;
      249 
      250 	desc_cnt = desc_cnt + 1;
      251 	desc_wd (desc_cnt) = my_word;
      252 
      253 /*[5.2-1]*/
      254 	if data_name.subscripted
      255 	then call sub_handler (addr (desc_wd (desc_cnt)));
      256 
      257      end;
      258 
      259 SD:
      260      proc;
      261 
      262 	main_level = data_name.level;
      263 	prev_level = main_level;
      264 	desc_cnt = 1;
      265 
      266 /* structure -- 17 */
      267 
      268 	if main_level = 1 | main_level = 77
      269 	then desc_wd (1) = "10100010000000000000"b || data_name.son_cnt;
      270 	else desc_wd (1) = "10100011000000000000"b || data_name.son_cnt;
      271 
      272 	dn_ptr = pointer (cobol_ntfp, dn_ptr -> data_based.offset);
      273 	cont_flag = "1"b;
      274 
      275 /*[5.2-1]*/
      276 	if data_name.subscripted
      277 	then call sub_handler (addr (desc_wd (1)));
      278 
      279 	do while (cont_flag);
      280 
      281 	     dn_ptr = addrel (dn_ptr, divide (data_name.size + 11, 8, 35, 0) * 2);
      282 
      283 	     if data_name.size = 0
      284 	     then return;
      285 
      286 	     do while (data_name.exp_redefining);
      287 
      288 		redef_level = data_name.level;
      289 		found = "0"b;			/*[3.0-1]*/
      290 		dn_ptr = addrel (dn_ptr, divide (data_name.size + 11, 8, 35, 0) * 2);
      291 
      292 		do while (^found);
      293 
      294 		     if data_name.level = 77 | data_name.level <= redef_level
      295 		     then found = "1"b;		/*[3.0-1]*/
      296 		     else dn_ptr = addrel (dn_ptr, divide (data_name.size + 11, 8, 35, 0) * 2);
      297 		end;
      298 
      299 		if data_name.level = 77 | data_name.level <= main_level
      300 		then return;
      301 
      302 	     end;
      303 
      304 	     if data_name.non_elementary
      305 	     then do;
      306 		     if data_name.level <= main_level
      307 		     then return;
      308 		     else do;
      309 
      310 			     prev_level = data_name.level;
      311 			     desc_cnt = desc_cnt + 1;
      312 
      313 /* structure -- 17 */
      314 
      315 			     desc_wd (desc_cnt) = "10100011000000000000"b || data_name.son_cnt;
      316 
      317 /*[5.2-1]*/
      318 			     if data_name.subscripted
      319 			     then call sub_handler (addr (desc_wd (desc_cnt)));
      320 
      321 			end;
      322 
      323 		end;
      324 	     else do;
      325 
      326 		     if data_name.level < 50
      327 		     then do;
      328 			     if data_name.level <= prev_level
      329 			     then if data_name.level <= main_level
      330 				then return;
      331 
      332 			     call set_descriptor;
      333 			end;
      334 
      335 		     else if data_name.level = 77
      336 		     then return;
      337 
      338 		end;
      339 
      340 	end;
      341 
      342      end;
      343 
      344 sub_handler:
      345      proc (desc_loc);
      346 
      347 /*[5.2-1]*/
      348 dcl	desc_loc		ptr;			/*[5.2-1]*/
      349 dcl	desc		bit (36) based (desc_loc);
      350 
      351 
      352 	occurs_ptr = addrel (dn_ptr, substr (unspec (data_name.occurs_ptr), 17, 18));
      353 	substr (desc_wd (desc_cnt), 10, 3) = substr (unspec (occurs.dimensions), 34, 3);
      354 
      355 	do i = occurs.dimensions to 1 by -1;
      356 
      357 	     desc_cnt = desc_cnt + 3;
      358 	     desc_wd (desc_cnt - 2) = "000000000000000000000000000000000001"b;
      359 	     desc_wd (desc_cnt - 1) = unspec (occurs.level.max (i));
      360 
      361 /*[5.2-1]*/
      362 	     if substr (desc, 8, 1) = "1"b
      363 	     then desc_wd (desc_cnt) = bit (fixed (divide (occurs.level.struc_length (i), 2, 35, 0) * 9, 36));
      364 	     else desc_wd (desc_cnt) = bit (fixed (divide (occurs.level.struc_length (i), 8, 35, 0), 36));
      365 	end;
      366 
      367      end;
