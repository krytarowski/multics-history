       22 /* Renamed on 01/17/77 by ORN from cobol_make_register_token to cobol_make_reg_token */
       23 /* Modified on 01/14/77 by ORN to signal command_error rather than cobol_compiler_error */
       24 /* Modified since Version 2.0 */
       25 
       26 /*{*/
       27 /* format: style3 */
       28 cobol_make_reg_token:
       29      proc (token_ptr, reg_no);
       30 
       31 /* create a token (type 100) for registers */
       32 dcl	token_ptr		ptr;
       33 dcl	reg_no		bit (4);			/*
       34* token_ptr  is a pointer to the token (input/output)
       35*	(a) if not null the caller will provide the space
       36*	to hold the token.
       37*	(b) if null this utility will provide the space
       38*	and will return the address of the token
       39*	in this parameter.
       40* reg_no	is the register number (input)
       41*		"0001"b - A register.
       42*		"0010"b - Q register.
       43*		"0011"b - A and Q register.
       44*		"1nnn"b - index register nnn.
       45* notes:
       46* 1. The token will have the following description.
       47*   (a) A standard token header, (type of 100, size set,
       48*       and the remainder zeros).
       49* 2. A field defined as bit (4).
       50*
       51**/
       52 						/*}*/
       53 
       54 dcl	1 token_overflow	static,
       55 	  2 name		char (32) init ("cobol_make_reg_token"),
       56 	  2 message_len	fixed bin (35) init (58),
       57 	  2 message	char (58) init ("The temporary token area has run over the end of a segment");
       58 dcl	signal_		entry (char (*), ptr, ptr);
       59 dcl	1 clear_token	aligned based (cobol_type100_ptr),
       60 	  2 clear_token_wd	(token_wd_size) fixed bin;
       61 dcl	token_wd_size	fixed bin init (5);		/* size of token in words */
       62 dcl	i		fixed bin;
       63 
       64 
       65 /*************************************/
       66 start:						/* get space for token (if not provided) */
       67 	if token_ptr = null ()
       68 	then do;
       69 		if (binary (rel (cobol_$temp_token_ptr)) + token_wd_size) > 262143
       70 		then do;				/* token will not fit in temp token area */
       71 			call signal_ ("command_error", null (), addr (token_overflow));
       72 			return;
       73 		     end;
       74 		token_ptr = cobol_$temp_token_ptr;
       75 		cobol_$temp_token_ptr = addrel (cobol_$temp_token_ptr, token_wd_size);
       76 	     end;
       77 
       78 /* setup token */
       79 
       80 	cobol_type100_ptr = token_ptr;		/* clear token to zeros */
       81 	do i = 1 to token_wd_size;
       82 	     clear_token_wd (i) = 0;
       83 	end;
       84 	cobol_type100.type = 100;
       85 	cobol_type100.size = 4 * (token_wd_size);
       86 	cobol_type100.register = reg_no;
       87 	return;
       88 
       89 /*****	Declaration for builtin function	*****/
       90 
       91 dcl	(substr, mod, binary, fixed, addr, addrel, rel, length, string, unspec, null, index)
       92 			builtin;
       93 
       94 /*****	End of declaration for builtin function	*****/
