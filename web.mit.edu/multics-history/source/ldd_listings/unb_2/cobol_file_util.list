       22 /* Modified on 8/11/76 by George Mercuri to add code for linage initialize. */
       23 /* Modified on 4/27/76 by G. Mercuri  change code for new cobol_operators_. */
       24 /* Modified on 4/21/76 by g. Mercuri to fix sxl5 in open_instr from 453 to 451. */
       25 /* format: style3 */
       26 cobol_file_util:
       27      proc (name_ptr, linage);
       28 
       29 open:
       30      entry (name_ptr, linage);
       31 
       32 dcl	name_ptr		ptr;			/* pts to type 12 token */
       33 dcl	linage		fixed bin;		/* 0= no linage, 1= linage*/
       34 
       35 dcl	open_instr	(4) bit (36) static init ("000000000000000000111010101000000111"b,
       36 						/* lxl5 <2*no_of_files>,du */
       37 			"000000000000000000111010110000000111"b,
       38 						/* lxl6 <2*fd_token.file_no>,du */
       39 			"000000000000011000111000000001000000"b,
       40 						/* tsx0 pr0|30 */
       41 			"000000000000011001111000000001000000"b);
       42 						/* tsx0 pr0|31 */
       43 
       44 
       45 dcl	temp		fixed bin;
       46 dcl	cobol_emit	entry (ptr, ptr, fixed bin);
       47 
       48 
       49 /*************************************/
       50 start:
       51 	temp = 2 * fixed_common.file_count;
       52 	substr (open_instr (1), 1, 18) = substr (unspec (temp), 19, 18);
       53 	temp = 2 * fd_token.file_no;
       54 	substr (open_instr (2), 1, 18) = substr (unspec (temp), 19, 18);
       55 
       56 	call cobol_emit (addr (open_instr), null (), 2);
       57 	if linage = 1
       58 	then call cobol_emit (addr (open_instr (3)), null (), 1);
       59 	else call cobol_emit (addr (open_instr (4)), null (), 1);
       60 	return;
       61 
       62 
       63 /*************************************/
       64 
       65 /*****	Declaration for builtin function	*****/
       66 
       67 dcl	(substr, mod, binary, fixed, addr, addrel, rel, length, string, unspec, null, index)
       68 			builtin;
       69 
       70 /*****	End of declaration for builtin function	*****/
