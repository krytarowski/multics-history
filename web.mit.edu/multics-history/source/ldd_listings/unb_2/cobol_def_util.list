      188 /* Relocation information for fixed part of Class-0 definition */
      189 /* for program's entry point   */
      190 
      191 dcl	reloc_cl0_def_fixed (6) bit (5) aligned static
      192 			init ("10101"b, "10101"b, "10000"b, "00000"b, "10101"b, "10101"b);
      193 
      194 /*	procedure called.	*/
      195 
      196 dcl	cobol_arg_descriptor$type9
      197 			entry (ptr, bit (36), fixed bin),
      198 	cobol_pool	entry (char (*), fixed bin, fixed bin),
      199 	cobol_reloc	entry (ptr, fixed bin, fixed bin),
      200 	signal_		entry (char (*), ptr, ptr);
      201 
      202 dcl	1 def_ovfl_error	aligned static,
      203 	  2 my_name	char (32) init ("cobol_def_util"),
      204 	  2 message_len	fixed bin init (35),
      205 	  2 message	char (35) init ("Definition Section length exceeded!");
      206 
