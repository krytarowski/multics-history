       28 /* format: style3 */
       29 cobol_def_util:
       30      proc (n_args, object_name, object_name_len, text_ptr, flag, descriptor_ptr, in_token_ptr);
       31 
       32 
       33 
       34 
       35 	parm_desc_ptr = addr (work_spaces);
       36 
       37 	if n_args > 127
       38 	then do;
       39 
       40 opd_of:
       41 		call signal_ ("command_abort_", null, addr (oprnd_ovfl));
       42 		goto opd_of;
       43 	     end;
       44 
       45 	class0_def_len = binary (substr (unspec (n_args), 18, 18)) + 4;
       46 
       47 	if substr (object_name, 1, 32) = cobol_$obj_seg_name
       48 	then do;
       49 		ep_acc_wd_len = 0;
       50 		ep_acc_relp = 13;
       51 	     end;
       52 	else do;
       53 		ep_acc_wd_len = binary (substr (unspec (object_name_len), 17, 18)) + 1;
       54 		ep_acc_relp = cobol_$def_wd_off + class0_def_len;
       55 	     end;
       56 
       57 
       58 /* Enter cobol_$def_wd_off in entry code sequence before updating */
       59 
       60 	def_ptr = addrel (cobol_$def_base_ptr, def_list_relp);
       61 	temp_ptr = addrel (cobol_$def_base_ptr, definition.forward_thread);
       62 
       63 	do while (temp_ptr -> a_word ^= 0);
       64 
       65 	     temp_thread = definition.forward_thread;
       66 	     def_ptr = temp_ptr;
       67 	     temp_ptr = addrel (cobol_$def_base_ptr, definition.forward_thread);
       68 	end;
       69 
       70 	if definition.forward_thread ^= "000000000000000010"b
       71 	then do while (definition.forward_thread ^= "000000000000000010"b);
       72 
       73 		temp_thread = definition.forward_thread;
       74 		def_ptr = addrel (cobol_$def_base_ptr, definition.forward_thread);
       75 	     end;
       76 
       77 	definition.forward_thread = substr (unspec (def_wd_off), 19, 18);
       78 	def_ptr = addrel (cobol_$def_base_ptr, cobol_$def_wd_off);
       79 	entry_seq.def_relp = substr (unspec (cobol_$def_wd_off), 19, 18);
       80 
       81 	cobol_$def_wd_off = cobol_$def_wd_off + class0_def_len + ep_acc_wd_len;
       82 
       83 def_error_loop:
       84 	if cobol_$def_wd_off > cobol_$def_max
       85 	then do;
       86 		call signal_ ("command_abort_", null, addr (def_ovfl_error));
       87 		goto def_error_loop;
       88 	     end;
       89 
       90 	definition.forward_thread = "000000000000000010"b;
       91 	definition.backward_thread = temp_thread;
       92 
       93 	temp = cobol_$text_wd_off + 2;
       94 
       95 	definition.value = substr (unspec (temp), 19, 18);
       96 	string (definition.flags) = "101100000000000"b;
       97 	string (entry_seq.flags) = "010000000000000000"b;
       98 	definition.class = "000"b;
       99 	definition.symbol_relp = substr (unspec (ep_acc_relp), 19, 18);
      100 	definition.segname_relp = "000000000000000011"b;
      101 
      102 	call cobol_reloc (addr (reloc_cl0_def_fixed), 6, 3003);
      103 
      104 	if n_args ^= 0 & fixed_common.descriptor ^= "00"b
      105 	then do;
      106 
      107 		parm_desc.n_args = n_args;
      108 		entry_seq.flags.has_descriptors = "1"b;
      109 		arg_reloc_code (0) = "00000"b;
      110 
      111 		do i = 1 to n_args;
      112 
      113 		     if flag = 0
      114 		     then substr (arg_desc_off, 1, 18) = substr (desc_off (i), 19, 18);
      115 						/*[5.1.1]*/
      116 		     else call cobol_arg_descriptor$type9 (in_token.token_ptr (i + 1), arg_desc_off, LOC);
      117 
      118 		     parm_desc.descriptor_relp (i) = substr (arg_desc_off, 1, 18);
      119 		     arg_reloc_code (i) = "10000"b;
      120 		end;
      121 
      122 		call cobol_reloc (addr (arg_reloc_code (0)), n_args + 1, 3003);
      123 
      124 		temp = 2 + 2 * n_args;
      125 		call cobol_pool (substr (work_spaces, 1, temp), 2, temp1);
      126 
      127 		temp1 = -temp1;
      128 		entry_seq.descr_relp_offset = substr (unspec (temp1), 19, 18);
      129 
      130 	     end;
      131 
      132 	if ep_acc_wd_len ^= 0
      133 	then do;
      134 		def_ptr = addrel (def_ptr, class0_def_len);
      135 		acc.length_of_string = object_name_len;
      136 		acc.string = substr (object_name, 1, object_name_len);
      137 
      138 		call cobol_reloc (null, ep_acc_wd_len + ep_acc_wd_len, 3003);
      139 
      140 	     end;
      141 
      142 	if flag = 1
      143 	then do;
      144 
      145 		entry_seq.eax7 = "000000000000000000110010111000000000"b;
      146 		entry_seq.epp2 = "111000000000101000011101010001010000"b;
      147 		entry_seq.tsp2 = "010000000000000110010111010001010000"b;
      148 
      149 	     end;
      150 
      151 	return;
      152 
