      154 dcl	n_args		fixed bin,		/* No of operands in USING phrase*/
      155 	flag		fixed bin,		/* initialization for eax7 etc. 
      156*		If flag = 1 : eax7 etx and descriptor offset are set here.
      157*		Else eax7 not set and descriptor offset passed from descriptor_ptr. */
      158 	descriptor_ptr	ptr,
      159 	desc_off		(n_args) bit (36) based (descriptor_ptr),
      160 	object_name	char (32),
      161 	object_name_len	fixed bin,
      162 	LOC		fixed bin,
      163 	arg_reloc_code	(0:256) bit (5) aligned,	/* Relocation code for class-0   */
      164 	class0_def_len	fixed bin,		/* Length of class-0 definition  */
      165 	ep_acc_wd_len	fixed bin,		/* Length in words of acc string */
      166 	ep_acc_relp	fixed bin,		/* Offset, relative to base of   */
      167 	arg_desc_off	bit (36),			/* Offset of argument descriptor */
      168 	i		fixed bin,		/* Do loop index	   */
      169 	offset		fixed bin,		/* Offset as returned by various */
      170 	n_con		fixed bin,		/* No of conditions	   */
      171 	n_off		fixed bin,		/* Offset of 1st wd of stack     */
      172 	name		(2) char (32),		/* Array of condition  names	   */
      173 	ln_nm		(2) fixed bin,		/* Array of condition name lngths*/
      174 	temp_ptr		ptr,			/* Temporary pointer used in	   */
      175 	temp_thread	bit (18),
      176 	work_spaces	char (256) static init (" "),
      177 	temp		fixed bin,
      178 	temp1		fixed bin,
      179 	a_word		fixed bin based;		/* A word used in creating	   */
      180 
      181 /* acc string definition   */
      182 
      183 dcl	1 acc		aligned based (def_ptr),
      184 	  2 length_of_string
      185 			fixed bin (8) unaligned,
      186 	  2 string	char (0 refer (acc.length_of_string)) unaligned;
