       14 gtss_drl_dio_: proc (mcpp, increment);
       15 
       16 /**	gtss user's disk i/o.
       17*
       18*   Authors: Robert J. Grimes	Created
       19*   -	  Albert N. Kepner	  1978
       20*   -	  Robert M. May
       21*   -	  David B. Ward
       22*   Change:  Dave Ward	3/30/79 Debug display dio action.
       23*   Change:  Bob Alvarado      08/22/79 added io  count to ust.
       24*   Change:  Paul Benjamin	09/26/79 Error message for attempt to write to read-only file
       25*   Change:  Dave Ward	10/30/79 Failure for quota overflow.
       26**/
       27 dcl  mcpp                     ptr parm;
       28 dcl  increment                fixed bin parm;
       29 	mcp = mcpp;
       30 
       31 /**
       32*
       33*   DRL DIO, DO I/O ON USER'S FILE (octal 1)
       34*
       35*
       36*   8_________1_6_____________________
       37*
       38*   DRL      DIO
       39*   Seek command
       40*   ZERO     L(fileid),L(dcw1)
       41*   Read/Write command
       42*   ZERO     L(fileid),L(dcw2)
       43*   ZERO     L(status),0
       44*
       45*   (See TSS SYSTEM PROGRAMMER'S
       46*   REFERENCE MANUAL DD17B, Rev. 0, July 1976
       47*   Page 3-35)
       48*
       49**/
       50 
       51 	increment = 5 ;				/* Five arguments */
       52 	scup = addr (mc.scu);
       53 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       54 	arg_list_ptr = addrel (gseg, fixed (scu.ilc, 18)+1); /* get address of arglist */
       55 
       56 /**	Obtain name of file on which to do i/o.	**/
       57 	if seek_locs.L_fileid >= gtss_ust.lsize.limit then do;
       58 	     call gtss_abort_subsystem_ (mcp,
       59 		"gtss_drl_dio_",
       60 		4,
       61 		"DIO: file name outside memory at (^6o)",
       62 		fixed (scu.ilc, 18));
       63 	     return;
       64 	end;
       65 	afn_ptr = addrel (gseg, seek_locs.L_fileid);
       66 
       67 /**	Obtain pointer to return status word.	**/
       68 	if L_status >= gtss_ust.lsize.limit then do;
       69 	     call gtss_abort_subsystem_ (mcp,
       70 		"gtss_drl_dio_",
       71 		4,
       72 		"DIO: Status word outside memory at (^6o)",
       73 		fixed (scu.ilc, 18));
       74 	     return;
       75 	end;
       76 	status_words_ptr = addrel (gseg, L_status);
       77 	unspec (status_words.WORD1) = "0"b;		/* => Major status = 0. */
       78 	Termination_indicator = "1"b;
       79 
       80 /**	Obtain (fn) the AFT index to the file.	**/
       81 	call gtss_aft_$find (ascii_file_name, fn, code);
       82 
       83 	if code = 1 then do;			/* Unknown file (i.e., not in AFT). */
       84 	     Major_status = "0001"b;			/* Device busy. */
       85 	     return;
       86 	end;
       87 	if db_drl_dio then
       88 	     call com_err_ (
       89 	     0
       90 	     , "gtss_drl_dio_"
       91 	     , "^a on file ""^a"" (aft ^i)"
       92 	     , iocmd (read_write_cmd.device_command)
       93 	     , translate (ascii_file_name, UPPER, lower)
       94 	     , fn
       95 	     );
       96 
       97 	call gtss_ios_io_ (
       98 	     fn
       99 	     , arg_list_ptr
      100 	     , arg_list_ptr
      101 	     , fixed (gtss_ust.lsize.limit, 24)
      102 	     , status
      103 	     , code);
      104 	if (status ^= 0) & (status ^= 1) then do;	/* Not success and not eof. */
      105 	     if db_drl_dio then
      106 		call com_err_ (
      107 		code
      108 		, "gtss_drl_dio_"
      109 		, "File ""^a"" (aft ^i) gtss_ios_io_ status=^i"
      110 		, translate (ascii_file_name, UPPER, lower)
      111 		, fn
      112 		, status
      113 		);
      114 	     if (status = 2) then
      115 		call gtss_abort_subsystem_ (
      116 		mcp
      117 		, "gtss_drl_dio_"
      118 		, 0
      119 		, "DIO(^6o) WRITE ATTEMPTED ON READ-ONLY FILE - ^a"
      120 		, fixed (scu.ilc, 18)
      121 		, translate (ascii_file_name, UPPER, lower)
      122 		);
      123 	     else
      124 	     if (status = 16) then
      125 		call gtss_abort_subsystem_ (
      126 		mcp
      127 		, "gtss_drl_dio_"
      128 		, 0
      129 		, "DIO(^6o) ^a EXCEEDED LLINKS AVAILABLE - ^a"
      130 		, fixed (scu.ilc, 18)
      131 		, iocmd (read_write_cmd.device_command)
      132 		, translate (ascii_file_name, UPPER, lower)
      133 		);
      134 	     else
      135 	     call gtss_abort_subsystem_ (
      136 		mcp
      137 		, "gtss_drl_dio_"
      138 		, 0
      139 		, "DIO (^6o) ^a FAILED FILE ""^a"" (AFT ^i)"
      140 		, fixed (scu.ilc, 18)
      141 		, iocmd (read_write_cmd.device_command)
      142 		, translate (ascii_file_name, UPPER, lower)
      143 		, fn
      144 		);
      145 	     return;
      146 	end;
      147 	no_of_disk_io = no_of_disk_io + 1;
      148 	return;
