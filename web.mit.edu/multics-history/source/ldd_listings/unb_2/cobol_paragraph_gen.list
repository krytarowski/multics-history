  4     5 /*  Input structure for   cobol_addr			   */
  4     6 
  4     7 declare 1 input_struc_basic aligned static,
  4     8 	2 type fixed bin aligned init(1),
  4     9 	2 operand_no fixed bin aligned init(0),
  4    10 	2 lock fixed bin aligned init(0),
  4    11 	2 segno fixed bin aligned,
  4    12 	2 char_offset fixed bin(24) aligned,
  4    13 	2 send_receive fixed bin aligned init(0);
  4    14 
  4    15 /*
  4    16*type	   indicates type of addressing requested.  Type 1
  4    17*	   indicates basic; i.e., data to be addressed is
  4    18*	   specified by segno and char_offset.
  4    19*
  4    20*operand_no   not applicable to type 1.
  4    21*
  4    22*lock	   indicates lock requirements for registers used in
  4    23*	   addressing; 0 - do not lock registers used.
  4    24*
  4    25*segno	   is the compiler designation of the segment in which
  4    26*	   the data to be addressed is located.
  4    27*
  4    28*char_offset  is the character offset within segno of the data to
  4    29*	   be addressed.
  4    30*
  4    31*send_receive indicates whether the data being addressed is a
  4    32*	   sending or receiving field for the instruction whose
  4    33*	   address field is being set; 0 indicates sending.
  4    34*						   */
  4    35 
  4    36 /*  Input structure for mc_register$load		   */
  4    37 
  4    38 declare 1 register_request aligned static,
  4    39 	2 requested_reg fixed bin aligned init(1),
  4    40 	2 assigned_reg bit(4) aligned,
  4    41 	2 lock fixed bin aligned init(0),
  4    42 	2 reg_set_now fixed bin aligned,
  4    43 	2 use_code fixed bin aligned init(0),
  4    44 	2 adjust_ptr_addr fixed bin aligned init(0),
  4    45 	2 content_ptr ptr aligned init(null),
  4    46 	2 literal_content bit(36) aligned init((36)"0"b);
  4    47 
  4    48 /*
  4    49*requested_reg   is a code designating the register requested;
  4    50*	      1 designates the a register.
  4    51*
  4    52*assigned_reg    is a code designating the register assigned.  It
  4    53*	      has no significance if a specific register is
  4    54*	      requested.
  4    55*
  4    56*lock	      indicates locking requirements; 1 requests that
  4    57*	      the register be locked.
  4    58*
  4    59*reg_set_now     not applicable for use_code = 0.
  4    60*
  4    61*use_code 	      specifies how the register is to be used by the
  4    62*	      requester; 0 signifies that such information is
  4    63*	      not meaningful for register optimization.
  4    64*
  4    65*adjust_ptr_addr inserted to make evident that since all pointers
  4    66*	      must be allocated on even word boundaries, the
  4    67*	      pl1 compiler will allocate structures containing
  4    68*	      pointers and all pointers therein on even word  
  4    69*	      boundaries leaving "gaps" where necessary.
  4    70*
  4    71*content_ptr     not applicable for use_code = 0.
  4    72*
  4    73*literal_content not applicable for use_code = 0.
  4    74*						   */
  4    75 
  4    76 /*  End-of-perform range alterable go instruction pair	   */
  4    77 
  4    78 declare prfrm_altgo_inst_pr(4) bit(18) unaligned static init
  4    79 	("000000000000000000"b, "010011101001000110"b,
  4    80 	 "000000000000000000"b, "111001000000000101"b);
  4    81 
  4    82 /*
  4    83*The instructions are:
  4    84*
  4    85*     lda  0
  4    86*     tra  0,al
  4    87*
  4    88*						   */
  4    89 
  4    90 /*  END INCLUDE FILE ... cobol_perform_altgo.incl.pl1		 */
  4    91 
