      118 start:						/*  Get procedure (tag) number of paragraph being entered. 	   */
      119 	proc_no = in_token.token_ptr (1) -> proc_def.proc_num;
      120 
      121 /*		Paragraph alterable?		   */
      122 
      123 	if cobol_$alter_list_ptr ^= null
      124 	then do;
      125 		if cobol_$alter_index <= alter_list.n
      126 		then if alter_list.goto.proc_num (cobol_$alter_index) = proc_no
      127 		     then cobol_$alter_flag = 1;
      128 
      129 		     else cobol_$alter_flag = 0;
      130 
      131 		else cobol_$alter_flag = 0;
      132 	     end;
      133 
      134 /*       	     Paragraph at end-of-perform range?              */
      135 
      136 	if cobol_$perform_list_ptr ^= null
      137 	then do;					/*  Paragraph being terminated.  */
      138 		if cobol_$para_eop_flag ^= 0
      139 		then /*  Insert alterable GO  */
      140 		     do;
      141 			call cobol_register$load (addr (register_request));
      142 			input_struc_basic.segno = perform_list.perf.target_a_segno (cobol_$perform_para_index);
      143 			input_struc_basic.char_offset =
      144 			     perform_list.perf.target_a_offset (cobol_$perform_para_index);
      145 			call cobol_addr (addr (input_struc_basic), addr (prfrm_altgo_inst_pr), null);
      146 			call cobol_emit (addr (prfrm_altgo_inst_pr), null, 2);
      147 			call cobol_define_tag (perform_list.perf.int_tag_no (cobol_$perform_para_index));
      148 			cobol_$para_eop_flag = 0;
      149 			cobol_$perform_para_index = cobol_$perform_para_index + 1;
      150 		     end;
      151 
      152 /*  Paragraph being entered.  */
      153 		if cobol_$perform_para_index <= perform_list.n
      154 		then if proc_no = perform_list.perf.proc_num (cobol_$perform_para_index)
      155 		     then cobol_$para_eop_flag = 1;
      156 
      157 		     else cobol_$para_eop_flag = 0;
      158 
      159 		else cobol_$para_eop_flag = 0;
      160 
      161 	     end;
      162 
      163 /*    Associate procedure number with next location in text.   */
      164 
      165 	call cobol_define_tag (proc_no);
      166 
      167 	return;
      168 
      169      end cobol_paragraph_gen;
