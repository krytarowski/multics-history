       47 dcl cond_arg char (32) varying parm;
       48 dcl find_condition_frame_ entry (ptr) returns (ptr);
       49 dcl find_condition_info_ entry (ptr, ptr, fixed bin (35));
       50 dcl sp ptr init (null());
