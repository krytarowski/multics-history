      115 	input_iocb_ptr = null;
      116 	ws_ptr = null;
      117 	dname = get_wdir_ ();
      118 	attach_desc = "";
      119 	fileno = 0;
      120 	nfiles = 0;
      121 	file_found (*) = "0"b;
      122 	tapename = "";
      123 	density = "1600";				/* default value */
