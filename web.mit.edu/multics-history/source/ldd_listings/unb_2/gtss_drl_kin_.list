       14 gtss_drl_kin_: proc (mcpp, increment);
       15 
       16 /* gtss terminal input.
       17*
       18*   DRL KIN, KEYBOARD INPUT LAST LINE (octal 4)
       19*
       20*   8_________1_6_____________________
       21*
       22*   DRL      KIN
       23*   ZERO     L(dat),L(count)
       24*   ZERO     L(status)
       25*
       26*   This derail retrieves the last line of input.  Normally this
       27*   sequence would follow immediately the  KOUTN  sequence;  however,
       28*   this  is not necessary.  KIN may be repeated to retrieve the same
       29*   line of input as many times  as  desired.   The  last  line  will
       30*   remain  in  the  buffer  until  some  output  or additional input
       31*   destroys it.  Dat is the location at which the  string  of  input
       32*   characters  is  to be stored.  Count is a word in which the count
       33*   of characters moved is in the lower part of  the  word.   A  zero
       34*   character  count  is  returned  if  there is no data in the input
       35*   buffer.  The parameter L(status) is not used by current  software
       36*   release.  The purpose of the status word is to receive the status
       37*   of  the  line when it is passed back to the subsystem for certain
       38*   conditions, such as  break,  disconnect,  etc.   In  the  present
       39*   implementation  the  handling  of these conditions is done by the
       40*   Executive, and the status word in the subsystem is  not  altered.
       41*
       42*   Authors: Robert J. Grimes	Created
       43*   -	  Albert N. Kepner	  1978
       44*   -	  Robert M. May
       45*   -	  David B. Ward
       46*   Changed: Dave Ward	08/19/81 Replace \ as final character with CR.
       47*                                       add %page, dcl percission, format changes
       48**/
       49 dcl  increment                fixed bin parm;
       50 dcl  mcpp                     ptr parm;
       51 	mcp = mcpp;
       52 	increment = 2 ;				/* two arguments */
       53 	scup = addr (mc.scu);
       54 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       55 	arg_ptr = addrel (gseg, fixed (scu.ilc)+1);	/* get addres of arglist */
       56 	waste_count, ntc = count_of_characters_transmitted; /* Number of characters available in terminal buffer. */
       57 	if L_count = 0 then count_ptr = addr (waste_count); /* Caller's has not supplied count word. */
       58 	else do;					/* Caller has provided count word. */
       59 	     if L_count > fixed (gtss_ust.lsize.limit, 18) then do;
       60 		call gtss_abort_subsystem_ (mcp,
       61 		     "gtss_drl_kin_",
       62 		     4,
       63 		     gtss_pnterr.err4,
       64 		     fixed (scu.ilc, 18));
       65 		return;
       66 	     end;
       67 	     count_ptr = addrel (gseg, L_count);
       68 	end;
       69 	if ntc<1 then do;				/* No character to provide caller. */
       70 	     if db_drl_kin then
       71 		call gtss_dump_kin_ ("");
       72 	     count_word = 0;
       73 	     return;
       74 	end;
       75 						/* check for addres violations */
       76 	if L_buffer > fixed (gtss_ust.lsize.limit, 18) then do;
       77 	     call gtss_abort_subsystem_ (mcp,
       78 		"gtss_drl_kin_",
       79 		4,
       80 		gtss_pnterr.err4,
       81 		fixed (scu.ilc, 18));
       82 	     return;
       83 	end;
       84 	buffer_ptr = addrel (gseg, L_buffer);
       85 	count_word = ntc;
       86 
       87 /*	Check that buffer fits within caller's memory. */
       88 	if (L_buffer + divide ((ntc+3), 4, 17, 0)-1) > fixed (gtss_ust.lsize.limit, 18) then do;
       89 	     call gtss_abort_subsystem_ (mcp,
       90 		"gtss_drl_kin_",
       91 		4,
       92 		gtss_pnterr.err4,
       93 		fixed (scu.ilc, 18));
       94 	     return;
       95 	end;
       96 						/* move the data */
       97 	buffer_ptr -> MS = addr (characters_transmitted) -> MS;
       98 	if ntc > 0 then				/* Examine final character. */
       99 	     if substr (buffer_ptr -> MS, ntc, 1) = "\" then
      100 		substr (buffer_ptr -> MS, ntc, 1) = CR; /* Replace \ with carriage return. */
      101 	if db_drl_kin then
      102 	     call gtss_dump_kin_ (buffer_ptr -> MS);
      103 	return;
