       34 /* This run initializes all global data and contains the main loop of lex. */
       35 
       36 /*   reserved_word    */
       37 
       38 dcl	1 reserved_word	based (cobol_current),
       39 	  2 fwd_link	pointer,
       40 	  2 back_link	pointer,
       41 	  2 rep_link	pointer,
       42 	  2 l_info	bit (8),
       43 	  2 size		fixed bin,
       44 	  2 line		fixed bin,
       45 	  2 column	fixed bin,
       46 	  2 type		fixed bin,		/* = 1 */
       47 	  2 key		fixed bin,
       48 	  2 class		bit (26),
       49 	  2 jump_index	fixed bin;
