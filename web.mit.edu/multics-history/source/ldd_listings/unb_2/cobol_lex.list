       67 dcl	cobol_gns$init_tok_string
       68 			entry;			/*[4.0-2]*/
       69 dcl	cobol_io_$initialize
       70 			entry;			/*[4.0-2]*/
       71 dcl	cobol_output_tokens$initialize
       72 			entry;			/*[4.0-2]*/
       73 dcl	cobol_res_words$initialize
       74 			entry;			/*[4.0-2]*/
       75 dcl	cobol_gns$initialize
       76 			entry;			/*[4.0-2]*/
       77 dcl	cobol_merge$initialize
       78 			entry;			/*[4.4-1]*/
