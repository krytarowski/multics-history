      208 drun:	entry;
      209 
      210 /** Check for one valid DRUN ID (nnnnD) **/
      211 
      212 	call cu_$arg_count (nargs);
      213 	if nargs ^= 1 then do;
      214 	     call com_err_ (0, "gtss$drun",
      215 		"Correct usage is gtss$drun drun_job_id");
      216 	     return;
      217 	end;
      218 
      219 	call cu_$arg_ptr (1, arg_ptr, argl, code);
      220 	if code ^= 0 then do;
      221 	     call com_err_ (code, "gtss$drun");
      222 	     return;
      223 	end;
      224 
      225 	if (argl ^= 5)
      226 	| ((d1 < "0") | (d1 > "9"))
      227 	| ((d2 < "0") | (d2 > "9"))
      228 	| ((d3 < "0") | (d3 > "9"))
      229 	| ((d4 < "0") | (d4 > "9"))
      230 	| ^((d5 = "d") | (d5 = "D"))
      231 	then do;
      232 	     call com_err_ (0, "gtss$drun",
      233 		"^a is not a valid drun job id", argument);
      234 	     return;
      235 	end;
      236 
      237 /** Tell DRUN it's time to execute **/
      238 	djid = argument;
      239 	call gtss_ascii_bcd_ (arg_ptr, 5, addr (drun_job_id));
      240 	execute_drun = 17;
      241 
      242 	goto start;
