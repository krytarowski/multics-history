  7    39 
  7    40         5 word_0,
  7    41 	6 defective bit (1) unal,			/* 1 = file has defective space */
  7    42 	6 io_errors bit (1) unal,			/* 1 = I/O errors encountered on file */
  7    43 	6 reserved bit (2) unal,			/* Reserved for GCOS */
  7    44 	6 sct bit (12) unal,			/* address of SCT for device on which
  7    45*						   file begins or if cataloged,
