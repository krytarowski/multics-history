      205 
      206 	def_ptr = addrel (def_ptr, 3);
      207 	definition.forward_thread = "000000000000000010"b;
      208 	definition.backward_thread = "000000000000000011"b;
      209 	definition.value = (18)"0"b;
      210 	string (definition.flags) = "100000000000000"b;
      211 	definition.class = "010"b;
      212 	definition.symbol_relp = "000000000000001001"b;
      213 	definition.segname_relp = "000000000000000011"b;
      214 
