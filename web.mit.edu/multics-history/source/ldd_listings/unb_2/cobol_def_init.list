      145 start:						/*  COMPUTE LENGTH OF ACC STRING FOR OBJECT SEGMENT NAME  */
      146 	len_segname_acc_str = search (obj_seg_name, " ");
      147 
      148 	if len_segname_acc_str = 0
      149 	then do;
      150 
      151 		len_segname_acc_str = 33;
      152 		wd_len = 9;
      153 
      154 	     end;
      155 
      156 	else do;
      157 
      158 		temp = len_segname_acc_str + 3;
      159 		wd_len = fixed (substr (unspec (temp), 1, 34));
      160 
      161 	     end;
      162 
      163 
      164 /*  UPDATE def_wd_off AND TEST AGAINST def_max  */
      165 
      166 	def_wd_off = wd_len + 13;
      167 
      168 	if def_wd_off > def_max
      169 	then do;
      170 
      171 		call signal_ ("command_abort_", null, addr (error_s));
      172 		return;
      173 
      174 	     end;
      175 
