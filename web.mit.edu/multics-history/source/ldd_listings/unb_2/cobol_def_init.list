      189 	zeros = (36)"0"b;
      190 
      191 
      192 /*  CREATE CLASS-3 DEFINITION FOR THE OBJECT SEGMENT  */
      193 
      194 	def_ptr = addrel (def_ptr, 1);
      195 	segname.forward_thread = "000000000000000110"b;
      196 	segname.backward_thread = "000000000000000010"b;
      197 	segname.segname_thread = "000000000000000010"b;
      198 	segname.flags = "100000000000000"b;
      199 	segname.class = "011"b;
      200 	segname.symbol_relp = "000000000000001101"b;
      201 	segname.first_relp = "000000000000000110"b;
      202 
      203 
