  2   172 pack:
  2   173 	procedure (aplsv_word) returns (bit (36) aligned);
  2   174 
  2   175 /* parameters */
  2   176 
  2   177 dcl	aplsv_word bit (36) aligned;
  2   178 
  2   179 /* automatic */
  2   180 
  2   181 dcl	word bit (36) aligned;
  2   182 
  2   183 /* program */
  2   184 
  2   185 	word = ""b;
  2   186 	substr (word, 5, 8) = substr (aplsv_word, 2, 8);
  2   187 	substr (word, 13, 8) = substr (aplsv_word, 11, 8);
  2   188 	substr (word, 21, 8) = substr (aplsv_word, 20, 8);
