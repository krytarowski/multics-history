        9 gtss_drl_msub_: proc (mcpp, increment);
       10 
       11 /**	gtss call a Multics subroutine.
       12*
       13*	Author:	Dave Ward			05/04/79
       14* **/
       15 dcl  mcpp                     ptr parm;
       16 dcl  increment                fixed bin parm;
       17 	mcp = mcpp;
       18 
       19 /**
       20*
       21*   DRL msub, CALL A MULTICS SUBROUTINE (octal -12)
       22*
       23*
       24*   8_________1_6_____________________
       25*
       26*   DRL      msub
       27*   ZERO	L(status),version
       28*   ZERO	L(entry),name-length
       29*   ZERO	L(input),input-length
       30*   ZERO	L(output),output-length
       31*
       32*   A one word status is returned (offset L_status):
       33*   0 => success.
       34*   1 => failure.
       35*
       36*   Version is a half word (2 ascii characters)
       37*   indicating the version of this derail.
       38*
       39*   L(entry) is an offset to an ascii string
       40*   (name-length characters) naming a Multics
       41*   entry to call.
       42*
       43*   L(input & output) are offsets to corresponding
       44*   input and output spaces in the GCOS caller's
       45*   memory (limited by input & output -length). Their
       46*   interpretation is particular to each subroutine
       47*   usage.
       48*
       49*	This derail is particular to gtss.
       50*
       51**/
       52 
       53 	increment = 4 ;				/* 4 arguments */
       54 	scup = addr (mc.scu);
       55 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       56 	msub_arg_list_ptr = addrel (gseg, fixed (scu.ilc, 18)+1); /* get address of msub_arg_list */
       57 	if (fixed (scu.ilc, 18)+3) >= fixed (gtss_ust.lsize.limit, 18) then
       58 	     call gtss_abort_subsystem_ (mcp,
       59 	     "gtss_drl_msub_",
       60 	     0,
       61 	     "File name outside memory (^o)",
       62 	     fixed (scu.ilc, 18)
       63 	     );
       64 
       65 /* Check name, input, output and
       66*   status within memory.
       67**/
       68 	np = pointer (msub_arg_list_ptr, L_entry);
       69 dcl  pointer                  builtin;
       70 	if db_drl_msub then do;			/* Print arguments. */
       71 	     call ioa_ ("PARM    OFFSET^-VALUE (parms at ^p)", msub_arg_list_ptr);
       72 	     call ioa_ ("entry  ^6o^-^i^-""^a""", L_entry, en, name);
       73 	     call ioa_ ("input  ^6o^-^i", L_input, in);
       74 	     call ioa_ ("output ^6o^-^i", L_output, in);
       75 	     call ioa_ ("status ^6o", L_status);
       76 	     call ioa_ ("version^-^-^i", version);
       77 	end;
       78 
       79 /* Obtain a pointer to the Multics entry
       80*   named in the caller's arguments.
       81**/
       82 dcl  hcs_$make_ptr            entry(ptr,char(*),char(*),ptr,fixed bin(35));
       83 
       84 /* Obtain segment and entry names. */
       85 	i = search (name, "$");
       86 	if i = 0 then				/* Both the same. */
       87 	     seg_name, entry_name = name;
       88 	else do;					/* Different. */
       89 	     seg_name = substr (name, 1, i-1);
       90 	     entry_name = substr (name, i+1);
       91 	end;
       92 
       93 	call hcs_$make_ptr (
       94 	     null ()				/* no calling dir. */
       95 	     , seg_name
       96 	     , entry_name
       97 	     , ep
       98 	     , code
       99 	     );
      100 	if code ^= 0 then do;
      101 	     call com_err_ (code, "gtss_drl_msub_",
      102 		"For ""^a$^a""", seg_name, entry_name);
      103 	     status = 1;
      104 	     return;
      105 	end;
      106 
      107 	call cu_$ptr_call (ep, msub_arg_list_ptr);
      108 	status = 0;
      109 	return;
