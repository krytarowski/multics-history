       14 gtss_drl_relmem_: proc (mcpp, increment);
       15 
       16 /**	gtss release memory.
       17*
       18*   Authors: Robert J. Grimes	Created
       19*   -	  Albert N. Kepner	  1978
       20*   -	  Robert M. May
       21*   -	  David B. Ward
       22*   **/
       23 
       24 dcl  mcpp                     ptr parm;
       25 dcl  increment                fixed bin parm;
       26 	mcp = mcpp;
       27 
       28 /**
       29*
       30*   DRL RELMEM, RELEASE MEMORY (octal 15)
       31*
       32*
       33*   8_________1_6_____________________
       34*
       35*   DRL      RELMEM
       36*   C(A)     return location,0
       37*   C(Q)     number  words low,number words high
       38*
       39*   (See TSS SYSTEM PROGRAMMER'S
       40*   REFERENCE MANUAL DD17C, Rev. 0, July 1976
       41*   Page 3-21)
       42*
       43**/
       44 
       45 	increment = 0 ;				/* Zero arguments */
       46 	scup = addr (mc.scu);
       47 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       48 	lw = divide (QU, 1024, 24)*1024;
       49 	hw = divide (QL, 1024, 24)*1024;
       50 
       51 	nl = fixed (gtss_ust.lsize.limit, 18, 0) - lw - hw; /* Calculate new length of memory. */
       52 
       53 	if nl <1024 then do;
       54 
       55 /* Exceeds installation limit. */
       56 	     call gtss_abort_subsystem_ (
       57 		mcp
       58 		, "gtss_drl_relmem_"
       59 		, 0
       60 		, gtss_pnterr.err101
       61 		, fixed (scu.ilc, 18)
       62 		);
       63 	     return;
       64 	end;
       65 
       66 	if (AU>nl) | (AL ^= 0) then do;
       67 	     call com_err_ (0, "gtss_drl_relmem_",
       68 		"Improper return address: AU=^6o AL=^6o lisize.limit=^6o",
       69 		AU, AL, nl);
       70 
       71 /* Improper return address */
       72 	     call gtss_abort_subsystem_ (
       73 		mcp
       74 		, "gtss_drl_relmem_"
       75 		, 4
       76 		, gtss_pnterr.err4
       77 		, fixed (scu.ilc, 18)
       78 		);
       79 	     return;
       80 	end;
       81 
       82 /**	Reduce memory.	**/
       83 	if lw>0 then do;				/* Move memory if lower words specified. */
       84 	     nc = nl*4;				/* New memory length in characters. */
       85 	     gseg -> MS = addrel (gseg, lw) -> MS;
       86 	end;
       87 
       88 	if gse_ext_$modes.gdb then
       89 	     if gtss_ext_$gdb_name ^= " " then do;
       90 		call gcos_debug_$loadtime (
       91 		     "-gtss"			/* memory segment name */
       92 		     , gtss_ext_$gdb_name
       93 		     , ""
       94 		     , 0
       95 		     );
       96 		gtss_ext_$gdb_name = " ";
       97 	     end;
       98 
       99 	gtss_ust.lswap.size, gtss_ust.lsize.limit = nl_r18; /* Save new limit in ust */
      100 
      101 	gtss_ust.lsize.bar = divide (nl+511, 512, 17, 0);
      102 	call gtss_set_slave_$load_bar (fixed (gtss_ust.lsize.bar, 18, 0)); /* Reset BAR register. */
      103 
      104 	gtss_spa.losti.ilc = bit (AU);		/* set up the return address in the slave area */
      105 	gtss_spa.losti.ir = "0"b;			/* zero the indicators */
      106 
      107 	increment = AU - fixed (scu.ilc, 18, 0) -1;	/* Restart execution of user's code return location. */
      108 
      109 	return;
