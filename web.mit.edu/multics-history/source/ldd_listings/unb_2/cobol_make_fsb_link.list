       28 /* format: style3 */
       29 cobol_make_fsb_link_:
       30 cobol_make_fsb_link:
       31      proc (ft_ptr);
       32 
       33 
       34 declare	linkoff		fixed bin,
       35 	ft_ptr		ptr;
       36 
       37 
       38 	if first_call ^= cobol_$compile_count
       39 	then do;
       40 		alloc_space = 0;
       41 		first_call = cobol_$compile_count;
       42 	     end;
       43 
       44 
       45 /*[4.0-1]*/
       46 	if file_table.alternate_keys > 0 & file_table.organization = 3
       47 	then fsb_init.n_words = size (fsb);
       48 	else fsb_init.n_words = 155;
       49 
       50 	call cobol_make_link$type_5 (linkoff, file_table.id, addr (fsb_init), alloc_space);
       51 
       52 	file_table.fsb.seg = -linkoff;
       53 	file_table.fsb.off = 0;
       54 
       55 	return;
       56 
