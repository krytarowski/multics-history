        9 gtss_drl_pseudo_: proc (mcpp, increment);
       10 
       11 /* *	gtss pseudo terminal input
       12*
       13*   Author:	Bob Grimes		06/09/78
       14*   Changed:	Dave Ward			06/23/78
       15*   Corrected usage of remote io buffer.
       16*   Changed:	Paul Benjamin		10/25/79
       17*   Turn off lbuf.tally
       18*   * */
       19 dcl  mcpp ptr parm;
       20 dcl  increment fixed bin parm;
       21 	mcp = mcpp;
       22 
       23 /* *
       24*
       25*   DRL PSEUDO, SIMULATED KEYBOARD INPUT (octal 64)
       26*
       27*
       28*   8_________1_6_____________________
       29*
       30*   DRL      PSEUDO
       31*   ZERO     L(tally),L(stat)
       32*
       33*   (See TSS SYSTEM PROGRAMMER'S
       34*   REFERENCE MANUAL DD17B, Rev. 0, July 1976
       35*   Page 3-20)
       36*
       37**/
       38 
       39 
       40 /*	External Entries 		*/
       41 
       42 
       43 /*	Work Variables		*/
       44 
       45 dcl  gesg ptr init(null());
       46 dcl  move_string char (number_chars) based;
       47 dcl  initial_tally_char (0:number_chars+3)char (1)based (addrel (gseg, tally.address));
       48 dcl  number_chars fixed bin (17);
       49 dcl  number_words fixed bin (17);
       50 
       51 dcl  arg_ptr ptr init(null());
       52 dcl 1 arglist aligned based (arg_ptr),
       53     2 tally fixed bin (17) unaligned,
       54     2 status fixed bin (17) unaligned;
       55 
       56 dcl  tally_ptr ptr init(null());
       57 dcl 1 tally aligned based (tally_ptr),
       58     2 address fixed bin (17) unaligned,
       59     2 chars fixed bin (11) unaligned,
       60     2 type bit (3) unaligned,
       61     2 offset fixed bin (2) unaligned;
       62 
       63 dcl  gseg ptr init(null());
       64 
       65 dcl (addr, addrel, substr) builtin;
       66 
       67 
       68 	scup = addr (mc.scu);
       69 						/* only one arguement */
       70 	increment = 1;
       71 						/* get pointer to segment */
       72 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       73 	arg_ptr = addrel (gseg, fixed (scu.ilc)+1);
       74 						/* check bounds for the arguments */
       75 	if arglist.tally > fixed (gtss_ust.lsize.limit, 18) then do;
       76 	     call gtss_abort_subsystem_ (mcp,
       77 		"gtss_drl_pseudo_",
       78 		4,
       79 		gtss_pnterr.err4,
       80 		fixed (scu.ilc, 18));
       81 	     return;
       82 	end;
       83 
       84 	if arglist.status > fixed (gtss_ust.lsize.limit, 18) then do;
       85 	     call gtss_abort_subsystem_ (mcp,
       86 		"gtss_drl_pseudo_",
       87 		4,
       88 		gtss_pnterr.err4,
       89 		fixed (scu.ilc, 18));
       90 	     return;
       91 	end;
       92 
       93 /*	Set up to find his tally */
       94 	tally_ptr = addrel (gseg, arglist.tally);
       95 						/* check for only tallyb */
       96 	if tally.type ^= "100"b then do;
       97 	     call gtss_abort_subsystem_ (mcp,
       98 		"gtss_drl_pseudo_",
       99 		49,
      100 		gtss_pnterr.err49,
      101 		fixed (scu.ilc, 18));
      102 	     return;
      103 	end;
      104 						/* allow no more than 244 characters to be placed */
      105 	if tally.chars > 244 then do;
      106 	     call gtss_abort_subsystem_ (mcp,
      107 		"gtss_drl_pseudo_",
      108 		49,
      109 		gtss_pnterr.err49,
      110 		fixed (scu.ilc, 18));
      111 	     return;
      112 	end;
      113 						/* make sure the tally address doesn't go out of bounds */
      114 	if tally.address > fixed (gtss_ust.lsize.limit, 18) then do;
      115 	     call gtss_abort_subsystem_ (mcp,
      116 		"gtss_drl_pseudo_",
      117 		4,
      118 		gtss_pnterr.err4,
      119 		fixed (scu.ilc, 18));
      120 	     return;
      121 	end;
      122 
      123 	number_chars = tally.chars;
      124 	number_words = divide ((number_chars+3), 4, 24, 0);
      125 
      126 /* also look at the end of his string to move for boundry check */
      127 	if (tally.address+ number_words) > fixed (gtss_ust.lsize.limit, 18) then do;
      128 	     call gtss_abort_subsystem_ (mcp,
      129 		"gtss_drl_pseudo_",
      130 		4,
      131 		gtss_pnterr.err4,
      132 		fixed (scu.ilc, 18));
      133 	     return;
      134 	end;
      135 	number_words_transmitted = number_words;
      136 	count_of_characters_transmitted = number_chars;
      137 	addr (characters_transmitted) -> move_string =
      138 	     addr (initial_tally_char (tally.offset)) -> move_string;
      139 	gtss_ust_ext_$ust.gtss_ust.lbuf.tally = "0"b;
      140 	return;
