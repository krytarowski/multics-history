       14 gtss_filact_funct08_: proc (mcp_val, high_val, gseg_val, arg_ptr_val, buffer_ptr_val, code);
       15 
       16 /**	Derail FILACT function 08 (PURGE CATALOG).
       17*
       18*	All parameters are input parameters except code.
       19*
       20*	code retuurned 0 => Successful.
       21*	code returned 4 => GCOS err4 (see gtss_pnterr structure).
       22*	code returned other => multics error code.
       23*
       24*   Author:  Robert J. Alvarado	Created	1979
       25*   Change:  Dave Ward	08/31/79 ret status from ascii_file_name.
       26*   Change:  Bob Alvarado	09/27/79 added call to aft_$delete.
       27***/
       28 dcl  mcp_val                  ptr parm;
       29 dcl  high_val                 bit(18)parm;
       30 dcl  gseg_val                 ptr parm;
       31 dcl  arg_ptr_val              ptr parm;
       32 dcl  buffer_ptr_val           ptr parm;
       33 dcl  code                     fixed bin(35)parm;
       34 	mcp = mcp_val;
       35 	high_b = high_val;
       36 	high_i = fixed (high_b, 18);
       37 	gseg = gseg_val;
       38 	arg_ptr = arg_ptr_val;
       39 	buffer_ptr = buffer_ptr_val;
       40 
       41 	code = 0;					/* Successful. */
       42 
       43 /**	Obtain purge|release catalog arglist. */
       44 	if					/* (Purge_Release_args.L_arglist < low_b) | */
       45 	((fixed (Purge_Release_args.L_arglist, 18) +3) > high_i) then do;
       46 return_err4:   ;
       47 	     code = 4;
       48 	     return;
       49 	end;
       50 	arglist_ptr = addrel (gseg, Purge_Release_args.L_arglist);
       51 
       52 /**	Obtain return status.	**/
       53 	call validate_status (Purge_Release_arglist.L_status_return, status_ptr);
       54 
       55 	if get_ascii_file_name (
       56 	"0"b
       57 	, Purge_Release_arglist.L_cat_filedescr
       58 	, addr (ascii_cat_file)
       59 	, grc
       60 	) then do;
       61 could_not_purge_release_catalog: ;
       62 	     call bcd_message (
       63 		status2.L_bcd_message
       64 		, status2.message_words
       65 		, buffer_ptr
       66 		, "Could not purge/release file."||rtrim (gtss_file_values.dname)||">"||rtrim (gtss_file_values.ename)
       67 		);
       68 	     status_word.pd = get_faulty_cat_file_entry ();
       69 	     status_word.status = grc;		/* Could not access file specified. */
       70 ret:	     ;
       71 	     return;
       72 	end;
       73 
       74 	call gtss_expand_pathname_ (
       75 	     addr (ascii_cat_file)
       76 	     , gtss_file_values.dname
       77 	     , gtss_file_values.ename
       78 	     , code
       79 	     );
       80 	if code ^= 0 then goto could_not_purge_release_catalog;
       81 	if gse_ext_$drm_rule = 2 then do;
       82 	     ascii_cat_file.nn = 1;
       83 	     ascii_cat_file.name = "dummy";
       84 	     call gtss_expand_pathname_ (
       85 		addr (ascii_cat_file)
       86 		, dummy_path.dname
       87 		, dummy_path.ename
       88 		, code
       89 		);
       90 	     if dummy_path.ename = gtss_file_values.ename & dummy_path.dname = gtss_file_values.dname then do;
       91 		call gtss_abort_subsystem_ (
       92 		     mcp
       93 		     , "gtss_filact_funct08_"
       94 		     , "400000"b3
       95 		     , "MULTICS:  Attempt to delete working directory.  REQUEST DENIED"
       96 		     );
       97 	     end;
       98 	end;
       99 	directory = rtrim (gtss_file_values.dname)||
      100 	     ">"||
      101 	     rtrim (gtss_file_values.ename);
      102 	sub_catalog = rtrim (gtss_file_values.ename)||">";
      103 	do i = lbound (aft_entry, 1) to hbound (aft_entry, 1);
      104 	     if aft_entry (i).used then do;
      105 		if rtrim (gtss_disk (i).dir_name) = directory |
      106 		index (rtrim (gtss_disk (i).dir_name), sub_catalog)
      107 		^= 0 then do;
      108 		     call gtss_aft_$delete (
      109 			(aft_entry (i).altname)
      110 			, (i)
      111 			, rs
      112 			);
      113 		     if rs ^= 0 then goto could_not_purge_release_catalog;
      114 		end;
      115 	     end;
      116 	end;
      117 
      118 	call delete_$path (
      119 	     gtss_file_values.dname
      120 	     , gtss_file_values.ename
      121 	     , "0"b
      122 	     ||"1"b
      123 	     ||"1"b
      124 	     ||"0"b
      125 	     ||"0"b
      126 	     ||"0"b
      127 	     , "gtss_filact_funct08_"
      128 	     , code
      129 	     );
      130 	if code ^= 0 then goto could_not_purge_release_catalog;
      131 
      132 	status_word.status = "400000"b3;
      133 	goto ret;
