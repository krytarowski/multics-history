  4     1 /* BEGIN INCLUDE FILE  gtss_filact_intp7.incl.pl1 */
  4     2 bcd_message: proc (ml, mw, bp, m);
  4     3 
  4     4 /**	Convert message (m) to bcd.
  4     5*	Store in user's buffer (location bmp).
  4     6*	Set ml to this location and mw to number of words
  4     7*	in message.
  4     8*
  4     9*	Zero ml and mw if not possible.
  4    10***/
  4    11 dcl  ml                       bit(18)unal parm;
  4    12 dcl  mw                       fixed bin(17)unal parm;
  4    13 dcl  bp                       ptr parm;
  4    14 dcl  m                        char(*)parm;
  4    15 	ml = "0"b;
  4    16 	mw = 0;
  4    17 
  4    18 	l = length (m);
  4    19 	n = divide (l+5, 6, 17, 0);			/* Number of words. */
  4    20 	if n>355 then do;
  4    21 	     n = 355;
  4    22 	     l = n*6;
  4    23 	end;
  4    24 
  4    25 	bmp = addrel (bp, 25);			/* => 355 FMS buffer portion of 380 word buffer. */
  4    26 	call gtss_ascii_bcd_ (addr (m), l, bmp);
  4    27 	f = mod (l, 6);				/* Characters to fill to word. */
  4    28 
  4    29 	if f>0 then msg.last_word.blank = "20"b3;
  4    30 	ml = rel (bmp);				/* Word offset to message (in caller's buffer). */
  4    31 	mw = n;
  4    32 	return;
  4    33 dcl  n                        fixed bin;
  4    34 dcl  f                        fixed bin;
  4    35 dcl  l                        fixed bin;
  4    36 
  4    37 dcl  bmp			ptr;
  4    38 dcl 1 msg aligned based(bmp),
  4    39     2 fill (n-1),
  4    40     2 last_word,
  4    41       3 skip  (f)bit(6)unal,
  4    42       3 blank (6-f)bit(6)unal;
  4    43      end						/* bcd_message */;
  4    44 /*   END INCLUDE FILE  gtss_filact_intp7.incl.pl1 */
