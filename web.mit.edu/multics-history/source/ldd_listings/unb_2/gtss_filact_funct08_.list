  3     1 /* BEGIN INCLUDE FILE gtss_filact_intp2.incl.pl1 */
  3     2 /*
  3     3*  Created: (Wardd Multics)  10/05/78 1601.2 mst Thu 
  3     4**/
  3     5 
  3     6 /* Change: Dave Ward	08/18/81 correct stringrange error.
  3     7**/
  3     8 get_ascii_file_name: proc (fnia, cfdl, acfp, grc)returns (bit (1));
  3     9 
  3    10 /**	Obtain the catalog/file description
  3    11*	in ascii.
  3    12*	Reset gcos retrun code (grc) only if error.
  3    13***/
  3    14 dcl  fnia                     bit(1)parm	/* "1"b => file name in ascii. */;
  3    15 dcl  cfdl                     bit(18)parm	/* Location of catalog file description. */;
  3    16 dcl  acfp                     ptr parm	/* Pointer to ascii_cat_file structure. */;
  3    17 dcl  grc			bit(18)parm	/* GCOS return status code. */;
  3    18 
  3    19 	ap = acfp;				/* Local value. */
  3    20 	cat_filedescr_name_offset = "0"b;		/* => offset to "current" name in catalog file description. */
  3    21 
  3    22 	if					/* (cfdl < low_b) | */
  3    23 	(fixed (cfdl, 18)+5 > high_i) then do;
  3    24 	     goto return_err4;
  3    25 	end;					/* Require USERID and atleast file name. */
  3    26 	descriptor_ptr = addrel (gseg, cfdl);
  3    27 
  3    28 	if cfd2.USERID = -1 then do;			/* cfd2.USERID is that of current terminal user. */
  3    29 	     descriptor_ptr -> bit72 = gtss_ust.lid;
  3    30 	     cfd2.USERID_password = (12)"20"b3;
  3    31 	end;
  3    32 
  3    33 	cat_filedescr_name_offset = cfdl;
  3    34 
  3    35 	do nic = 0 to 5;				/* Search for end of list. */
  3    36 	     if nic = 0 then
  3    37 		if cdf4.end_of_list = -1 then do;	/* Only a USERID. */
  3    38 		     acf.nn = 1;
  3    39 		     goto gal;
  3    40 		end;
  3    41 
  3    42 	     if (fixed (cfdl, 18)+ (nic*2)+6) > high_i then do;
  3    43 		goto return_err4;
  3    44 	     end;					/* cat/filedescr entry out of memory. */
  3    45 
  3    46 	     if cfd2.end_of_list = -1 then do;		/* cat/filedescr isolated. */
  3    47 		cat_filedescr_name_offset =
  3    48 		     bit (fixed (fixed (cfdl, 18) + (nic*4) + 4, 18)); /* Offset to name entry (global). */
  3    49 		acf.nn = nic+2;
  3    50 
  3    51 gal:		;
  3    52 		do i = 1 to acf.nn;
  3    53 
  3    54 /**	Store catalog file description for expand path. **/
  3    55 
  3    56 		     if (i > 1) & (i = acf.nn) & fnia then do;
  3    57 			callers_buf.restore_switch = "000000000001"b3; /* => file name in ascii. */
  3    58 			acf.name (i) = translate (cfd3 (i).name,
  3    59 			     "abcdefghijklmnopqrstuvwxyz",
  3    60 			     "ABCDEFGHIJKLMNOPQRSTUVWXYZ");
  3    61 		     end;
  3    62 		     else
  3    63 		     call gtss_bcd_ascii_$lc (
  3    64 			addr (cfd3 (i).name)
  3    65 			, 12
  3    66 			, addr (acf.name (i))
  3    67 			);
  3    68 		end;
  3    69 
  3    70 		if substr (string (gtss_ext_$db), 09, 11) then do;
  3    71 		     call ioa_ ("cat/filedescr:");
  3    72 		     do i = 1 to acf.nn;
  3    73 			call ioa_ ("^3i. ""^a""", i, acf.name (i));
  3    74 		     end;
  3    75 		end;
  3    76 
  3    77 		if substr (acf.name (1), 1, 6) = "!!!!!!" then
  3    78 		     acf.name (1) = gse_ext_$umc_name;
  3    79 		callers_buf.file_id_in_ascii =
  3    80 		     substr (acf.name (acf.nn), 1, length (callers_buf.file_id_in_ascii));
  3    81 
  3    82 /* Determine if there are illegal characters
  3    83*   in the name.
  3    84**/
  3    85 		do i = 1 to acf.nn;
  3    86 		     if acf.name (i) = "*src" then return ("0"b);
  3    87 		     if verify (
  3    88 		     rtrim (acf.name (i))
  3    89 		     , "0123456789abcdefghijklmnopqrstuvwxyz.-_"
  3    90 		     )>0 then do;
  3    91 			grc = "403400"b3;		/* Yes, illegal characters. */
  3    92 			return ("1"b);
  3    93 		     end;
  3    94 		end;
  3    95 
  3    96 		return ("0"b);			/* Success. */
  3    97 	     end;
  3    98 	end;
  3    99 
  3   100 /**	cat/filedescr has no end of list marker. **/
  3   101 	cat_filedescr_name_offset = "0"b;
  3   102 	grc = "403500"b3;				/* Illegal cat/file desc. */
  3   103 	return ("1"b);				/* Failure. */
  3   104 
  3   105 dcl  ap                       ptr	/* Local acfp */;
  3   106 dcl 1 acf aligned based(ap) like ascii_cat_file;
  3   107 
  3   108 dcl 1 callers_buf		aligned based(buffer_ptr),
  3   109       3 restore_switch	bit(36),
  3   110       3 file_id_in_ascii	char(8);
  3   111 
  3   112 dcl 1 cfd2		aligned based(descriptor_ptr)	/* 2nd description of catalog/file list. */
  3   113 ,     3 user_master_catalog
  3   114 ,       4 USERID		fixed bin(71)
  3   115 ,       4 USERID_password	bit(72)
  3   116 ,     3 intermediate_catalogs	(nic)
  3   117 ,       4 CATALOG		bit(72)
  3   118 ,       4 CATALOG_password	bit(72)
  3   119 ,     3 file_to_be_accessed
  3   120 ,       4 FILENAME		bit(72)
  3   121 ,       4 FILENAME_password	bit(72)
  3   122 ,     3 end_of_list		fixed bin(35)
  3   123 ;
  3   124 
  3   125 dcl 1 cfd3		(7) aligned based(descriptor_ptr)	/* 3rd description of catalog/file list. */
  3   126 ,     3 name		char(8)
  3   127 ,     3 password		bit(72)
  3   128 ;
  3   129 
  3   130 dcl 1 cdf4		aligned based(descriptor_ptr)	/* 4th description of catalog/file list. */
  3   131 ,     3 user_master_catalog
  3   132 ,       4 USERID		fixed bin(71)
  3   133 ,       4 USERID_password	bit(72)
  3   134 ,     3 end_of_list		fixed bin(35)
  3   135 ;
  3   136      end						/* get_ascii_file_name */;
  3   137 
  3   138 /*   END INCLUDE FILE gtss_filact_intp2.incl.pl1 */
