      103 	     /* msg_1  */ "reading-cr",
      104 	     /* msg_2  */ "reading-mt",
      105 	     /* msg_3  */ "reading-rmt",
      106 	     /* msg_4  */ "wait-aloc",
      107 	     /* msg_5  */ "wait-perip",
      108 	     /* msg_6  */ "wait-core",
      109 	     /* msg_7  */ "in hold",
      110 	     /* msg_8  */ "in limbo",
      111 	     /* msg_9  */ "executing",
      112 	     /* msg_10 */ "swapped",
      113 	     /* msg_11 */ "wait-media",
      114 	     /* msg_12 */ "too-big",
      115 	     /* msg_13 */ "overdue",
      116 	     /* msg_14 */ "in restart",
      117 	     /* msg_15 */ "terminating",
      118 	     /* msg_16 */ "output waiting",
      119 	     /* msg_17 */ "output complete",
      120 	     /* msg_18 */ "not accesible",
      121 	     /* msg_19 */ "not your job",
      122 	     /* msg_20 */ "aborted" );
      123 
