        9 gtss_drl_jsts_: proc (mcpp, increment);
       10 
       11 /**	gtss obtain job status.
       12*
       13*	Author:	Mel Wilson			29mar79
       14* **/
       15 dcl  mcpp                     ptr parm;
       16 dcl  increment fixed bin parm;
       17 	mcp = mcpp;
       18 
       19 /**
       20*
       21*   DRL JSTS, OBTAIN JOB STATUS (octal ('drl_code'))
       22*
       23*
       24*   8_________1_6_____________________
       25*
       26*   DRL      JSTS
       27*on input:
       28*word_1
       29*   BCI     1,snumb
       30*word_2 (optional)
       31*   VFD     18/L_buffer,9/0,6/,3/n
       32*
       33*on output:
       34*word_1
       35*   VFD     9/status_code,9/activity_number,18/
       36*word_2, if present, is not affected
       37*
       38*
       39*    (See TSS SYSTEM PROGRAMMER'S
       40*         REFERENCE MANUAL DD17C, Rev. 0, July 1976
       41*         Page 3-13)
       42*
       43**/
       44 
       45 	scup = addr (mc.scu);
       46 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       47 	if fixed (scu.ilc, 18) + 1 >= fixed (gtss_ust.lsize.limit, 18) then
       48 	     call gtss_abort_subsystem_ (mcp, "gtss_drl_jsts_", 0,
       49 	     "DRL parameter list outside memory (^o)", fixed (scu.ilc, 18));
       50 	arg_list_ptr = addrel (gseg, fixed (scu.ilc, 18) + 1);
       51 
       52 	call gtss_bcd_ascii_$lc (arg_list_ptr, 5, addr (given_snumb));
       53 
       54 	call gtss_get_user_state_ (u_state_ptr);
       55 	do i = 1 to entry_count while (snumb (i) ^= given_snumb);
       56 	end;
       57 	if i > entry_count then do;			/* given snumb not known */
       58 	     return_status = 18;
       59 	     return_activity = 0;
       60 	end;
       61 	else do;					/* set up status from gtss_user_state_ entry */
       62 	     return_status = status (i);
       63 	     return_activity = activity (i);
       64 	end;
       65 	return_filler = "0"b;
       66 
       67 	if opcode ^= "0"b then			/* no optional buffer supplied */
       68 	     increment = 1;
       69 	else do;					/* return status message into buffer, if all is legal */
       70 	     increment = 2;
       71 	     if L_buffer > fixed (gtss_ust.lsize.limit, 18) - max (320, 320*buf_n) then
       72 		call gtss_abort_subsystem_ (mcp, "gtss_drl_jsts_", 0,
       73 		"Buffer outside memory (^o)", fixed (scu.ilc, 18));
       74 	     call gtss_ascii_bcd_ (addr (status_msg (return_status)),length (rtrim (status_msg (return_status))),
       75 		addrel (gseg, L_buffer));
       76 	     ret_tally_adr = L_buffer;		/* set up buffer tally for return */
       77 	     ret_tally_cnt = length (rtrim (status_msg (return_status)));
       78 	     ret_tally_off = 0;
       79 	end;
       80 
       81 	return;
       82 
       83 /** gtss_drl_jsts_ local declarations */
       84 
       85 dcl  arg_list_ptr ptr;
       86 dcl 1 arg_list based (arg_list_ptr),
       87    2 bcd_snumb unal,
       88       3 return_status fixed bin (9) unsigned unal,
       89       3 return_activity fixed bin (9) unsigned unal,
       90       3 return_filler bit (18),
       91    2 optional_word_2 unal,
       92       3 L_buffer fixed bin (18) unsigned unal,
       93       3 opcode bit (9) unal,
       94       3 filler1 bit(6) unal,
       95       3 buf_n fixed bin (3) unsigned unal;
       96 
       97 dcl  gseg ptr;
       98 dcl  given_snumb char (5) init ("");
       99 dcl  i fixed bin;
      100 
