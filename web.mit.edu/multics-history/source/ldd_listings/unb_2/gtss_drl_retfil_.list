       14 gtss_drl_retfil_: proc (mcpp, increment);
       15 
       16 /* *	Close file and remove from aft.
       17*
       18*   Authors: Robert J. Grimes	Created
       19*   -	  Albert N. Kepner	  1978
       20*   -	  Robert M. May
       21*   -	  David B. Ward
       22*   Change: Dave Ward	04/13/79 db_ debug code.
       23*   Change: Dave Ward	08/03/79 Revised ios close call.
       24*   Change: Bob Alvarado	12/05/79 Added code to not let sy** be removed.
       25*   * */
       26 dcl  mcpp ptr parm;
       27 dcl  increment fixed bin (24)parm;
       28 
       29 /*
       30*
       31*   DRL RETFIL, RETURN A FILE (octal 14)
       32*
       33*
       34*   8_________1_6_____________________
       35*
       36*   DRL      RETFIL
       37*   ZERO     L(filed),L(buff)
       38*
       39*   (See TSS SYSTEM PROGRAMMER'S
       40*   REFERENCE MANUAL DD17B, Rev. 0, July 1976
       41*   Page 3-53)
       42*
       43**/
       44 
       45 
       46 	mcp = mcpp;				/* Set local value. */
       47 	increment = 1;				/* one arguement */
       48 	scup = addr (mc.scu);
       49 
       50 /* *	Obtain pointer to callers "memory".	* */
       51 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       52 
       53 /* *	Obtain pointer to 1 word drl argument list.	* */
       54 	arg_list_ptr = addrel (gseg, fixed (scu.ilc)+1);
       55 
       56 /* *	Check location of caller's fileid.	* */
       57 	if fixed (arg_list.L_fileid, 18)>fixed (gtss_ust.lsize.limit, 18) then do;
       58 	     call gtss_abort_subsystem_ (mcp,
       59 		"gtss_drl_retfil_",
       60 		4,
       61 		"RETFIL: Fileid out of memory at (^6o)",
       62 		fixed (scu.ilc, 18));
       63 	     return;
       64 	end;
       65 
       66 /* *	Obtain pointer to caller's fileid.	* */
       67 	fileid_ptr = addrel (gseg, arg_list.L_fileid);
       68 
       69 	if right_char = (9)"1"b then do;		/* Octal 777 => all but SY** */
       70 	     if db_drl_retfil then
       71 		call com_err_ (0, "gtss_drl_retfil_",
       72 		" Returning all but SY**");
       73 	     call all_but ("SY**");
       74 	     return;
       75 	end;
       76 
       77 	if right_char = "111111110"b then do;		/* Octal 776 => all but *CF */
       78 	     if db_drl_retfil then
       79 		call com_err_ (0, "gtss_drl_retfil_",
       80 		"Returning all but *CF");
       81 	     call all_but ("*CF");
       82 	     return;
       83 	end;
       84 
       85 /* *	Otherwise, designated file. * */
       86 	if db_drl_retfil then
       87 	     call com_err_ (0, "gtss_drl_retfil_",
       88 	     "Returning file ""^a""",
       89 	     fileid);
       90 	call remove (fileid);
       91 	return;
       92 
       93 all_but:	proc (fileid);
       94 
       95 /* *	Remove all files except that
       96*   named by parameter fileid.
       97*   * */
       98 dcl  fileid char (8) parm;
       99 	     do i = 1 to hbound (aft_entry, 1);
      100 		if used (i) then
      101 		     if fileid ^= altname (i) then call remove (fileid);
      102 	     end;
      103 	     return;
      104 
      105 dcl  i fixed bin (24);
      106 	end					/* all_but */;
      107 
      108 remove:	proc (fileid);
      109 
      110 /* *	Remove file named by fileid.	* */
      111 dcl  fileid char (8) parm;
      112 	     unspec (fms_status) = "0"b;
      113 /** SY** cannot be removed  **/
      114 	     if fileid = "SY**    " then return;
      115 	     call gtss_aft_$delete (fileid, fn, code);
      116 	     if code = 0 then do;
      117 		call gtss_ios_close_ (fn, addr (fms_status), code);
      118 		if fms_status.bit12 ^= "4000"b3 then do;
      119 dcl 1 fms_status aligned,
      120       2 bit12 bit(12)unal,
      121       2 bit60 bit(60)unal;
      122 		     call gtss_abort_subsystem_ (
      123 			mcp
      124 			, "gtss_drl_retfil_"
      125 			, 0
      126 			, "RETFIL: Can not return ""^a"" (DRL at ^6o) gtss_ios_close_ status=^w"
      127 			, fileid
      128 			, fixed (scu.ilc, 18)
      129 			, fms_status
      130 			);
      131 		end;
      132 	     end;
      133 
      134 	     return;
      135 
      136 dcl  fn fixed bin (24);
      137 dcl  code fixed bin (35);
      138 	end					/* remove */;
      139 
      140 /* *	Variables for gtss_drl_retfil_:
      141*   IDENTIFIER	ATTRIBUTES	* */
      142 dcl  addrel builtin;
      143 dcl  arg_list_ptr ptr init(null());
      144 dcl  code fixed bin (35);
      145 dcl  fileid char (8) based (fileid_ptr);
      146 dcl  fileid_ptr ptr init(null());
      147 dcl  fn fixed bin (24);
      148 dcl  gseg ptr init(null());
      149 
      150 dcl 1 arg_list aligned based (arg_list_ptr)
      151      , 2 L_fileid bit (18)unal
      152      , 2 L_buff bit (18)unal
      153      ;
      154 
      155 dcl 1 fileid_2 aligned based (fileid_ptr)
      156      , 2 filler bit (27)unal
      157      , 2 right_char bit (9)
      158      ;
