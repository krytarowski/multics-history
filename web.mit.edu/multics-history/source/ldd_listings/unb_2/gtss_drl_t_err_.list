       14 gtss_drl_t_err_: proc (mcpp, increment);
       15 
       16 /**	gtss error detected by a subsystem.
       17*
       18*   Authors:	Robert J. Grimes	Created
       19*		Albert N. Kepner	  1978
       20*		Robert M. May
       21*		David B. Ward
       22*
       23*   Changed:	Al Dupuis	10/09/79. Added tests for lflg2.b8,
       24*				lflg2.b4 and lswt2.b5.
       25*   * */
       26 dcl  mcpp ptr parm;
       27 dcl  increment fixed bin parm;
       28 	mcp = mcpp;
       29 
       30 /* *
       31*
       32*   DRL T.ERR, ERROR DETECTED (octal 103)
       33*
       34*
       35*   8_________1_6_____________________
       36*
       37*   DRL      T.ERR
       38*
       39*   (See TSS SYSTEM PROGRAMMER'S
       40*   REFERENCE MANUAL DD17C, Rev. 0, July 1976
       41*   Page 3-33.1)
       42*
       43**/
       44 
       45 	increment = 0 ;				/* Zero arguments */
       46 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
       47 	gtss_spa.lwrap.b23 = "1"b;
       48 	if gtss_ust.lflg2.b8 then gtss_ust.lcfst.start_term = 14;
       49 	initial_lxxx = fixed (rel (addr (gtss_ust.lxxx)))-fixed (rel (addr (gtss_ust)));
       50 	ssname = gtss_prgdes_ext_$prgdes.ss_name
       51 	     (gtss_ust.lprgs.b0_17 (gtss_ust.lxxx.b0_17 - initial_lxxx));
       52 	ssname = translate (ssname, "ABCDEFGHIJKLMNOPQRSTUVWXYZ",
       53 	     "abcdefghijklmnopqrstuvwxyz");
       54 	if gtss_ust.lswt2.b6 then do;
       55 	     gtss_ext_$flags.gtss_com_err_sw = "1"b;
       56 	     call gtss_abort_subsystem_ (mcp, "gtss_drl_t_err_", 0,
       57 		"<67> ERROR DETECTED IN ^a PROCESSING", ssname);
       58 	     return;
       59 	end;
       60 	if ^((gtss_ust.lflg2.b8) & (^gtss_ust.lflg2.b4))	/* If CFP on  and TALK off go on, else return */
       61 	then return;
       62 	if gtss_ust.lswt2.b5 then return;		/* trap mode disabled */
       63 	gtss_ext_$flags.gtss_com_err_sw = "1"b;
       64 	call gtss_abort_subsystem_ (mcp, "gtss_drl_t_err_", 0,
       65 	     "<67> ERROR DETECTED IN ^a PROCESSING", ssname);
       66 	return;
       67 
