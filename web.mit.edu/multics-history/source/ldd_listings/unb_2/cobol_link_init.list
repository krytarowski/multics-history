       25 /* format: style3 */
       26 cobol_link_init:
       27      proc;					/*
       28*The procedure cobol_link_init initializes the Linkage Section and  
       29*outputs corresponding relocation information.  In particular it:
       30*
       31*     1.  Initializes all items in the header with the exception 
       32*         of def_section_relp and linkage_section_length.
       33*
       34*     2.  Initializes all fixed internal static data (stat and
       35*         trace_control_word) with the exception of stat.data_len.
       36*
       37*cobol_link_init is called once per compilation, prior to code
       38*generation.
       39*
       40*
       41*U__s_a_g_e:_
       42*
       43*     declare cobol_link_init entry;
       44*
       45*     call cobol_link_init;
       46*
       47*D__a_t_a:_
       48*
       49*     % include cobol_;
       50*
       51*	Items in cobol include file used (u) and/or set(s) by
       52*	cobol_link_init:
       53*
       54*               cobol_ptr (u)
       55*	     com_ptr (u)
       56*               link_base_ptr (u)
       57*               link_wd_off (s)
       58*	     link_max (u)
       59*
       60*
       61*     % include fixed_common;
       62*
       63*          Items in fixed_common include file used (u) and/or set (s)
       64*	by cobol_link_init:
       65*
       66*	     prog_name (u)				  */
