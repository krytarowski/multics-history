  1     2 /*  BEGIN INCLUDE FILE ...   cobol_linkage_header.incl.pl1  */
  1     3 
  1     4 /*  Last modified 07/19/78 by RAL to conform with MULTICS standard linkage map */
  1     5 /*  Last modified June 26, 1974 by AEG              */
  1     6 
  1     7 
  1     8 
  1     9 declare	1 linkage_header aligned based(link_base_ptr),
  1    10 	  2 pad bit(36),
  1    11 	  2 def_section_relp bit(18) unaligned,
  1    12 	  2 first_reference_relp bit(18) unaligned,
  1    13 	  2 obsolete_ptr ptr unal,
  1    14 	  2 original_linkage_ptr ptr unal,
  1    15 	  2 unused bit (72),
  1    16 	  2 links_relp bit(18) unaligned,
  1    17 	  2 linkage_section_length bit(18) unaligned,
  1    18 	  2 object_seg bit(18) unaligned,
  1    19 	  2 obsolete_length bit(18) unaligned;
  1    20 
  1    21 
  1    22 /*
  1    23*link_base_ptr		is a pointer to the base of the
  1    24*			linkage section and is an element
  1    25*			of the structure   cobol_ located in
  1    26*			  cobol_external_seg.
  1    27*
  1    28*pad			is reserved for future use and
  1    29*			must be zero.
  1    30*
  1    31*def_section_relp		is a pointer (relative to the base
  1    32*			of the object segment) to the base
  1    33*			of the definition section.
  1    34*
  1    35*first_reference_relp	is a pointer (relative to the base
  1    36*			of the linkage section) to the
  1    37*			array of first-reference traps.
  1    38*			If the value of this item is "0"b
  1    39*			then there are no first-reference
  1    40*			traps.
  1    41*
  1    42*obsolete_ptr		for historical reasons, linkage
  1    43*			sections are sometimes threaded
  1    44*			together to form a linkage list.
  1    45*			This variable is a pointer to the
  1    46*			next linkage section on the thread.
  1    47*			This variable is described for 
  1    48*			completemess;  it is not intended
  1    49*			for general use.
  1    50*
  1    51*original_linkage_ptr	is a pointer to the original link-
  1    52*			age section within the object seg-
  1    53*			ment.  It is used by the link un-
  1    54*			snapping mechanism.  It is init-
  1    55*			ialized by the linker.
  1    56*
  1    57*links_relp		is a pointer (retative to the base
  1    58*			of the linkage section) to the
  1    59*			first link (the base of the link
  1    60*			structure).
  1    61*
  1    62*linkage_section_length	is the length in words of the
  1    63*			linkage section.
  1    64*
  1    65*object_seg		is the segment number of the object
  1    66*			segment.  It is initialized by the
  1    67*			linker.
  1    68*
  1    69*obsolete_length		when several linkage sections are
  1    70*			combined into a list, this item
  1    71*			(in the first linkage section in
  1    72*			the list) contains the length of
  1    73*			the entire list.  See the above
  1    74*			discussion under obsolete_ptr.
  1    75*						  */
  1    76 /*  END INCLUDE FILE ...   cobol_linkage_header.incl.pl1		  */
  1    77 
