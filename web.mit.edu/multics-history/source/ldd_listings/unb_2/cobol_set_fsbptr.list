       22 /* Modified on 1/25/77 by Bob Chang to implement profile option.	*/
       23 /* Modified on 11/11/76 by Bob Chang to change mft into file_table.	*/
       24 /* Modified since Version 2.0.	*/
       25 /* format: style3 */
       26 cobol_set_fsbptr:
       27      proc (ft_ptr);
       28 
       29 dcl	ft_ptr		ptr;
       30 
       31 dcl	1 basic_struct	static,
       32 	  2 type		fixed bin init (1),
       33 	  2 operand_no	fixed bin init (0),
       34 	  2 lock		fixed bin init (0),
       35 	  2 seg		fixed bin,
       36 	  2 offset	fixed bin,
       37 	  2 send_receive	fixed bin init (0);
       38 
       39 dcl	instr		(3) bit (36);
       40 dcl	reloc		(6) bit (5) aligned;
       41 dcl	instr_ptr		ptr;
       42 dcl	reloc_ptr		ptr;
       43 
       44 /* fixup directive for link, used when profile options is specified.	*/
       45 dcl	1 fixup_directive	aligned static,
       46 	  2 operation	bit (1) unal init ("0"b),
       47 	  2 type		bit (4) unal init ("1111"b),
       48 	  2 reserved	bit (9) unal init ("000000000"b),
       49 	  2 location	unal,
       50 	    3 half	bit (1) unal init ("0"b),
       51 	    3 base	bit (3) unal init ("001"b),
       52 	    3 offset	fixed bin unal,
       53 	  2 tag_number	fixed bin aligned;
       54 dcl	epp1		bit (12) static init ("011101001101"b);
       55 dcl	epp1_indmod	bit (18) static init ("011101001101010000"b);
       56 
       57 dcl	utemp		fixed bin;
       58 dcl	i		fixed bin;
       59 dcl	ic		fixed bin;
       60 
       61 dcl	cobol_addr	entry (ptr, ptr, ptr);
       62 dcl	cobol_make_fixup	entry (ptr);
       63 dcl	cobol_emit	entry (ptr, ptr, fixed bin);
       64 
       65 
       66 /*************************************/
       67 start:
       68 	instr_ptr = addr (instr);
       69 	reloc_ptr = addr (reloc);
       70 	do i = 1 to 6;
       71 	     reloc (i) = ""b;
       72 	end;
       73 	ic = 1;
       74 	if file_table.fsb.seg < 0
       75 	then do;					/* external file - link to fsb */
       76 		utemp = -file_table.fsb.seg;
       77 		instr (1) = "100"b || substr (unspec (utemp), 22, 15) || epp1_indmod;
       78 		if file_table.fsb.off ^= 0
       79 		then do;
       80 			instr (2) = "001"b || substr (unspec (file_table.fsb.off), 22, 15) || epp1;
       81 			ic = 2;
       82 		     end;
       83 		reloc (1) = "10100"b;
       84 	     end;
       85 	else do;					/* internal file - fsb in cobol data segment */
       86 		instr (1) = (18)"0"b || epp1_indmod;
       87 		basic_struct.seg = file_table.fsb.seg;
       88 		basic_struct.offset = file_table.fsb.off;
       89 		call cobol_addr (addr (basic_struct), instr_ptr, reloc_ptr);
       90 	     end;
       91 	if fixed_common.options.profile
       92 	then do;
       93 		fixup_directive.location.offset = cobol_$text_wd_off;
       94 		call cobol_make_fixup (addr (fixup_directive));
       95 	     end;
       96 	call cobol_emit (instr_ptr, reloc_ptr, ic);
       97 exit:
       98 	return;
       99 
