      101 /* work variables */
      102 dcl  fixed builtin;
      103 dcl  addr builtin;
      104 dcl  gseg ptr init(null());
      105 dcl  timer_manager_$cpu_call entry (fixed bin (71), bit (2), entry);
      106 dcl  timer_manager_$reset_cpu_call entry (entry);
      107 
      108 	scup = addr (mc.scu);
      109 	gseg = gtss_ext_$gtss_slave_area_seg (gtss_ext_$stack_level_);
      110 	increment = 0;				/* No arguments. */
      111 	gtss_ust.lflg2.b14 = "1"b;			/* set bit in flagword indicating execution */
      112 
      113 /* If user did a drl drlimt (limit non zero) then set up timer,
      114*   but if one subsystem calls another, and it wants to set up a timer also,
      115*   they do not have a nested timer effect. i.e. If subsystem one sets
      116*   a 10 second timer, and subsystem two sets a 100 second timer and only
      117*   uses 5 seconds, when control goes back to subsystem one the 100 second
      118*   timer is still ticking away. (Strange, but GCOS does it that way.) */
      119 	if gtss_ust_ext_$ust.gtss_ust.limit ^= 0 then do;
      120 /* If a timer is already ticking, kill it.	*/
      121 	     if gtss_ext_$flags.ss_time_limit_set then
      122 		call timer_manager_$reset_cpu_call (
      123 		gtss_fault_processor_$timer_runout);
      124 	     call timer_manager_$cpu_call (
      125 		divide (divide (gtss_ust_ext_$ust.gtss_ust.limit, 64, 36), 1000, 71),
      126 		"11"b, gtss_fault_processor_$timer_runout);
      127 	     gtss_ext_$flags.ss_time_limit_set = "1"b;
      128 	end;
      129 
      130 	if fixed (gtss_ust.lsize.limit, 18, 0)>gtss_install_values_$memory_limit then do;
      131 	     gtss_spa.lsztm.b34 = "1"b;
      132 	     if gtss_spa.lsztm.tra ^= "0"b then do;	/* Caller providing code for exceeding memory condition. */
      133 
      134 /* *	Adjust increment parameter to cause transfer
      135*   to caller's code.
      136*   * */
      137 		gtss_spa.losti.ilc = substr(gtss_spa.lsztm.tra,1,18); /* Set return address in slave prefix. */
      138 		gtss_spa.losti.ir = "0"b;		/* Zero indicators. */
      139 		increment = fixed (tra, 18, 0) - fixed (scu.ilc, 18, 0) -1;
      140 		return;
      141 	     end;
      142 
      143 	     call gtss_abort_subsystem_ (mcp,
      144 		"gtss_drl_objtim_",
      145 		45,
      146 		gtss_pnterr.err45);
      147 	end;
      148 	return;
