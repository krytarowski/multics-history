       26 /* format: style3 */
       27 cobol_cancel_gen:
       28      proc (mp_ptr);
       29 
       30 dcl	mp_ptr		ptr;
       31 dcl	1 mp		based (mp_ptr),
       32 	  2 n		fixed bin,		/* always 3 */
       33 	  2 pt1		ptr,			/* pts to type1 token */
       34 	  2 pt2		ptr,			/* pts to type9 or type3 token */
       35 	  2 pt3		ptr;			/* pts to meaningless type19 token */
       36 
       37 dcl	1 mpout,
       38 	  2 n		fixed bin,
       39 	  2 pt1		ptr,
       40 	  2 pt2		ptr,
       41 	  2 pt3		ptr,
       42 	  2 pt4		ptr;
       43 
       44 dcl	1 type9,
       45 	  2 alignment	ptr,			/* so as to double word align the space */
       46 	  2 rest		char (140);
       47 dcl	1 type19		static,
       48 	  2 size		fixed bin init (38),
       49 	  2 line		fixed bin init (0),
       50 	  2 column	fixed bin init (0),
       51 	  2 type		fixed bin init (19),
       52 	  2 verb		fixed bin init (5),		/* CALL */
       53 	  2 e		fixed bin init (1),		/* one operand after USING */
       54 	  2 hij		(3) fixed bin init (0, 0, 0),
       55 	  2 a		bit (3) init ("001"b),	/* USING operands exist */
       56 	  2 bcdfgk	bit (13) init (""b);
       57 dcl	1 type3		static,
       58 	  2 size		fixed bin init (45),
       59 	  2 line		fixed bin init (0),
       60 	  2 column	fixed bin init (0),
       61 	  2 type		fixed bin init (3),
       62 	  2 bits		bit (8) init (""b),
       63 	  2 lit_size	fixed bin init (13),
       64 	  2 lit		char (13) init ("cobol_cancel_");
       65 
       66 dcl	1 pr3_struct	static,
       67 	  2 pr3		fixed bin init (3),
       68 	  2 pointer_no	bit (3),
       69 	  2 lock		fixed bin init (0),
       70 	  2 switch	fixed bin init (0),
       71 	  2 segno		fixed bin,
       72 	  2 offset	fixed bin,
       73 	  2 reset		fixed bin;
       74 
       75 dcl	dn_ptr		ptr;
       76 dcl	offset		fixed bin,
       77 	temp		fixed bin,
       78 	cobol_emit	entry (ptr, ptr, fixed bin),
       79 	cobol_call_op	entry (fixed bin, fixed bin),
       80 	cobol_get_size$omit_sign
       81 			entry (ptr, fixed bin, fixed bin),
       82 	cobol_set_pr	entry (ptr, ptr),
       83 	cobol_make_type9$type2_3
       84 			entry (ptr, ptr);
       85 
       86 dcl	inst_seq		(4) bit (18) static init ("000000000000000000"b, "011101011100000100"b,
       87 						/* epp3	name_loc,ic	*/
       88 			"000000000000000000"b, "010011101000000111"b);
       89 						/* lda	name_len,dl	*/
       90 
       91 
       92 /*************************************/
       93 start:
       94 	mpout.pt1 = mp.pt1;
       95 	mpout.pt2 = addr (type3);
       96 	dn_ptr = mp.pt2;
       97 	if data_name.type ^= 9
       98 	then do;
       99 		dn_ptr = addr (type9);
      100 		call cobol_make_type9$type2_3 (dn_ptr, mp.pt2);
      101 	     end;
      102 	call cobol_set_pr (addr (pr3_struct), dn_ptr);
      103 	call cobol_get_size$omit_sign (dn_ptr, 0, 0);
      104 	call cobol_call_op (49, 0);
      105 	return;
